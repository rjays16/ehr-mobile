ult.filepath: ', result.filepath);\r\n              this.openWithInAppBrowser(result.filepath);\r\n            }\r\n          }\r\n        });\r\n      }, err => {\r\n        console.log(err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n        });\r\n      });\r\n    }\r\n  }\r\n\r\n\r\n  openWithInAppBrowser(url: string) {\r\n    this.inAppBrowser.create(url, '_system', this.options);\r\n  }\r\n\r\n  openAccountPage(title: string) {\r\n    const token = this.loginService.getAuthentication().token;\r\n    this.loginApiService.setToken(token);\r\n    this.loginApiService.setUrl(this.apiService.getUrl());\r\n    console.log('token:', token);\r\n    if (title === 'Settings') {\r\n      this.router.navigate(['/account-settings']);\r\n      this.menuController.close('accountMenu');\r\n    } else  if (title === 'Update app Configuration') {\r\n      this.loadingService.present();\r\n      this.loginApiService.getConfigAndDefaults().subscribe((configAndDefaults) => {\r\n        this.loadingService.dismiss().then(_ => {\r\n          console.log('configAndDefaults: ', configAndDefaults);\r\n          if (configAndDefaults) {\r\n            if (configAndDefaults.code && configAndDefaults.code !== 200) {\r\n              this.alertServ.openAlertModal('fail', 'Failed', configAndDefaults.message, 'Ok');\r\n            } else {\r\n              this.alertServ.openAlertModal('success', 'Success', 'Update complete', 'Ok');\r\n              this.loginService.setConfigAndDefaults(configAndDefaults);\r\n              const authentication =  this.loginService.getAuthentication();\r\n              authentication.user.permissions = configAndDefaults['user-permissions'].map((i:any)=>new Permission(i));\r\n              this.loginService.setAuthentication(authentication);\r\n              this.storageService.set('authentication',JSON.stringify(authentication));\r\n              this.menuService.initMenuByConfig();\r\n              this.router.navigate(['/menu/']);\r\n              this.menuController.close('accountMenu');\r\n            }\r\n          }\r\n        });\r\n      }, err => {\r\n        console.log('Failed', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n        });\r\n      });\r\n    } else  if (title === 'Pull hot fixes') {\r\n      this.menuController.close('accountMenu');\r\n      this.updateServ.checkCodePush();\r\n    }\r\n    else  if (title === 'Logout') {\r\n      this.loadingService.present();\r\n      this.loginApiService.logout().subscribe((respose) => {\r\n        this.loadingService.dismiss().then(_ => {\r\n          this.patientService.setPatientList(null);\r\n          this.patientService.setNursePatientList(null);\r\n          this.storageService.remove('authentication');\r\n          this.navController.setDirection('root');\r\n          this.router.navigate(['/']);\r\n        });\r\n      }, err => {\r\n        console.log(err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          // const message = this.stringUtilsService.parseErrorMessage(err);\r\n          // this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          this.storageService.remove('authentication');\r\n          this.navController.setDirection('root');\r\n          this.router.navigate(['/']);\r\n        });\r\n      });\r\n    }\r\n  }\r\n\r\n  ngOnInit() {\r\n    \r\n\r\n  }\r\n\r\n\r\n}\r\n"],"sourceRoot":"webpack:///"}