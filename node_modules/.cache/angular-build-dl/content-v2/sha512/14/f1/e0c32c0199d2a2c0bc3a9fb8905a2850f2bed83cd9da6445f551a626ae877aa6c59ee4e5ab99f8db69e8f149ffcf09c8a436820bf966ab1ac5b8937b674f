selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n\r\n        this.loadingService.dismiss().then(_ => {\r\n          if (result.length > 0) {\r\n            this.parseBatchNotes(result);\r\n          }\r\n          resolve(result);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  getNurseCarryOutOrders(): Promise<any> {\r\n    return new Promise((resolve, reject) => {\r\n      this.loadingService.present();\r\n\r\n      this.nurseCarryOutOrdersApiService.getCarryOutOrders(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n\r\n        this.loadingService.dismiss().then(_ => {\r\n          if (result.carryoutOrders) {\r\n            this.parseCarryOutOrders(result.carryoutOrders);\r\n          }\r\n          resolve(result);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  parseBatchNotes(batchNotesList: any) {\r\n    this.unfinalizedBatchNotes = null;\r\n    this.batchNotesList = batchNotesList;\r\n\r\n    this.batchNotesList.forEach(notes => {\r\n      this.parseBatch(notes);\r\n    });\r\n\r\n    const unfinalizedIndex = this.batchNotesList.findIndex(item => {\r\n      return item.is_finalized === false || item.is_finalized === 0;\r\n    });\r\n    // if not finalized add list to request orders list (tab)\r\n    console.log('unfinalizedIndex: ', unfinalizedIndex);\r\n    if (unfinalizedIndex >= 0) {\r\n      const unfinalizedBatchCopy =  this.batchNotesList[unfinalizedIndex];\r\n      const unfinalizedBatch = JSON.parse(JSON.stringify(unfinalizedBatchCopy));\r\n\r\n      console.log('unfinalizedBatch: ', unfinalizedBatch);\r\n\r\n      this.unfinalizedBatchNotes = unfinalizedBatch;\r\n\r\n      // remove unfinalized batch\r\n      this.batchNotesList.splice(unfinalizedIndex, 1);\r\n    }\r\n\r\n    console.log('batchNotesList: ', this.batchNotesList);\r\n  }\r\n\r\n  parseBatch(notes: any) {\r\n    const darnoteslist = notes.notes.filter(item => {\r\n      // tslint:disable-next-line: max-line-length\r\n      return item.darnotes && item.darnotes !== null;\r\n    });\r\n\r\n    if (darnoteslist) {\r\n      darnoteslist.forEach(note => {\r\n        note.darnotes.bgcolor = this.getLightRandomColor();\r\n      });\r\n    }\r\n\r\n    notes.darnoteslist = darnoteslist;\r\n    let nurse_name = '';\r\n    if (notes.personnel && notes.personnel.p) {\r\n      const name_first = notes.personnel.p.name_first ? notes.personnel.p.name_first : '';\r\n      const name_middle = notes.personnel.p.name_middle ? notes.personnel.p.name_middle : '';\r\n      const name_last = notes.personnel.p.name_last ? notes.personnel.p.name_last : '';\r\n      const suffix = notes.personnel.p.suffix ? notes.personnel.p.suffix : '';\r\n      nurse_name = name_first + ' ' + name_middle + ' ' + name_last + ' ' + suffix;\r\n    }\r\n    notes.nurse_name = nurse_name;\r\n  }\r\n\r\n  parseCarryOutOrders(carryoutOrders: any) {\r\n    this.carryoutOrders.prescriptionList = carryoutOrders.prescription;\r\n    this.carryoutOrders.referralList = carryoutOrders.referral;\r\n    this.carryoutOrders.doctorOrderOrActionList = carryoutOrders.courseward;\r\n    this.carryoutOrders.laboratoryList = carryoutOrders.laboratory;\r\n    this.carryoutOrders.radiologyList = carryoutOrders.radiology;\r\n\r\n    if (this.carryoutOrders.referralList.length > 0) {\r\n      this.carryoutOrders.referralList.forEach(element => {\r\n        const ty = this.types.find(item => {\r\n          return element.referral_type === item.id;\r\n        });\r\n        element.referral_desc = ty.desc;\r\n\r\n        console.log('element: ', element);\r\n        const dept = this.departments.find(item => {\r\n          return element.department === item.id.toString() || (typeof element.department === 'number' && item.id);\r\n        });\r\n\r\n        console.log('dept: ', dept);\r\n        const rea = this.reasons.find(item => {\r\n          return element.reason === item.id.toString() || (typeof element.reason === 'number' && item.id);\r\n        });\r\n        console.log('rea: ', rea);\r\n\r\n        element.dept_desc = dept ? dept.area_desc : '';\r\n        element.purpose_desc = rea ? rea.purpose_desc : '';\r\n      });\r\n    }\r\n  }\r\n\r\n  doRefreshPatientChart(event: any) {\r\n    if (this.selectedTab === 'Orders') {\r\n      this.getNurseCarryOutOrders().then(_ => {\r\n        this.ordersIsLoaded = true;\r\n        event.target.complete();\r\n      }).catch(_ => {\r\n        event.target.complete();\r\n      });\r\n    } else if (this.selectedTab === 'Notes') {\r\n      this.getNurseDarNotes().then(_ => {\r\n        this.notesIsLoaded = true;\r\n        event.target.complete();\r\n      }).catch(_ => {\r\n        event.target.complete();\r\n      });\r\n    }\r\n  }\r\n\r\n  async openPromptModal(option: number, data: any, title: string, message: string,  ok: string = 'Save', cancel: string = 'Cancel') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        prompt,\r\n        title,\r\n        message,\r\n        cancel,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (result.data && result.data.button === 'Delete') {\r\n          if (option === this.DELETE_NURSE_DAR_NOTES) {\r\n            this.deleteUnfinalizedNoteFunction(data.index);\r\n          }\r\n        } else if (result.data && result.data.button === 'Finalize') {\r\n          if (option === this.FINALIZE_NURSE_DAR_NOTES) {\r\n            this.finalizedNotesFunction();\r\n          }\r\n        }\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n  async openAlertModal(alert: string, title: string, message: string, ok: string = 'Done') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        alert,\r\n        title,\r\n        message,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n  addDarNotes() {\r\n    this.router.navigate(['/menu/patient-chart/dar-note', {action: this.ACTION_ADD}]);\r\n  }\r\n\r\n  async openAddNotesModal() {\r\n    const modal = await this.modalController.create({\r\n      component: NurseNoteComponent,\r\n      cssClass: 'my-nurse-dar-notes-modal-css',\r\n      backdropDismiss: false\r\n    });\r\n\r\n    modal.onDidDismiss().then((result: any) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (result.data.data.title) {\r\n          const darNote = new DarNotes();\r\n          darNote.focus = result.data.data.title;\r\n          darNote.data = result.data.data.data;\r\n          darNote.action = result.data.data.action;\r\n          darNote.response = result.data.data.response;\r\n          console.log('darNote: ', darNote);\r\n        }\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n  keyboardShowHandler(e) {\r\n    const kH = e.keyboardHeight;\r\n    console.log('show: ', e.keyb