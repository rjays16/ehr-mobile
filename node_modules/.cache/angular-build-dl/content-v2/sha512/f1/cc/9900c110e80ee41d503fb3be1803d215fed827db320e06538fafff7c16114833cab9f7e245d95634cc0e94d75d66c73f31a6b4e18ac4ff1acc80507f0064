ial';\r\n    }\r\n    */\r\n    console.log(bodyMoveStyle.bottom);\r\n  }\r\n\r\n  keyboardHideHandler() {\r\n    console.log('gone');\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n\r\n  removeIonAppStyles() {\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n}\r\n\r\nexport declare interface OnQuickSearchSelected {\r\n  /**\r\n   * A callback method that is invoked immediately after the\r\n   * default change detector has checked the directive's\r\n   * data-bound properties for the first time,\r\n   * and before any of the view or content children have been checked.\r\n   * It is invoked only once when the directive is instantiated.\r\n   */\r\n  onQuickSearch(patient: Patient): void;\r\n}\r\n"],"sourceRoot":"webpack:///"}