/* harmony import */ var src_app_app_services_alert_service_service__WEBPACK_IMPORTED_MODULE_9__ = __webpack_require__(/*! src/app/app/services/alert-service.service */ "./src/app/app/services/alert-service.service.ts");










let ReferralsPage = class ReferralsPage {
    constructor(referralsApiService, patientService, apiService, loginService, loadingService, alertServ, stringUtilsService, menuController) {
        this.referralsApiService = referralsApiService;
        this.patientService = patientService;
        this.apiService = apiService;
        this.loginService = loginService;
        this.loadingService = loadingService;
        this.alertServ = alertServ;
        this.stringUtilsService = stringUtilsService;
        this.menuController = menuController;
        this.pPatientReferralSave = true;
        this.selectedEncounterNo = this.patientService.getSelectedEncounterNo();
        this.referralsApiService.setUrl(this.apiService.getUrl());
        this.referralsApiService.setToken(this.loginService.getAuthentication().token);
        this.referralConfigAndDefaults = this.loginService.getConfigAndDefaults().referral;
    }
    initReferrals() {
        return new Promise((resolve, reject) => {
            this.loadingService.present();
            this.referralsApiService.getReferralsList(this.selectedEncounterNo).subscribe((result) => {
                console.log('result: ', result);
                if (result && result.length > 0) {
                    this.referrals = result;
                }
                this.loadingService.dismiss().then(_ => {
                    resolve(result);
                });
            }, (err) => {
                console.log('err ', err);
                this.loadingService.dismiss().then(_ => {
                    const message = this.stringUtilsService.parseErrorMessage(err);
                    this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
                    reject(err);
                });
            });
        });
    }
    ngOnInit() {
        this.menuController.swipeGesture(true, 'mainMenu');
        this.menuController.swipeGesture(true, 'accountMenu');
        this.initReferrals();
    }
    ionViewWillEnter() {
        console.log('ionViewWillEnter');
        const tempSelectedEncounterNo = this.patientService.getSelectedEncounterNo();
        if (this.selectedEncounterNo !== tempSelectedEncounterNo) {
            console.log('selected patient changed');
            this.selectedEncounterNo = tempSelectedEncounterNo;
            this.initReferrals();
        }
    }
    doRefreshReferrals(event) {
        this.initReferrals().then(_ => {
            event.target.complete();
        }).catch(_ => {
            event.target.complete();
        });
    }
};
ReferralsPage.ctorParameters = () => [
    { type: _services_referrals_api_service__WEBPACK_IMPORTED_MODULE_2__["ReferralsApiService"] },
