r\n      console.log('ionViewWillEnter');\r\n      window.addEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n      window.addEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n      window.addEventListener('touchstart', this.tapCoordinates);\r\n    });\r\n  }\r\n\r\n  ionViewWillLeave() {\r\n    console.log('ionViewWillLeave');\r\n    window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n    window.removeEventListener('touchstart', this.tapCoordinates);\r\n    this.removeIonAppStyles();\r\n  }\r\n\r\n  continueToApplication() {\r\n    this.loginApiService.setUrl(this.apiService.getUrl());\r\n\r\n    this.uniqueDeviceID.get().then((uuid: any) => {\r\n      console.log(uuid);\r\n      this.loginCredentials.device_device_unique_id = uuid;\r\n    }).catch((error: any) => {\r\n      console.log(error);\r\n    });\r\n    this.platform.ready().then(_ => {\r\n      if (this.device) {\r\n        this.loginCredentials.device_platform = this.device.platform;\r\n        this.loginCredentials.device_uuid = this.device.uuid;\r\n        this.loginCredentials.device_model = this.device.model;\r\n      }\r\n    });\r\n\r\n    // is autologin is enabled\r\n    this.storageService.get('isAutologin').then((val)=>{\r\n      console.log('isAutologin', val);\r\n      if(val == 'true' || val == null){\r\n          this.storageService.getObject('authentication').then((auth) => {\r\n            console.log('authentication: ', auth);\r\n            if (auth) {\r\n              if (!(this.platform.is('android') || this.platform.is('ios'))) {\r\n                this.loadingService.present();\r\n              }\r\n              this.loginApiService.setToken(auth.token);\r\n              this.loginApiService.getConfigAndDefaults().subscribe((configAndDefaults) => {\r\n                this.loadingService.dismiss().then(_ => {\r\n                  console.log('configAndDefaults: ', configAndDefaults);\r\n                  if (configAndDefaults) {\r\n                    if (configAndDefaults.code && configAndDefaults.code !== 200) {\r\n                      this.splashScreen.hide();\r\n                      this.openAlertModal('fail', 'Failed', configAndDefaults.message, 'Ok');\r\n                    } else {\r\n                      this.loginService.setAuthentication(auth);\r\n                      this.permissionValidatorService.setAuthentication(auth);\r\n                      this.loginService.setConfigAndDefaults(configAndDefaults);\r\n                      this.loginService.setTestServerIdent(auth.isTestServer);\r\n                      auth.user.permissions = configAndDefaults['user-permissions'].map((i:any)=>new Permission(i));\r\n                      this.storageService.set('authentication',JSON.stringify(auth));\r\n                      this.navController.navigateRoot('/menu').then(_ => {\r\n                        this.splashScreen.hide();\r\n                      });\r\n                    }\r\n                  }\r\n                });\r\n              }, err => {\r\n                console.log('Failed', err);\r\n                this.loadingService.dismiss().then(_ => {\r\n                  this.splashScreen.hide();\r\n                  this.storageService.remove('authentication');\r\n                  const message = this.stringUtilsService.parseErrorMessage(err);\r\n                  this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n                });\r\n              });\r\n            } else {\r\n              this.loadingService.dismiss().then(_ => {\r\n                this.splashScreen.hide();\r\n              });\r\n            }\r\n          });\r\n        }\r\n      });\r\n  }\r\n\r\n  ngOnInit() {\r\n\r\n  }\r\n\r\n  viewPassword() {\r\n    const password = ( document.getElementById('password') as HTMLInputElement);\r\n    if (password.type === 'password') {\r\n      password.type = 'text';\r\n      this.showPassword = true;\r\n    } else {\r\n      password.type = 'password';\r\n      this.showPassword = false;\r\n    }\r\n  }\r\n\r\n  generateRandomText(length: number, divider: number = 0) {\r\n    let result = '';\r\n    const characters  = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';\r\n    const charactersLength = characters.length;\r\n    for (let i = 0; i < length; i++ ) {\r\n      if (divider > 0 && i >= divider && i % divider === 0) {\r\n        result += '-';\r\n      }\r\n      result += characters.charAt(Math.floor(Math.random() * charactersLength));\r\n    }\r\n    return result;\r\n  }\r\n\r\n  addDefaultCredentials() {\r\n    if (!this.loginCredentials.device_uuid) {\r\n      this.loginCredentials.device_uuid = this.generateRandomText(16, 4);\r\n    }\r\n    if (!this.loginCredentials.device_device_unique_id) {\r\n      this.loginCredentials.device_device_unique_id = this.generateRandomText(16, 4);\r\n    }\r\n    if (!this.loginCredentials.device_model) {\r\n      this.loginCredentials.device_model = 'model-' + this.generateRandomText(5);\r\n    }\r\n    if (!this.loginCredentials.device_platform) {\r\n      this.loginCredentials.device_platform = 'platform-' + this.generateRandomText(5);\r\n    }\r\n  }\r\n\r\n  loginClick() {\r\n    \r\n    this.barcodeScanner.scan().then(barcodeData => {\r\n      console.log('Barcode data', barcodeData);\r\n    }).catch(err => {\r\n        console.log('Error', err);\r\n    });\r\n    return false;\r\n    this.loginCredentials.username = this.username;\r\n    this.loginCredentials.password = this.password;\r\n    this.addDefaultCredentials();\r\n    this.errorMessage = '';\r\n\r\n    this.loadingService.present();\r\n    console.log('login clicked', this.loginCredentials);\r\n    this.loginApiService.login(this.loginCredentials).subscribe((authentication) => {\r\n      console.log(authentication);\r\n      if (authentication && authentication.code && authentication.code === 200) {\r\n        console.log('Success');\r\n        this.loginService.setAuthentication(authentication);\r\n        this.loginService.setTestServerIdent(authentication.isTestServer);\r\n        this.permissionValidatorService.setAuthentication(authentication);\r\n        this.loginApiService.setToken(authentication.token);\r\n        this.storageService.setObject('authentication', authentication).then(_ => {\r\n          this.loginApiService.getConfigAndDefaults().subscribe((configAndDefaults) => {\r\n            this.loadingService.dismiss().then(_ => {\r\n              if (configAndDefaults && configAndDefaults.code && configAndDefaults.code !== 200) {\r\n                this.openAlertModal('fail', 'Failed', configAndDefaults.message, 'Ok');\r\n              } else {\r\n                this.storageService.setObject('credentials', this.loginCredentials);\r\n                this.loginService.setConfigAndDefaults(configAndDefaults);\r\n                this.navController.navigateRoot('/menu');\r\n              }\r\n            });\r\n          }, err => {\r\n            console.log('Failed', err);\r\n            this.loadingService.dismiss().then(_ => {\r\n              const message = this.stringUtilsService.parseErrorMessage(err);\r\n              this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n            });\r\n          });\r\n        }).catch(err => {\r\n          console.log('error',  err);\r\n          this.loadingService.dismiss();\r\n        });\r\n      } else {\r\n        console.log('Login Failed');\r\n        this.loadingService.dismiss().then(_ => {\r\n          this.openAlertModal('fail', 'Failed', authentication.message, 'Ok');\r\n        });\r\n      }\r\n    }, err => {\r\n      console.log(err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        if (err.status !== 401) {\r\n          this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n        } else {\r\n          this.errorMessage = message;\r\n        }\r\n      });\r\n    });\r\n  }\r\n\r\n  onUsernameFocus(event: any) {\r\n    console.log('onUsernameFocus');\r\n    this.scrollTo('username');\r\n  }\r\n\r\n  onPasswordFocus(event: any) {\r\n    console.log('onPasswordFocus');\r\n    this.scrollTo('password');\r\n  }\r\n\r\n  setUrl() {\r\n    this.presentPrompt();\r\n  }\r\n\r\n  async presentPrompt() {\r\n    let defaultUrl = this.apiService.getUrl();\r\n    if (!defaultUrl) {\r\n      defaultUrl = 'http://';\r\n    }\r\n    const alert = this.alertController.create({\r\n      message: 'Set Url',\r\n      inputs: [\r\n        {\r\n          name: 'url_online',\r\n          type: 'radio',\r\n          label: 'Online',\r\n          value: environment.url,\r\n          checked: defaultUrl == environment.url\r\n        },\r\n        {\r\n          name: 'url_local',\r\n          type: 'radio',\r\n          label: 'Local',\r\n          value: environment.url_local,\r\n          checked: defaultUrl == environment.url_local\r\n        }\r\n      ],\r\n      buttons: [\r\n        {\r\n          text: 'Others',\r\n          handler: data => {\r\n            console.log('Others clicked');\r\n            this.presentPromptOthersurl();\r\n          }\r\n        },\r\n        {\r\n          text: 'Cancel',\r\n          handler: data => {\r\n            console.log('Cancel clicked');\r\n          }\r\n        },\r\n        {\r\n          text: 'OK',\r\n          handler: data => {\r\n            let url = data;\r\n\r\n            if (!url.startsWith('http://')) {\r\n              url = 'http://' + url;\r\n            }\r\n            console.log(url);\r\n            this.storageService.set('url', url).then(_ => {\r\n              this.apiService.setUrl(url);\r\n              this.loginApiService.setUrl(this.apiService.getUrl());\r\n            });\r\n          }\r\n        }\r\n      ]\r\n    }).then(alert => {\r\n      alert.present();\r\n    });\r\n  }\r\n\r\n\r\n  async presentPromptOthersurl() {\r\n    let defaultUrl = 'http://';\r\n    \r\n    const alert = this.alertController.create({\r\n      message: 'Set Url',\r\n      inputs: [\r\n        {\r\n          name: 'url',\r\n          label: 'Others',\r\n          type:'text',\r\n          placeholder: 'URL',\r\n          value: defaultUrl\r\n        }\r\n      ],\r\n      buttons: [\r\n        {\r\n          text: 'Cancel',\r\n          handler: data => {\r\n            console.log('Cancel clicked');\r\n          }\r\n        },\r\n        {\r\n          text: 'OK',\r\n          handler: data => {\r\n            let url = data.url;\r\n\r\n            if (!url.startsWith('http://')) {\r\n              url = 'http://' + url;\r\n            }\r\n            console.log(url);\r\n            this.storageService.set('url', url).then(_ => {\r\n              this.apiService.setUrl(url);\r\n              this.loginApiService.setUrl(this.apiService.getUrl());\r\n            });\r\n          }\r\n        }\r\n      ]\r\n    }).then(alert => {\r\n      alert.present();\r\n    });\r\n  }\r\n\r\n  async openAlertModal(alert: string, title: string, message: string, ok: string = 'Done') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      // cssClass: 'auto-height',\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        alert,\r\n        title,\r\n        message,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n  tapCoordinates(e) {\r\n    this.y = e.touches[0].clientY;\r\n    this.h = window.innerHeight;\r\n    this.offsetY = (this.h - this.y);\r\n    console.log('offset: ', this.offsetY);\r\n  }\r\n\r\n  keyboardShowHandler(e) {\r\n    const kH = e.keyboardHeight;\r\n    console.log('show: ', e.keyboardHeight);\r\n    const bodyMove = document.querySelector('ion-app') as HTMLElement, bodyMoveStyle = bodyMove.style;\r\n\r\n    // Adjust full keyboard height\r\n    bodyMoveStyle.bottom = kH + 'px';\r\n    /*\r\n    if (this.offsetY < kH + 40) {\r\n      bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';\r\n      // bodyMoveStyle.top = 'initial';\r\n    }\r\n    */\r\n    console.log(bodyMoveStyle.bottom);\r\n  }\r\n\r\n  keyboardHideHandler() {\r\n    console.log('gone');\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n\r\n  removeIonAppStyles() {\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n\r\n  scrollTo(element: string) {\r\n    const htmlElement = document.getElementById(element);\r\n    const offsetTop = htmlElement.offsetTop;\r\n    const offsetHeight = htmlElement.offsetHeight;\r\n\r\n    console.log('offsetTop: ', offsetTop);\r\n    console.log('offsetHeight: ', offsetHeight, (offsetHeight * 2));\r\n    this.content.scrollToPoint(0, offsetTop + (offsetHeight * 2), 500);\r\n  }\r\n}\r\n","import { Injectable } from '@angular/core';\r\nimport { Storage } from '@ionic/storage';\r\n\r\n@Injectable({\r\n  providedIn: 'root'\r\n})\r\nexport class StorageService {\r\n\r\n  constructor(public storage: Storage) { }\r\n\r\n  async set(key: string, value: any): Promise<any> {\r\n    try {\r\n      const result = await this.storage.set(key, value);\r\n      console.log('set string in storage: ' + result);\r\n      return true;\r\n    } catch (reason) {\r\n      console.log(reason);\r\n      return false;\r\n    }\r\n  }\r\n\r\n  async get(key: string): Promise<any> {\r\n    try {\r\n      const result = await this.storage.get(key);\r\n      console.log('storageGET: ' + key + ': ' + result);\r\n      if (result != null) {\r\n        return result;\r\n      }\r\n      return null;\r\n    } catch (reason) {\r\n      console.log(reason);\r\n      return null;\r\n    }\r\n  }\r\n\r\n  async setObject(key: string, object: any) {\r\n    try {\r\n      const result = await this.storage.set(key, JSON.stringify(object));\r\n      console.log('set Object in storage: ' + result);\r\n      return true;\r\n    } catch (reason) {\r\n      console.log(reason);\r\n      return false;\r\n    }\r\n  }\r\n\r\n  async getObject(key: string): Promise<any> {\r\n    try {\r\n      const result = await this.storage.get(key);\r\n      if (result != null) {\r\n        return JSON.parse(result);\r\n      }\r\n      return null;\r\n    } catch (reason) {\r\n      console.log(reason);\r\n      return null;\r\n    }\r\n  }\r\n\r\n  remove(key: string) {\r\n    this.storage.remove(key);\r\n  }\r\n\r\n  clear() {\r\n    this.storage.clear();\r\n  }\r\n}\r\n"],"sourceRoot":"webpack:///","file":"pages-login-login-module-es5.js"}