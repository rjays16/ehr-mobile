.get('note');\r\n      if (note) {\r\n        const n: any = JSON.parse(note);\r\n        console.log('selectedNote: ', n);\r\n        const darnotes = n.darnotes;\r\n        this.id = n.document_id;\r\n        this.title = darnotes.focus;\r\n        this.data = darnotes.data;\r\n        this.action = darnotes.action;\r\n        this.response = darnotes.response;\r\n      }\r\n    }\r\n  }\r\n\r\n  ionViewWillEnter() {\r\n    console.log('ionViewWillEnter');\r\n    window.addEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.addEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n  }\r\n\r\n  ionViewWillLeave() {\r\n    console.log('ionViewWillLeave');\r\n    window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n    this.removeIonAppStyles();\r\n  }\r\n\r\n  saveNotes(event: any) {\r\n    console.log(event.form.value);\r\n    const n = event.form.value;\r\n    const darNote = {\r\n      focus: n.note_title,\r\n      data: n.note_data ? n.note_data   : '',\r\n      action: n.note_action ? n.note_action : '',\r\n      response: n.note_response ? n.note_response : '',\r\n      document_id: this.id ? this.id : ''\r\n    };\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.SAVE_DAR_NOTE, { darNote }, 'DAR Notes', 'Would you like to save this DAR Note?', 'Save');\r\n  }\r\n\r\n  saveNotesFunction(darNote: DarNotes) {\r\n    console.log('darNote: ', darNote);\r\n    this.loadingService.present();\r\n    this.nurseDarNotesApiService.saveNurseDarNote(this.selectedEncounterNo, darNote).subscribe((response) => {\r\n      console.log(response);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (response.code && response.code === 200) {\r\n          this.nurseDarNotesDataService.setData(response.data.data.nurseListsOfNotes);\r\n          console.log('nurseListsOfNotes: ', response.data.data.nurseListsOfNotes);\r\n          this.openAlertModal('success', 'Success', response.message);\r\n        } else {\r\n          this.openAlertModal('fail', 'Failed', response.message, 'Ok');\r\n        }\r\n      });\r\n    }, err => {\r\n      console.log(err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n\r\n  }\r\n\r\n  backToPatientChart() {\r\n    console.log('back');\r\n    this.navCtrl.pop();\r\n  }\r\n\r\n  async openPromptModal(option: number, data: any, title: string, message: string,  ok: string = 'Save', cancel: string = 'Cancel') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        prompt,\r\n        title,\r\n        message,\r\n        cancel,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (result.data && result.data.button === 'Save') {\r\n          if (option === this.SAVE_DAR_NOTE) {\r\n            this.saveNotesFunction(data.darNote);\r\n          }\r\n        }\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n  async openAlertModal(alert: string, title: string, message: string, ok: string = 'Done') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        alert,\r\n        title,\r\n        message,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (alert === 'success' && result.data.button === 'Done') {\r\n          this.backToPatientChart();\r\n        }\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n  tapCoordinates(e) {\r\n    this.y = e.touches[0].clientY;\r\n    this.h = window.innerHeight;\r\n    this.offsetY = (this.h - this.y);\r\n    console.log('offset: ', this.offsetY);\r\n  }\r\n\r\n  keyboardShowHandler(e) {\r\n    const kH = e.keyboardHeight;\r\n    console.log('show: ', e.keyboardHeight);\r\n    const bodyMove = document.querySelector('ion-app') as HTMLElement, bodyMoveStyle = bodyMove.style;\r\n\r\n    // Adjust full keyboard height\r\n    bodyMoveStyle.bottom = kH + 'px';\r\n    /*\r\n    if (this.offsetY < kH + 40) {\r\n      bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';\r\n      // bodyMoveStyle.top = 'initial';\r\n    }\r\n    */\r\n    console.log(bodyMoveStyle.bottom);\r\n  }\r\n\r\n  keyboardHideHandler() {\r\n    console.log('gone');\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n\r\n  removeIonAppStyles() {\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n}\r\n"],"sourceRoot":"webpack:///","file":"dar-note-dar-note-module-es5.js"}