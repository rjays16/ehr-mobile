any) => {\r\n      console.log('result: ', result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && result.filepath) {\r\n          if ((result.filepath.indexOf('.pdf') > -1) || (result.filepath.indexOf('.html') > -1)) {\r\n            console.log('result.filepath: ', result.filepath);\r\n            this.openWithInAppBrowser(result.filepath);\r\n          }\r\n        }\r\n      });\r\n    }, err => {\r\n      console.log(err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  openWithInAppBrowser(url: string) {\r\n    this.inAppBrowser.create(url, '_system', this.options);\r\n  }\r\n\r\n  viewDoctorOrder() {\r\n    this.router.navigate(['/menu/plan-management/doctor-order', {action: this.ACTION_ADD}]);\r\n  }\r\n\r\n  viewReferralOrder() {\r\n    if (this.referralList.length > 0) {\r\n      const selectedOrder = this.referralList[0];\r\n      this.router.navigate(['/menu/plan-management/referral-order', {order: JSON.stringify(selectedOrder), action: this.ACTION_EDIT}]);\r\n    } else {\r\n      this.router.navigate(['/menu/plan-management/referral-order', {action: this.ACTION_ADD}]);\r\n    }\r\n  }\r\n\r\n  viewRadiologyOrder() {\r\n    if (this.radiologyOrdersList.length > 0) {\r\n      // tslint:disable-next-line: max-line-length\r\n      this.router.navigate(['/menu/plan-management/radiology-order', {action: this.ACTION_EDIT, radiologyList: JSON.stringify(this.radiologyOrdersList)}]);\r\n    } else {\r\n      this.router.navigate(['/menu/plan-management/radiology-order', {action: this.ACTION_ADD}]);\r\n    }\r\n  }\r\n\r\n  viewLaboratoryOrder() {\r\n    if (this.laboratoryOrdersList.length > 0) {\r\n      // tslint:disable-next-line: max-line-length\r\n      this.router.navigate(['/menu/plan-management/laboratory-order', {action: this.ACTION_ED