on();\r\n    this.planManagementApiService.setToken(this.authentication.token);\r\n    this.planManagementApiService.setUrl(this.apiService.getUrl());\r\n    this.selectedEncounterNo = this.patientService.getSelectedEncounterNo();\r\n\r\n    this.configAndDefaults = this.loginService.getConfigAndDefaults();\r\n    this.referralDefaultOptions = this.configAndDefaults.referral['m-patient-referral']['default-options'];\r\n    console.log(this.referralDefaultOptions);\r\n\r\n    const t = this.referralDefaultOptions['refer-types'];\r\n    const ts = [];\r\n    Object.keys(t).forEach(key => {\r\n      const p = {\r\n        id: key,\r\n        desc: t[key]\r\n      };\r\n      ts.push(p);\r\n    });\r\n    this.referralTypes = ts;\r\n    this.referralDepartments = this.referralDefaultOptions['refer-department'];\r\n    this.referralReasons = this.referralDefaultOptions['refer-reason'];\r\n  }\r\n\r\n  initUserInterfaceByRole() {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pPatientCoursewardSave = this.permissionValidatorService.hasPlanManEdit();\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pPatientLaboratorySave = false;\r\n    // tslint:disable-next-line: max-line-length\r\n    this.prescriptionSave = this.permissionValidatorService.hasPlanManEdit();\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pPatientRadiologySave = false;\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pPatientReferralSave = this.permissionValidatorService.hasPlanManEdit();\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pPatientRepetetiveSave = this.permissionValidatorService.hasPlanManEdit();\r\n\r\n    this.setActiveTab();\r\n    console.log('this.pPatientCoursewardSave: ', this.pPatientCoursewardSave);\r\n    console.log('this.pPatientLaboratorySave: ', this.pPatientLaboratorySave);\r\n    console.log('this.prescriptionSave: ', this.prescriptionSave);\r\n    console.log('this.pPatientRadiologySave: ', this.pPatientRadiologySave);\r\n    console.log('this.pPatientReferralSave: ', this.pPatientReferralSave);\r\n    console.log('this.pPatientRepetetiveSave: ', this.pPatientRepetetiveSave);\r\n  }\r\n\r\n\r\n  setActiveTab() {\r\n    this.selectedTab = (this.pPatientReferralSave || this.pPatientCoursewardSave || this.prescriptionSave || this.pPatientRadiologySave || this.pPatientLaboratorySave || this.pPatientRepetetiveSave) ? 'request-order' : 'finalized-orders';\r\n  }\r\n\r\n  ionViewWillEnter() {\r\n    console.log('ionViewWillEnter');\r\n    const tempSelectedEncounterNo = this.patientService.getSelectedEncounterNo();\r\n    if (this.selectedEncounterNo !== tempSelectedEncounterNo) {\r\n      console.log('selected patient changed');\r\n      this.selectedEncounterNo = tempSelectedEncounterNo;\r\n      this.initUserInterfaceByRole();\r\n      this.getAllOrders();\r\n    }\r\n\r\n    const unfinalizedBatchOrders = this.planManagementDataService.getUnfinalizedBatch();\r\n    if (unfinalizedBatchOrders) {\r\n      console.log('unfinalizedOrders: ', unfinalizedBatchOrders);\r\n      this.unfinalizedOrderList = [];\r\n      const unfinalizedBatchCopy = unfinalizedBatchOrders[0];\r\n      const unfinalizedBatch = JSON.parse(JSON.stringify(unfinalizedBatchCopy));\r\n      this.parseBatch(unfinalizedBatch);\r\n\r\n      this.prescriptionList =  unfinalizedBatch.prescriptionOrders;\r\n\r\n      this.radiologyOrdersList =  unfinalizedBatch.radiologyOrders;\r\n\r\n      this.laboratoryOrdersList =  unfinalizedBatch.laboratoryOrders;\r\n\r\n      this.repetitiveOrdersList =  unfinalizedBatch.repetitiveOrders;\r\n\r\n      this.doctorOrderOrActionList =  unfinalizedBatch.doctorOrdersOrActions;\r\n      this.doctorOrderOrActionList.forEach(item => {\r\n        item.create_by = unfinalizedBatch.doctor;\r\n      });\r\n\r\n      this.referralList =  unfinalizedBatch.referralOrders;\r\n      this.referralList.forEach(item => {\r\n        if (typeof item.referral_date === 'string') {\r\n          item.referral_date = Number(item.referral_date);\r\n        }\r\n        item.create_by = unfinalizedBatch.doctor;\r\n      });\r\n\r\n      this.unfinalizedOrderList.push(unfinalizedBatch);\r\n      console.log('this.unfinalizedOrderList: ', this.unfinalizedOrderList);\r\n      this.planManagementDataService.setUnfinalizedBatch(null);\r\n    }\r\n  }\r\n\r\n  ngOnInit() {\r\n    this.menuController.swipeGesture(true, 'mainMenu');\r\n    this.menuController.swipeGesture(true, 'accountMenu');\r\n    this.initUserInterfaceByRole();\r\n    this.getAllOrders();\r\n  }\r\n\r\n  getAllOrders(): Promise<any> {\r\n    return new Promise((resolve, reject) => {\r\n      this.loadingService.present();\r\n      this.planManagementApiService.getAllOrders(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n        this.resetRequestOrders();\r\n        try {\r\n          this.parseBatchOrders(result.batchOrders);\r\n\r\n          this.loadingService.dismiss().then(_ => {\r\n            resolve(result);\r\n          });\r\n        } catch (error) {\r\n          this.loadingService.dismiss().then(_ => {\r\n            this.alertServ.openAlertModal('fail', 'Failed', 'Cannot parse Batch Order List values!', 'Ok');\r\n            reject(error);\r\n          });\r\n        }\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  parseBatchOrders(batchOrders: any) {\r\n    this.unfinalizedOrderList = [];\r\n    this.orderList = batchOrders;\r\n\r\n    this.orderList.forEach(order => {\r\n      this.parseBatch(order);\r\n    });\r\n\r\n    const unfinalizedIndex = this.orderList.findIndex(item => {\r\n      return item.finalized === false;\r\n    });\r\n\r\n    // if not finalized add list to request orders list (tab)\r\n    console.log('unfinalizedIndex: ', unfinalizedIndex);\r\n    if (unfinalizedIndex >= 0) {\r\n      const unfinalizedBatchCopy =  this.orderList[unfinalizedIndex];\r\n      const unfinalizedBatch = JSON.parse(JSON.stringify(unfinalizedBatchCopy));\r\n\r\n      console.log('unfinalizedBatch: ', unfinalizedBatch);\r\n      this.prescriptionList =  unfinalizedBatch.prescriptionOrders;\r\n\r\n      this.radiologyOrdersList =  unfinalizedBatch.radiologyOrders;\r\n\r\n      this.laboratoryOrdersList =  unfinalizedBatch.laboratoryOrders;\r\n\r\n      this.repetitiveOrdersList =  unfinalizedBatch.repetitiveOrders;\r\n\r\n      this.doctorOrderOrActionList =  unfinalizedBatch.doctorOrdersOrActions;\r\n      this.doctorOrderOrActionList.forEach(item => {\r\n        item.create_by = unfinalizedBatch.doctor;\r\n      });\r\n\r\n      this.referralList =  unfinalizedBatch.referralOrders;\r\n      this.referralList.forEach(item => {\r\n        if (typeof item.referral_date === 'string') {\r\n          item.referral_date = Number(item.referral_date);\r\n        }\r\n        item.create_by = unfinalizedBatch.doctor;\r\n      });\r\n\r\n      this.unfinalizedOrderList.push(unfinalizedBatch);\r\n\r\n      // remove unfinalized batch\r\n      this.orderList.splice(unfinalizedIndex, 1);\r\n    }\r\n\r\n    console.log('this.orderList: ', this.orderList);\r\n    console.log('this.unfinalizedOrderList: ', this.unfinalizedOrderList);\r\n  }\r\n\r\n  parseBatch(order: any) {\r\n    const dateObject = new Date(order.created * 1000);\r\n    order.created_date = moment(dateObject).format('MMM DD,YY h:mm a');\r\n\r\n    order.laboratoryOrders = order.orders.filter(item => {\r\n      // tslint:disable-next-line: max-line-length\r\n      return item.kardexGroup && item.kardexGroup.toLocaleLowerCase() === 'Diagnostic'.toLocaleLowerCase() && item.sname === 'LABORATORY';\r\n    });\r\n\r\n    order.radiologyOrders = order.orders.filter(item => {\r\n      // tslint:disable-next-line: max-line-length\r\n      return item.kardexGroup && item.kardexGroup.toLocaleLowerCase() === 'Diagnostic'.toLocaleLowerCase() && item.sname === 'RADIOLOGY';\r\n    });\r\n\r\n    order.prescriptionOrders = order.orders.filter(item => {\r\n      return item.kardexGroup && item.kardexGroup.toLocaleLowerCase() === 'Pharmacologic/Medication'.toLocaleLowerCase();\r\n    });\r\n\r\n    order.referralOrders = order.orders.filter(item => {\r\n      return item.kardexGroup && item.kardexGroup.toLocaleLowerCase() === 'Referral'.toLocaleLowerCase();\r\n    });\r\n\r\n    if (order.referralOrders.length > 0) {\r\n      order.referralOrders.forEach(element => {\r\n        console.log('element: ', element);\r\n        const dept = this.referralDepartments.find(item => {\r\n          return element.department == item.id;\r\n        });\r\n\r\n        console.log('dept: ', dept);\r\n        const rea = this.referralReasons.find(item => {\r\n          return element.reason == item.id;\r\n        });\r\n        console.log('rea: ', rea);\r\n\r\n        element.dept_desc = dept ? dept.area_desc : '';\r\n        element.purpose_desc = rea ? rea.purpose_desc : '';\r\n      });\r\n    }\r\n\r\n    order.doctorOrdersOrActions = order.orders.filter(item => {\r\n      return item.kardexGroup && item.kardexGroup.toLocaleLowerCase() === 'Doctor Order / Action'.toLocaleLowerCase();\r\n    });\r\n\r\n    order.repetitiveOrders = order.orders.filter(item => {\r\n      return item.kardexGroup && item.kardexGroup.toLocaleLowerCase() === 'Repetitive'.toLocaleLowerCase();\r\n    });\r\n\r\n  }\r\n\r\n  onSegmentChange(event: any) {\r\n    console.log(event);\r\n  }\r\n\r\n  saveTempPrescriptionList() {\r\n    console.log('saveTempPrescriptionList');\r\n  }\r\n\r\n  resetTempPrescriptionList() {\r\n    console.log('resetTempPrescriptionList');\r\n  }\r\n\r\n  deletePrescription(index: number) {\r\n    this.openPromptModal(this.DELETE_PRESCRIPTION, { index }, 'Prescription Order', 'Would you like to delete this item?', 'Delete');\r\n  }\r\n\r\n  deleteRepetitiveUnfinalized(index: number) {\r\n    this.openPromptModal(this.DELETE_REPETITIVE_ORDER, { index }, 'Repetitive Order', 'Would you like to delete this item?', 'Delete');\r\n  }\r\n\r\n  deleteRepetitivefinalized(index: number, itemIndex: number) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.DELETE_FINALIZED_REPETITIVE_ORDER, { index , itemIndex}, 'Repetitive Order', 'Would you like to delete this finalized item?', 'Delete');\r\n  }\r\n\r\n  deletePrescriptionFunction(index: number) {\r\n    console.log('this.prescriptionList: ', this.prescriptionList);\r\n\r\n    // create a temp prescription list\r\n    const prescriptions = [];\r\n\r\n    // Recreate items to be sent\r\n    this.prescriptionList.forEach(element => {\r\n      const prescription = JSON.parse(JSON.stringify(element));\r\n      // Delete unnecessary keys\r\n      if (prescription.is_custom === 0) {\r\n        delete prescription.is_custom;\r\n        prescription.item = prescription.item.id;\r\n      }\r\n\r\n      if (prescription.order_check === true || prescription.order_check === false) {\r\n        prescription.check = prescription.order_check;\r\n      }\r\n      if (!prescription.date) {\r\n        prescription.date = prescription.order_date;\r\n      }\r\n      if (!prescription.time) {\r\n        prescription.time = prescription.order_time;\r\n      }\r\n\r\n      delete prescription.order_check;\r\n      delete prescription.order_date;\r\n      delete prescription.order_time;\r\n\r\n      if (prescription.order_check) {\r\n        delete prescription.order_check;\r\n      }\r\n\r\n      // Delete keys if isEmpty or null\r\n      Object.keys(prescription).forEach(key => {\r\n        if (typeof prescription[key] === 'string' && prescription[key].length === 0) {\r\n          delete prescription[key];\r\n        } else if (prescription[key] === null) {\r\n          delete prescription[key];\r\n        }\r\n      });\r\n\r\n      delete prescription.cash;\r\n      delete prescription.id;\r\n      delete prescription.is_ehr;\r\n      delete prescription.kardexGroup;\r\n      delete prescription.kardexGroupRef;\r\n      delete prescription.order_date_new;\r\n      delete prescription.order_time_ampm;\r\n      delete prescription.refno;\r\n      delete prescription.stat;\r\n      delete prescription.status;\r\n\r\n      prescriptions.push(prescription);\r\n    });\r\n\r\n    // Have a copy of to be remove prescription\r\n    const toBeDeletedPrescription = JSON.parse(JSON.stringify(this.prescriptionList[index]));\r\n    console.log('toBeDeletedPrescription: ', toBeDeletedPrescription);\r\n\r\n    // Remove the prescription you wish to delete in the temp list before submitting\r\n    prescriptions.splice(index, 1);\r\n    console.log('prescriptions: ', prescriptions);\r\n\r\n    this.loadingService.present();\r\n    const encounter_no = this.selectedEncounterNo;\r\n    this.planManagementApiService.savePrescription(encounter_no, prescriptions).subscribe((response) => {\r\n      console.log(response);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (response.code && response.code === 200) {\r\n          this.alertServ.openAlertModal('success', 'Success', response.message);\r\n          const i = this.prescriptionList.findIndex(item => {\r\n            return toBeDeletedPrescription.id === item.id;\r\n          });\r\n          this.prescriptionList.splice(i, 1);\r\n        } else {\r\n          this.alertServ.openAlertModal('fail', 'Failed', response.message, 'Ok');\r\n        }\r\n      });\r\n    }, err => {\r\n      console.log(err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n\r\n  }\r\n\r\n  editPrescription(index: number) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.router.navigate(['/menu/plan-management/prescription-orders', {action: this.ACTION_EDIT, index, prescriptionList: JSON.stringify(this.prescriptionList)}]);\r\n  }\r\n\r\n  deleteDoctorOrderOrAction(index: number) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.DELETE_DOCTOR_ORDER_OR_ACTION, { index }, 'Doctor\\'s Order/Action', 'Would you like to delete this item?', 'Delete');\r\n  }\r\n\r\n  deleteDoctorOrderOrActionFunction(index: number) {\r\n    const selectedOrder = this.doctorOrderOrActionList[index];\r\n    console.log('selectedOrder: ', selectedOrder);\r\n\r\n    const encounter_no = this.selectedEncounterNo;\r\n    const id = selectedOrder.id;\r\n\r\n    this.loadingService.present();\r\n    this.planManagementApiService.deleteCourseward(encounter_no, id).subscribe((result: any) => {\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 === result.code) {\r\n          const i = this.doctorOrderOrActionList.findIndex(item => {\r\n            return id === item.id;\r\n          });\r\n          this.doctorOrderOrActionList.splice(i, 1);\r\n\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  editDoctorOrderOrAction(index: number) {\r\n    const selectedOrder = this.doctorOrderOrActionList[index];\r\n    this.router.navigate(['/menu/plan-management/doctor-order', {order: JSON.stringify(selectedOrder), action: this.ACTION_EDIT}]);\r\n  }\r\n\r\n  deleteLaboratoryOrder(index: number) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.DELETE_DIAGNOSTICS_LABORATORY_ORDER, { index }, 'Laboratory Order', 'Would you like to delete this item?', 'Delete');\r\n  }\r\n\r\n  deleteLaboratoryOrderFunction(index: number) {\r\n    const selectedOrder = this.laboratoryOrdersList[index];\r\n    console.log('selectedOrder: ', selectedOrder);\r\n\r\n    const encounter_no = this.selectedEncounterNo;\r\n    const id = selectedOrder.id;\r\n\r\n    this.loadingService.present();\r\n    this.planManagementApiService.deleteDiagnosticLaboratory(encounter_no, id).subscribe((result: any) => {\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 === result.code) {\r\n          const i = this.laboratoryOrdersList.findIndex(item => {\r\n            return id === item.id;\r\n          });\r\n          this.laboratoryOrdersList.splice(i, 1);\r\n\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  editLaboratoryOrder(index: number) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.router.navigate(['/menu/plan-management/laboratory-order', {action: this.ACTION_EDIT, index, laboratoryList: JSON.stringify(this.laboratoryOrdersList)}]);\r\n  }\r\n\r\n  deleteRadiologyOrder(index: number) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.DELETE_DIAGNOSTICS_RADIOLOGY_ORDER, { index }, 'Radiology Order', 'Would you like to d