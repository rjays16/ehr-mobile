this.loadingService.present();\r\n      this.patientApiService.getPatientProfileById(this.encounter_no).subscribe((result) => {\r\n        console.log(result);\r\n        this.loadingService.dismiss().then(_ => {\r\n          this.patient = result;\r\n          console.log(this.patient);\r\n          this.encounteTypeColor = this.patient.is_discharged == 1 ? this.otherDefaultOptionsColors.discharged : this.otherDefaultOptionsColors[this.patient.current_dept_encounter.deptenc_code];\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n        });\r\n      });\r\n    } else if (this.isNurse) {\r\n      this.loadingService.present();\r\n      this.patientApiService.getPatientProfileByIdForNurse(this.encounter_no).subscribe((result) => {\r\n        console.log(result);\r\n        this.loadingService.dismiss().then(_ => {\r\n          this.patient = result;\r\n          console.log(this.patient);\r\n          this.encounteTypeColor = this.patient.is_discharged == 1 ? this.otherDefaultOptionsColors.discharged : this.otherDefaultOptionsColors[this.patient.current_dept_encounter.deptenc_code];\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n        });\r\n      });\r\n    }\r\n  }\r\n\r\n  ionViewWillEnter() {\r\n    console.log('ionViewWillEnter');\r\n    const tempSelectedEncounterNo = this.patientService.getTempEncounterNo();\r\n    if (this.encounter_no !== tempSelectedEncounterNo) {\r\n      console.log('selected patient changed');\r\n      this.encounter_no = tempSelectedEncounterNo;\r\n      this.patientService.setTempEncounterNo('');\r\n      this.initEncounterDetails();\r\n    }\r\n  }\r\n\r\n  ngOnInit() {\r\n    this.initEncounterDetails();\r\n  }\r\n\r\n  backToHistory() {\r\n    console.log('back');\r\n    this.navCtrl.pop();\r\n  }\r\n\r\n  async openAlertModal(alert: string, title: string, message: string, ok: string = 'Done') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        alert,\r\n        title,\r\n        message,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n}\r\n"],"sourceRoot":"webpack:///","file":"encounter-details-encounter-details-module-es5.js"}