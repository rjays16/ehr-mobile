smiss().then(_ => {\r\n        if (response.code && response.code === 200) {\r\n          if (response.carryoutOrders) {\r\n            this.nurseCarryOutOrdersDataService.setCarryOutOrders(response.carryoutOrders);\r\n          }\r\n          this.openAlertModal('success', 'Success', response.message);\r\n        } else {\r\n          this.openAlertModal('fail', 'Failed', response.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  async openPromptModal(option: number, data: any, title: string, message: string,  ok: string = 'Save', cancel: string = 'Cancel') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        prompt,\r\n        title,\r\n        message,\r\n        cancel,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (result.data && result.data.button === 'Carry Out') {\r\n          if (option === this.CARRY_OUT_SELECTED_ORDERS) {\r\n            this.carryOutSelectedOrdersFunction(data.selectedOrders);\r\n          }\r\n        }\r\n      }\r\n    });\r\n\r\n    return await modal.present();\r\n  }\r\n\r\n  async openAlertModal(alert: string, title: string, message: string, ok: string = 'Done') {\r\n    const modal = await this.modalController.create({\r\n      component: PromptModalPage,\r\n      cssClass: 'my-custom-modal-css',\r\n      componentProps: {\r\n        alert,\r\n        title,\r\n        message,\r\n        ok\r\n      }\r\n    });\r\n\r\n    modal.onDidDismiss().then((result) => {\r\n      if (result !== null) {\r\n