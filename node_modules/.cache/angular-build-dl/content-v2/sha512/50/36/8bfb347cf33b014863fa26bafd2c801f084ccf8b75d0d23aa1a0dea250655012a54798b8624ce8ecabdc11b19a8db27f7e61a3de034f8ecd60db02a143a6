    getLightRandomColor() {
        const colorIndex = Math.floor(Math.random() * this.colors.length);
        console.log('colorIndex: ', colorIndex, 'color: ', this.colors[colorIndex]);
        return this.colors[colorIndex];
    }
    viewUnfinalizedNote(index) {
        const selectedNote = this.unfinalizedBatchNotes.darnoteslist[index];
        this.router.navigate(['/menu/patient-chart/dar-note', { note: JSON.stringify(selectedNote), action: this.ACTION_VIEW }]);
    }
    deleteUnfinalizedNote(index) {
        this.openPromptModal(this.DELETE_NURSE_DAR_NOTES, { index }, 'DAR Notes', 'Would you like to delete this DAR Note?', 'Delete');
    }
    deleteUnfinalizedNoteFunction(index) {
        const selectedNote = this.unfinalizedBatchNotes.darnoteslist[index];
        this.loadingService.present();
        this.nurseDarNotesApiService.deleteNurseDarNote(selectedNote.id).subscribe((response) => {
            console.log(response);
            this.loadingService.dismiss().then(_ => {
                if (response.code && response.code === 200) {
                    this.openAlertModal('success', 'Success', response.message);
                    const i = this.unfinalizedBatchNotes.darnoteslist.findIndex(item => {
                        return selectedNote.id === item.id;
                    });
                    this.unfinalizedBatchNotes.darnoteslist.splice(i, 1);
                }
                else {
                    this.openAlertModal('fail', 'Failed', response.message, 'Ok');
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    editUnfinalizedNote(index) {
        const selectedNote = this.unfinalizedBatchNotes.darnoteslist[index];
        this.router.navigate(['/menu/patient-chart/dar-note', { note: JSON.stringify(selectedNote), action: this.ACTION_EDIT }]);
    }
    finalizedNotes() {
        if (this.unfinalizedBatchNotes) {
            this.openPromptModal(this.FINALIZE_NURSE_DAR_NOTES, {}, 'DAR Notes', 'Would you like to finalize the DAR Notes?', 'Finalize');
        }
    }
    viewFinalizedNote(batchNoteIndex, index) {
        const selectedNote = this.batchNotesList[batchNoteIndex].darnoteslist[index];
        this.router.navigate(['/menu/patient-chart/dar-note', { note: JSON.stringify(selectedNote), action: this.ACTION_VIEW }]);
    }
    finalizedNotesFunction() {
        this.loadingService.present();
        this.nurseDarNotesApiService.finalizeNurseDarNotes(this.selectedEncounterNo, this.unfinalizedBatchNotes.id).subscribe((result) => {
            console.log(result);
            this.loadingService.dismiss().then(_ => {
                if (result && 200 === result.code) {
                    this.unfinalizedBatchNotes = null;
                    this.openAlertModal('success', 'Success', result.message).then(_ => {
                        this.getNurseDarNotes();
                    });
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    getNurseDarNotes() {
        return new Promise((resolve, reject) => {
            this.loadingService.present();
            this.nurseDarNotesApiService.getNurseDarNotes(this.selectedEncounterNo).subscribe((result) => {
                console.log(result);
                this.loadingService.dismiss().then(_ => {
                    if (result.length > 0) {
                        this.parseBatchNotes(result);
                    }
                    resolve(result);
                });
            }, (err) => {
                console.log('err ', err);
                this.loadingService.dismiss().then(_ => {
                    const message = this.stringUtilsService.parseErrorMessage(err);
                    this.openAlertModal('fail', 'Failed', message, 'Ok');
                    reject(err);
                });
            });
        });
    }
    getNurseCarryOutOrders() {
        return new Promise((resolve, reject) => {
            this.loadingService.present();
            this.nurseCarryOutOrdersApiService.getCarryOutOrders(this.selectedEncounterNo).subscribe((result) => {
                console.log(result);
                this.loadingService.dismiss().then(_ => {
                    if (result.carryoutOrders) {
                        this.parseCarryOutOrders(result.carryoutOrders);
                    }
                    resolve(result);
                });
            }, (err) => {
                console.log('err ', err);
                this.loadingService.dismiss().then(_ => {
                    const message = this.stringUtilsService.parseErrorMessage(err);
                    this.openAlertModal('fail', 'Failed', message, 'Ok');
                    reject(err);
                });
            });
        });
    }
    parseBatchNotes(batchNotesList) {
        this.unfinalizedBatchNotes = null;
        this.batchNotesList = batchNotesList;
        this.batchNotesList.forEach(notes => {
            this.parseBatch(notes);
        });
        const unfinalizedIndex = this.batchNotesList.findIndex(item => {
            return item.is_finalized === false || item.is_finalized === 0;
        });
        // if not finalized add list to request orders list (tab)
        console.log('unfinalizedIndex: ', unfinalizedIndex);
        if (unfinalizedIndex >= 0) {
            const unfinalizedBatchCopy = this.batchNotesList[unfinalizedIndex];
            const unfinalizedBatch = JSON.parse(JSON.stringify(unfinalizedBatchCopy));
            console.log('unfinalizedBatch: ', unfinalizedBatch);
            this.unfinalizedBatchNotes = unfinalizedBatch;
            // remove unfinalized batch
            this.batchNotesList.splice(unfinalizedIndex, 1);
        }
        console.log('batchNotesList: ', this.batchNotesList);
    }
    parseBatch(notes) {
        const darnoteslist = notes.notes.filter(item => {
            // tslint:disable-next-line: max-line-length
            return item.darnotes && item.darnotes !== null;
        });
        if (darnoteslist) {
            darnoteslist.forEach(note => {
                note.darnotes.bgcolor = this.getLightRandomColor();
            });
        }
        notes.darnoteslist = darnoteslist;
        let nurse_name = '';
        if (notes.personnel && notes.personnel.p) {
            const name_first = notes.personnel.p.name_first ? notes.personnel.p.name_first : '';
            const name_middle = notes.personnel.p.name_middle ? notes.personnel.p.name_middle : '';
            const name_last = notes.personnel.p.name_last ? notes.personnel.p.name_last : '';
            const suffix = notes.personnel.p.suffix ? notes.personnel.p.suffix : '';
            nurse_name = name_first + ' ' + name_middle + ' ' + name_last + ' ' + suffix;
        }
        notes.nurse_name = nurse_name;
    }
    parseCarryOutOrders(carryoutOrders) {
        this.carryoutOrders.prescriptionList = carryoutOrders.prescription;
        this.carryoutOrders.referralList = carryoutOrders.referral;
        this.carryoutOrders.doctorOrderOrActionList = carryoutOrders.courseward;
        this.carryoutOrders.laboratoryList = carryoutOrders.laboratory;
        this.carryoutOrders.radiologyList = carryoutOrders.radiology;
        if (this.carryoutOrders.referralList.length > 0) {
            this.carryoutOrders.referralList.forEach(element => {
                const ty = this.types.find(item => {
                    return element.referral_type === item.id;
                });
                element.referral_desc = ty.desc;
                console.log('element: ', element);
                const dept = this.departments.find(item => {
                    return element.department === item.id.toString() || (typeof element.department === 'number' && item.id);
                });
                console.log('dept: ', dept);
                const rea = this.reasons.find(item => {
                    return element.reason === item.id.toString() || (typeof element.reason === 'number' && item.id);
                });
                console.log('rea: ', rea);
                element.dept_desc = dept ? dept.area_desc : '';
                element.purpose_desc = rea ? rea.purpose_desc : '';
            });
        }
    }
    doRefreshPatientChart(event) {
        if (this.selectedTab === 'Orders') {
            this.getNurseCarryOutOrders().then(_ => {
                this.ordersIsLoaded = true;
                event.target.complete();
            }).catch(_ => {
                event.target.complete();
            });
        }
        else if (this.selectedTab === 'Notes') {
            this.getNurseDarNotes().then(_ => {
                this.notesIsLoaded = true;
                event.target.complete();
            }).catch(_ => {
                event.target.complete();
            });
        }
    }
    openPromptModal(option, data, title, message, ok = 'Save', cancel = 'Cancel') {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            const modal = yield this.modalController.create({
                component: _pages_prompt_modal_prompt_modal_page__WEBPACK_IMPORTED_MODULE_13__["PromptModalPage"],
                cssClass: 'my-custom-modal-css',
                componentProps: {
                    prompt,
                    title,
                    message,
                    cancel,
                    ok
                }
            });
            modal.onDidDismiss().then((result) => {
                if (result !== null) {
                    console.log('result.data: ', result.data);
                    if (result.data && result.data.button === 'Delete') {
                        if (option === this.DELETE_NURSE_DAR_NOTES) {
                            this.deleteUnfinalizedNoteFunction(data.index);
                        }
                    }
                    else if (result.data && result.data.button === 'Finalize') {
                        if (option === this.FINALIZE_NURSE_DAR_NOTES) {
                            this.finalizedNotesFunction();
                        }
                    }
                }
            });
            return yield modal.present();
        });
    }
    openAlertModal(alert, title, message, ok = 'Done') {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            const modal = yield this.modalController.create({
                component: _pages_prompt_modal_prompt_modal_page__WEBPACK_IMPORTED_MODULE_13__["PromptModalPage"],
                cssClass: 'my-custom-modal-css',
                componentProps: {
                    alert,
                    title,
                    message,
                    ok
                }
            });
            modal.onDidDismiss().then((result) => {
                if (result !== null) {
                    console.log('result.data: ', result.data);
                }
            });
            return yield modal.present();
        });
    }
    addDarNotes() {
        this.router.navigate(['/menu/patient-chart/dar-note', { action: this.ACTION_ADD }]);
    }
    openAddNotesModal() {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            const modal = yield this.modalController.create({
                component: _components_nurse_note_nurse_note_component__WEBPACK_IMPORTED_MODULE_3__["NurseNoteComponent"],
                cssClass: 'my-nurse-dar-notes-modal-css',
                backdropDismiss: false
            });
            modal.onDidDismiss().then((result) => {
                if (result !== null) {
                    console.log('result.data: ', result.data);
                    if (result.data.data.title) {
                        const darNote = new _models_dar_notes__WEBPACK_IMPORTED_MODULE_14__["DarNotes"]();
                        darNote.focus = result.data.data.title;
                        darNote.data = result.data.data.data;
                        darNote.action = result.data.data.action;
                        darNote.response = result.data.data.response;
                        console.log('darNote: ', darNote);
                    }
                }
            });
            return yield modal.present();
        });
    }
    keyboardShowHandler(e) {
        const kH = e.keyboardHeight;
        console.log('show: ', e.keyboardHeight);
        const bodyMove = document.querySelector('ion-app'), bodyMoveStyle = bodyMove.style;
        // push screen to 100% and push bottom half of keyboard height
        bodyMoveStyle.bottom = (kH / 2) + 'px';
        bodyMoveStyle.height = '100%';
        bodyMoveStyle.top = 'initial';
        /*
        if (this.offsetY < kH + 40) {
          bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';
          // bodyMoveStyle.top = 'initial';
        }
        */
        console.log(bodyMoveStyle.bottom);
    }
    keyboardHideHandler() {
        console.log('gone');
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
    removeIonAppStyles() {
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
};
