 // tslint:disable-next-line: max-line-length\r\n    this.pSurgicalHistorySave = this.permissionValidatorService.hasPastMedEdit();\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pFamilyHistorySave = this.permissionValidatorService.hasPastMedEdit();\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pSocialHistorySave = this.permissionValidatorService.hasPastMedEdit();\r\n    this.pMenstrualSave = this.permissionValidatorService.hasPastMedEdit();\r\n    this.pPregnantSave = this.permissionValidatorService.hasPastMedEdit();\r\n    this.pImmunizationSave = this.permissionValidatorService.hasPastMedEdit();\r\n\r\n    this.tabs = [\r\n      'present-illness',\r\n      'past-history',\r\n      'surgical-history',\r\n      'family-history',\r\n      'social-history',\r\n      'gynecological-history',\r\n      'immunization-record'\r\n    ];\r\n\r\n    \r\n\r\n    if (this.selectedPatient.spin0.person_his.gender == 'm') {\r\n      this.pGynecologicalObstetricHistoryView = false;\r\n    } else {\r\n      this.pGynecologicalObstetricHistoryView = this.pMenstrualView || this.pPregnantView;\r\n    }\r\n    if (this.pMenstrualView) {\r\n      this.gynecologicalTab = 'menstrual-history';\r\n    } else if (this.pPregnantView) {\r\n      this.gynecologicalTab = 'pregnant-history';\r\n    }\r\n    if (!this.pGynecologicalObstetricHistoryView) {\r\n      const i = this.tabs.indexOf('gynecological-history');\r\n      if (i >= 0) { this.tabs.splice(i, 1); }\r\n    }\r\n\r\n    \r\n    console.log(this.tabs);\r\n    if (this.tabs.length > 0) {\r\n      this.selectedTab = this.tabs[0];\r\n    }\r\n  }\r\n\r\n  getPresentIllness(): Promise<PresentIllness> {\r\n    return new Promise((resolve, reject) => {\r\n      this.historyOfPresentIllness = '';\r\n      this.loadingService.present();\r\n      this.pastMedicalHistoryApiService.getPresentIllness(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n        this.loadingService.dismiss().then(_ => {\r\n          let presentIllness = new PresentIllness(result);\r\n          if (result.id) {\r\n            presentIllness = result;\r\n            this.haspresentIllness = true;\r\n            this.historyOfPresentIllness = presentIllness.history ? presentIllness.history : '';\r\n          } else {\r\n            this.haspresentIllness = false;\r\n            this.historyOfPresentIllness = '';\r\n          }\r\n          this.presentIllnessModified.modified_by = presentIllness.modified_name;\r\n          this.presentIllnessModified.modified_dt = presentIllness.updated_at;\r\n          console.log(this.historyOfPresentIllness);\r\n          resolve(presentIllness);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  fetchDefaultOptions(): Promise<any> {\r\n    return new Promise((resolve, reject) => {\r\n\r\n      if (this.adultImmunizationCodes.length > 0) {\r\n        console.log('Past med options already loaded...');\r\n        resolve([]);\r\n      } else {\r\n        this.pastMedicalHistoryApiService.getDefaultOptions().subscribe((result: any) => {\r\n          this.config.m_patient_pastmedicalhistory['default-options'].pastmedical.disease = result.pastmedical.disease;\r\n          this.config.m_patient_familyhistory['default-options'].familyhistory.disease = result.familyhistory.disease;\r\n          this.config.m_patient_immunization['default-options'].immunization.childimm = result.immunization.childimm;\r\n          this.config.m_patient_immunization['default-options'].immunization.adultimm = result.immunization.adultimm;\r\n          this.config.m_patient_immunization['default-options'].immunization.elderlyimm = result.immunization.elderlyimm;\r\n\r\n          this.pastIllnesses = result.pastmedical.disease;\r\n\r\n          this.familyIllnesses = result.familyhistory.disease;\r\n\r\n          this.childImmunizationCodes = result.immunization.childimm;\r\n\r\n          this.adultImmunizationCodes = result.immunization.adultimm;\r\n\r\n          this.elderlyImmunizationCodes = result.immunization.elderlyimm;\r\n\r\n          this.pastIllnessDefaultIndex = 0;\r\n          this.familyIllnessDefaultIndex = 0;\r\n          this.childImmunizationIndex = 0;\r\n          this.adultImmunizationIndex = 0;\r\n          this.elderlyImmunizationIndex = 0;\r\n\r\n        }, (err) => {\r\n          console.log('err ', err);\r\n          this.loadingService.dismiss().then(_ => {\r\n            const message = this.stringUtilsService.parseErrorMessage(err);\r\n            this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n            reject(err);\r\n          });\r\n        });\r\n      }\r\n    });\r\n  }\r\n\r\n\r\n  getPastMedicalHistories(): Promise<PastMedicalHistory[]> {\r\n    return new Promise((resolve, reject) => {\r\n      this.medicalHistories = [];\r\n      this.loadingService.present();\r\n      this.pastMedicalHistoryApiService.getPastMedicalHistories(this.selectedEncounterNo).subscribe((result: any) => {\r\n        this.loadingService.dismiss().then(_ => {\r\n          this.medicalHistories = result;\r\n          if (result.length > 0) {\r\n            this.medicalHistories = this.medicalHistories.filter((item: PastMedicalHistory) => {\r\n              return item.is_deleted == 0;\r\n            });\r\n            this.hasPastMedicalHistory = true;\r\n            this.pastMedicalHistoryModified.modified_by = result[result.length - 1].modified_name;\r\n            this.pastMedicalHistoryModified.modified_dt = result[result.length - 1].updated_at;\r\n          } else {\r\n            this.hasPastMedicalHistory = false;\r\n            this.pastMedicalHistoryModified.modified_by = null;\r\n            this.pastMedicalHistoryModified.modified_dt = null;\r\n          }\r\n\r\n          this.pastIllnessDefaultIndex = 0;\r\n          resolve(result);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  getSurgicalHistories(): Promise<SurgicalHistory[]> {\r\n    return new Promise((resolve, reject) => {\r\n      this.surgicalHistories = [];\r\n      this.loadingService.present();\r\n      this.pastMedicalHistoryApiService.getSurgicalHistories(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n        this.surgicalHistories = result;\r\n        this.loadingService.dismiss().then(_ => {\r\n          if (result.length > 0) {\r\n            this.surgicalHistories = this.surgicalHistories.filter((item: SurgicalHistory) => {\r\n              return item.is_deleted == 0;\r\n            });\r\n            this.hasSurgicalHistory = true;\r\n            this.surgicalHistoryModified.modified_by = result[result.length - 1].modified_name;\r\n            this.surgicalHistoryModified.modified_dt = result[result.length - 1].updated_at;\r\n          } else {\r\n            this.surgicalHistoryModified.modified_by = '';\r\n            this.surgicalHistoryModified.modified_dt = '';\r\n            this.hasSurgicalHistory = false;\r\n          }\r\n          resolve(result);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  getFamilyMedicalHistories(): Promise<FamilyMedicalHistory[]> {\r\n    return new Promise((resolve, reject) => {\r\n      this.familyMedicalHistories = [];\r\n      this.loadingService.present();\r\n      this.pastMedicalHistoryApiService.getFamilyMedicalHistories(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n        this.loadingService.dismiss().then(_ => {\r\n          this.familyMedicalHistories = result;\r\n          if (result.length > 0) {\r\n            this.familyMedicalHistories = this.familyMedicalHistories.filter((item: FamilyMedicalHistory) => {\r\n              return item.is_deleted == 0;\r\n            });\r\n            this.hasFamilyHistory = true;\r\n            this.familyHistoryModified.modified_by = result[result.length - 1].modified_name;\r\n            this.familyHistoryModified.modified_dt = result[result.length - 1].updated_at;\r\n          } else {\r\n            this.familyHistoryModified.modified_by = '';\r\n            this.familyHistoryModified.modified_dt = '';\r\n            this.hasFamilyHistory = false;\r\n          }\r\n\r\n          this.familyIllnessDefaultIndex = 0;\r\n          resolve(result);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  getSocialMedicalHistory(): Promise<SocialMedicalHistory> {\r\n    return new Promise((resolve, reject) => {\r\n      this.isSmoker = 'N';\r\n      this.isDrinker = 'N';\r\n      this.isDrugUser = 'N';\r\n      this.yearsOfSmoking = 0;\r\n      this.stickPerDay = 0;\r\n      this.stickPerYear = 0;\r\n      this.noOfBottles = 0;\r\n      this.loadingService.present();\r\n      this.pastMedicalHistoryApiService.getSocialMedicalHistory(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n        this.loadingService.dismiss().then(_ => {\r\n          if (result.length > 0) {\r\n            this.socialHistory = result[0];\r\n          } else {\r\n            this.socialHistory = result;\r\n          }\r\n          if (this.socialHistory) {\r\n            this.hasSocialHistory = false;\r\n            this.socialHistoryModified.modified_by = this.socialHistory.modified_name;\r\n            this.socialHistoryModified.modified_dt = this.socialHistory.updated_at;\r\n            if (this.socialHistory.is_smoke) {\r\n              this.isSmoker = this.socialHistory.is_smoke.toLocaleUpperCase();\r\n\r\n              if ('Y' == this.isSmoker) {\r\n                this.yearsOfSmoking = this.socialHistory.years_smoking;\r\n                this.stickPerDay = this.socialHistory.stick_per_day;\r\n                this.stickPerYear = this.computeStringPerYear(this.yearsOfSmoking, this.stickPerDay);\r\n              }\r\n            }\r\n\r\n            if (this.socialHistory.is_alcohol) {\r\n              this.isDrinker = this.socialHistory.is_alcohol.toLocaleUpperCase();\r\n\r\n              if ('Y' == this.isDrinker) {\r\n                this.noOfBottles = this.socialHistory.no_bottles;\r\n              }\r\n            }\r\n\r\n            if (this.socialHistory.is_drug) {\r\n              this.isDrugUser = this.socialHistory.is_drug.toLocaleUpperCase();\r\n\r\n              if ('Y' == this.isDrugUser) {\r\n\r\n              }\r\n            }\r\n          }\r\n\r\n          if ( result.length == 0 ) {\r\n            this.hasSocialHistory = true;\r\n        }\r\n          resolve(result);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  getGynecologicalHistory(): Promise<any> {\r\n    return new Promise((resolve, reject) => {\r\n      this.isMenopause = 'N';\r\n      this.isMenstrualApplicable = 'N';\r\n      this.isPregnantlApplicable = 'N';\r\n      this.ageOfFirstMenstrual = 0;\r\n      this.dateOfLastMenstrual = '';\r\n      this.durationOfMenstrualPeriod = 0;\r\n      this.intervalOfMenstrual = 0;\r\n      this.noOfPadsPerDay = 0;\r\n      this.ageOfFirstSexualIntercourse = 0;\r\n      this.birthControlMethodUsed = '';\r\n      this.ageOfMenopause = 0;\r\n      this.menstrualRemarks = '';\r\n      this.noOfPregnancy = 0;\r\n      this.noOfDelivery = 0;\r\n      this.typeOfDeliver = '';\r\n      this.noOfFullTermPregnancy = 0;\r\n      this.noOfPrematurePregnancy = 0;\r\n      this.noOfAbortion = 0;\r\n      this.noOfLivingChildren = 0;\r\n      this.inducedHypertension = '';\r\n      this.accessToFamilyPlanning = '';\r\n      this.pregnancyRemarks = '';\r\n\r\n      this.loadingService.present();\r\n      this.pastMedicalHistoryApiService.getGynecologicalObstetricHistory(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n        this.loadingService.dismiss().then(_ => {\r\n          if (result.length > 0) {\r\n            if (result[0].data) {\r\n              if (result[0].data.menstrualHistory) {\r\n                this.menstrualHistory = result[0].data.menstrualHistory;\r\n              }\r\n              if (result[0].data.pregnantHistory) {\r\n                this.pregnantHistory = result[0].data.pregnantHistory;\r\n              }\r\n            }\r\n          } else {\r\n            if (result.data) {\r\n              if (result.data.menstrualHistory) {\r\n                this.menstrualHistory = result.data.menstrualHistory;\r\n              }\r\n              if ( result.data.menstrualHistory == null ) {\r\n                  this.hasMenstrualHistory = false;\r\n                  this.menstrualHistoryModified.modified_by = '';\r\n                  this.menstrualHistoryModified.modified_dt = '';\r\n              } else {\r\n                  this.hasMenstrualHistory = true;\r\n                  this.menstrualHistoryModified.modified_by = this.menstrualHistory.modified_name;\r\n                  this.menstrualHistoryModified.modified_dt = this.menstrualHistory.updated_at;\r\n              }\r\n\r\n              if ( result.data.pregnantHistory == null ) {\r\n                this.pregnantHistoryModified.modified_by = '';\r\n                this.pregnantHistoryModified.modified_dt = '';\r\n                this.hasPregnantHistory = false;\r\n              } else {\r\n                this.pregnantHistory = result.data.pregnantHistory;\r\n                this.hasPregnantHistory = true;\r\n                this.pregnantHistoryModified.modified_by = this.pregnantHistory.modified_name;\r\n                this.pregnantHistoryModified.modified_dt = this.pregnantHistory.updated_at;\r\n              }\r\n            }\r\n          }\r\n          console.log(this.menstrualHistory);\r\n          if (this.menstrualHistory) {\r\n            if (this.menstrualHistory.is_applicable_menstrual) {\r\n              this.isMenstrualApplicable = this.menstrualHistory.is_applicable_menstrual;\r\n              this.ageOfFirstMenstrual = this.menstrualHistory.age_first_menstrual;\r\n              this.dateOfLastMenstrual = this.menstrualHistory.last_period_menstrual;\r\n              this.durationOfMenstrualPeriod = this.menstrualHistory.no_days_menstrual_period;\r\n              this.intervalOfMenstrual = this.menstrualHistory.interval_menstrual_period;\r\n              this.noOfPadsPerDay = this.menstrualHistory.no_pads;\r\n              this.ageOfFirstSexualIntercourse = this.menstrualHistory.age_sex_intercourse;\r\n              this.birthControlMethodUsed = this.menstrualHistory.birth_control_used;\r\n              this.isMenopause = this.menstrualHistory.is_menopause;\r\n              this.ageOfMenopause = this.menstrualHistory.age_menopause;\r\n              this.menstrualRemarks = this.menstrualHistory.remarks;\r\n            }\r\n          }\r\n\r\n          console.log(this.pregnantHistory);\r\n          if (this.pregnantHistory) {\r\n            if (this.pregnantHistory.is_applicable_pregnant) {\r\n              this.isPregnantlApplicable = this.pregnantHistory.is_applicable_pregnant;\r\n              this.noOfPregnancy = this.pregnantHistory.date_gravidity;\r\n              this.noOfDelivery = this.pregnantHistory.date_parity;\r\n              this.typeOfDeliver = this.pregnantHistory.type_delivery;\r\n              this.noOfFullTermPregnancy = this.pregnantHistory.no_full_term_preg;\r\n              this.noOfPrematurePregnancy = this.pregnantHistory.no_premature;\r\n              this.noOfAbortion = this.pregnantHistory.no_abortion;\r\n              this.noOfLivingChildren = this.pregnantHistory.no_living_children;\r\n              this.inducedHypertension = this.pregnantHistory.induced_hyper;\r\n              this.accessToFamilyPlanning = this.pregnantHistory.family_planning;\r\n              this.pregnancyRemarks = this.pregnantHistory.remarks;\r\n            }\r\n          }\r\n          resolve(result);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  getImmunizationRecord(): Promise<ImmunizationRecord> {\r\n    return new Promise((resolve, reject) => {\r\n\r\n      this.loadingService.present();\r\n      this.pastMedicalHistoryApiService.getImmunizationRecord(this.selectedEncounterNo).subscribe((result: any) => {\r\n        console.log(result);\r\n        this.loadingService.dismiss().then(_ => {\r\n          this.childImmunizationIndex = 0;\r\n          this.adultImmunizationIndex = 0;\r\n          this.elderlyImmunizationIndex = 0;\r\n          this.otherImmunization = '';\r\n          this.immunizationRemarks = '';\r\n          \r\n          let immunizationRecord: ImmunizationRecord;\r\n          if (result.length > 0) {\r\n            immunizationRecord = result;\r\n          } else {\r\n            immunizationRecord = result;\r\n          }\r\n          if (immunizationRecord) {\r\n            this.hasImmunization = true;\r\n            this.immunizationRecordModified.modified_by = immunizationRecord.modified_name;\r\n            this.immunizationRecordModified.modified_dt = immunizationRecord.updated_at;\r\n            if (immunizationRecord.code) {\r\n              this.alertServ.openAlertModal('fail', 'Failed', immunizationRecord.message, 'Ok');\r\n            } else if (immunizationRecord.id) {\r\n              const cIndex = this.childImmunizationCodes.findIndex(item => {\r\n                const cId = immunizationRecord.child_id ? immunizationRecord.child_id : 0;\r\n                return item.id == cId;\r\n              });\r\n              this.childImmunizationIndex = cIndex;\r\n\r\n              const aIndex = this.adultImmunizationCodes.findIndex(item => {\r\n                const aId = immunizationRecord.young_id ? immunizationRecord.young_id : 0;\r\n                return item.id == aId;\r\n              });\r\n              this.adultImmunizationIndex = aIndex;\r\n\r\n              const eIndex = this.elderlyImmunizationCodes.findIndex(item => {\r\n                const eId = immunizationRecord.elder_id ? immunizationRecord.elder_id : 0;\r\n                return item.id == eId;\r\n              });\r\n              this.elderlyImmunizationIndex = eIndex;\r\n\r\n              this.otherImmunization = immunizationRecord.other_code;\r\n              this.immunizationRemarks = immunizationRecord.remarks;\r\n            }\r\n          }\r\n\r\n          if ( result.length == 0 ) {\r\n              this.hasImmunization = false;\r\n              this.immunizationRecordModified.modified_by = null;\r\n              this.immunizationRecordModified.modified_dt = null;\r\n          }\r\n\r\n\r\n\r\n          resolve(immunizationRecord);\r\n        });\r\n      }, (err) => {\r\n        console.log('err ', err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n          reject(err);\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  ngOnInit() {\r\n    this.initUserInterfaceByRole();\r\n\r\n    this.menuController.swipeGesture(true, 'mainMenu');\r\n    this.menuController.swipeGesture(false, 'accountMenu');\r\n\r\n    this.onUpdateTab();\r\n  }\r\n\r\n  ionViewWillEnter() {\r\n    console.log('ionViewWillEnter');\r\n    const tempSelectedEncounterNo = this.patientService.getSelectedEncounterNo();\r\n    if (this.selectedEncounterNo !== tempSelectedEncounterNo) {\r\n      console.log('selected patient changed');\r\n      this.selectedEncounterNo = tempSelectedEncounterNo;\r\n      // init load flag\r\n      this.isPresentIllnessLoaded = false;\r\n      this.isPastMedicalHistoryLoaded = false;\r\n      this.isSurgicalHistoryLoaded = false;\r\n      this.isFamilyHistoryLoaded = false;\r\n      this.isSocialHistoryLoaded = false;\r\n      this.isGynecologicalHistoryLoaded = false;\r\n      this.isImmunizationRecordLoaded = false;\r\n\r\n      this.initUserInterfaceByRole();\r\n\r\n      // if tab doest not change after initUserInterfaceByRoleAndSelectedPatient()\r\n      if (this.currentTab == this.selectedTab) {\r\n        this.onUpdateTab();\r\n      }\r\n    }\r\n\r\n    window.addEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.addEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n  }\r\n\r\n  ionViewWillLeave() {\r\n    console.log('ionViewWillLeave');\r\n    window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n    this.removeIonAppStyles();\r\n    this.currentTab = this.selectedTab;\r\n  }\r\n\r\n  focusSegment(segmentId) {\r\n    document.getElementById(segmentId).scrollIntoView({\r\n      behavior: 'smooth',\r\n      block: 'start',\r\n      inline: 'start'\r\n    });\r\n  }\r\n\r\n  async onSegmentChange(event: any) {\r\n    console.log(event);\r\n    console.log(this.selectedTab);\r\n    const segment = this.tabs.indexOf(this.selectedTab);\r\n    this.focusSegment(event.srcElement.children[segment].id);\r\n    await this.slider.slideTo(segment);\r\n  }\r\n\r\n  async onSlideChange(event: any) {\r\n    console.log(event);\r\n    console.log(this.selectedTab);\r\n    await this.slider.getActiveIndex().then(index => {\r\n      this.selectedTab = this.tabs[index];\r\n      this.onUpdateTab();\r\n    });\r\n  }\r\n\r\n  onUpdateTab() {\r\n    if (this.selectedTab == 'present-illness') {\r\n      this.pageTitle = 'History of Present Illness';\r\n      this.menuController.swipeGesture(true, 'mainMenu');\r\n      this.menuController.swipeGesture(false, 'accountMenu');\r\n      if (!this.isPresentIllnessLoaded) {\r\n        this.getPresentIllness().then(_ => {\r\n          this.isPresentIllnessLoaded = true;\r\n        }).catch(_ => {\r\n          // do nothing\r\n        });\r\n      }\r\n    } else if (this.selectedTab == 'past-history') {\r\n      this.pageTitle = 'Past Medical History';\r\n      this.menuController.swipeGesture(false, 'mainMenu');\r\n      this.menuController.swipeGesture(false, 'accountMenu');\r\n      if (!this.isPastMedicalHistoryLoaded) {\r\n        this.getPastMedicalHistories().then(_ => {\r\n          this.isPastMedicalHistoryLoaded = true;\r\n        }).catch(_ => {\r\n          // do nothing\r\n        });\r\n      }\r\n    } else if (this.selectedTab == 'surgical-history') {\r\n      this.pageTitle = 'Surgical History';\r\n      this.menuController.swipeGesture(false, 'mainMenu');\r\n      this.menuController.swipeGesture(false, 'accountMenu');\r\n      if (!this.isSurgicalHistoryLoaded) {\r\n        this.getSurgicalHistories().then(_ => {\r\n          this.isSurgicalHistoryLoaded = true;\r\n        }).catch(_ => {\r\n          // do nothing\r\n        });\r\n      }\r\n    } else if (this.selectedTab == 'family-history') {\r\n      this.pageTitle = 'Family History';\r\n      this.menuController.swipeGesture(false, 'mainMenu');\r\n      this.menuController.swipeGesture(false, 'accountMenu');\r\n      if (!this.isFamilyHistoryLoaded) {\r\n        this.getFamilyMedicalHistories().then(_ => {\r\n          this.isFamilyHistoryLoaded = true;\r\n        }).catch(_ => {\r\n          // do nothing\r\n        });\r\n      }\r\n    } else if (this.selectedTab == 'social-history') {\r\n      this.pageTitle = 'Social History';\r\n      this.menuController.swipeGesture(false, 'mainMenu');\r\n      this.menuController.swipeGesture(false, 'accountMenu');\r\n      if (!this.isSocialHistoryLoaded) {\r\n        this.getSocialMedicalHistory().then(_ => {\r\n          this.isSocialHistoryLoaded = true;\r\n        }).catch(_ => {\r\n          // do nothing\r\n        });\r\n      }\r\n    } else if (this.selectedTab == 'gynecological-history') {\r\n      this.pageTitle = 'Gynecological/ Obstetric History';\r\n      this.menuController.swipeGesture(false, 'mainMenu');\r\n      this.menuController.swipeGesture(false, 'accountMenu');\r\n      if (!this.isGynecologicalHistoryLoaded) {\r\n        this.getGynecologicalHistory().then(_ => {\r\n          this.isGynecologicalHistoryLoaded = true;\r\n        }).catch(_ => {\r\n          // do nothing\r\n        });\r\n      }\r\n    }  else if (this.selectedTab == 'immunization-record') {\r\n      this.pageTitle = 'Immunization Record';\r\n      this.menuController.swipeGesture(false, 'mainMenu');\r\n      this.menuController.swipeGesture(true, 'accountMenu');\r\n      if (!this.isImmunizationRecordLoaded) {\r\n        this.getImmunizationRecord().then(_ => {\r\n          this.isImmunizationRecordLoaded = true;\r\n        }).catch(_ => {\r\n          // do nothing\r\n        });\r\n      }\r\n    }\r\n  }\r\n\r\n  onSavePresentDisease(presentDisease: any) {\r\n    this.openPromptModal(this.SAVE_PRESENT_DISEASE, {presentDisease}, 'Present Illness', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onSavePresentDiseaseFunction(presentDisease: any) {\r\n    console.log(presentDisease.value);\r\n\r\n    const data = {\r\n      id: this.selectedEncounterNo,\r\n      encounter_no: this.selectedEncounterNo,\r\n      history: presentDisease.value.name,\r\n      spin: this.selectedPatient.spin0.pid,\r\n      modified_by: this.authentication.user.person.pid\r\n    };\r\n    console.log(data);\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.savePresentIllness(data).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n          this.presentIllnessModified.modified_by = result.data.modified_by;\r\n          this.presentIllnessModified.modified_dt = result.data.updated_at;\r\n          this.haspresentIllness = true;\r\n        } else {\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onDiseaseChange(event: any) {\r\n    console.log(event);\r\n  }\r\n\r\n  onAddDisease(disease: any) {\r\n    this.openPromptModal(this.ADD_PAST_MEDICAL_HISTORY, {disease}, 'Past Medical History', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onAddDiseaseFunction(disease: any) {\r\n    console.log(disease);\r\n    const specificDescription = this.pastMed_DeseacDescription;\r\n    const i = this.pastIllnessDefaultIndex;\r\n    const d = this.pastIllnesses[i];\r\n\r\n    const newDisease = {\r\n      encounter_no: this.selectedEncounterNo,\r\n      specific_disease_description: specificDescription,\r\n      disease_id: d.id,\r\n      phil_disease: {\r\n        mdisease_description: d.mdisease_description\r\n      },\r\n      modified_by: this.authentication.user.person.pid\r\n    };\r\n\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.addPastMedicalHistory(newDisease).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then( _ => {\r\n        if (result && 200 == result.code) {\r\n          const med = new PastMedicalHistory(newDisease);\r\n          med.id = result.data.data.id;\r\n          this.medicalHistories.push(med);\r\n          this.pastIllnessDefaultIndex = 0;\r\n          this.pastMedicalHistoryModified.modified_by = result.data.modified_by;\r\n          this.pastMedicalHistoryModified.modified_dt = result.data.updated_at;\r\n          this.hasPastMedicalHistory = true;\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onRemoveDisease(disease: any) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.REMOVE_PAST_MEDICAL_HISTORY, {disease}, 'Past Medical History', 'Are you sure you want to delete this item?', 'Delete');\r\n  }\r\n\r\n  onRemoveDiseaseFunction(disease: any) {\r\n    console.log(disease);\r\n    this.loadingService.present();\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pastMedicalHistoryApiService.deletePastMedicalHistory(this.selectedEncounterNo, disease.id).subscribe((result: any) => {\r\n      console.log(result);\r\n\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          console.log('onRemoveDisease success');\r\n          this.medicalHistories.splice(this.medicalHistories.indexOf(disease), 1);\r\n\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('onRemoveDisease failed');\r\n\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onAddSurgery(surgery: any) {\r\n    this.openPromptModal(this.ADD_SURGICAL_HISTORY, {surgery}, 'Surgical History', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onAddSurgeryFunction(surgery: any) {\r\n    console.log(surgery.value);\r\n\r\n    const dateObject = new Date(surgery.value.date);\r\n    const date_of_operation = moment(dateObject).format('YYYY-MM-DD');\r\n    console.log('dateObject: ', date_of_operation);\r\n\r\n    const newSurgery = {\r\n      encounter_no: this.selectedEncounterNo,\r\n      description: surgery.value.description,\r\n      date_of_operation,\r\n      remarks: surgery.value.remarks,\r\n      modified_by: this.authentication.user.person.pid\r\n    };\r\n\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.addSurgicalHistory(newSurgery).subscribe((result: any) => {\r\n      console.log(result);\r\n\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          console.log(newSurgery);\r\n          const surg = new SurgicalHistory(newSurgery);\r\n          surg.id = result.data.data.id;\r\n          this.surgicalHistories.push(surg);\r\n          surgery.reset();\r\n          this.surgicalHistoryModified.modified_by = result.data.modified_by;\r\n          this.surgicalHistoryModified.modified_dt = result.data.updated_at;\r\n          this.hasSurgicalHistory = false;\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onRemoveSurgery(surgery: any) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.REMOVE_SURGICAL_HISTORY, {surgery}, 'Surgical History', 'Are you sure you want to delete this item?', 'Delete');\r\n  }\r\n\r\n  onRemoveSurgeryFunction(surgery: any) {\r\n    console.log(surgery);\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.deleteSurgicalHistory(this.selectedEncounterNo, surgery.id).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          this.surgicalHistories.splice(this.surgicalHistories.indexOf(surgery), 1);\r\n\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else{\r\n          console.log('failed');\r\n\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onFamilyDiseaseChange(event: any) {\r\n    console.log(event);\r\n  }\r\n\r\n  onAddFamilyDisease(disease: any) {\r\n    this.openPromptModal(this.ADD_FAMILY_MEDICAL_HISTORY, {disease}, 'Family Medical History', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onAddFamilyDiseaseFunction(disease: any) {\r\n    console.log(disease);\r\n\r\n    const specificDescription = this.familyIllnessDefaultDescription;\r\n    const i = this.familyIllnessDefaultIndex;\r\n    const d = this.familyIllnesses[i];\r\n\r\n    const newDisease = {\r\n      encounter_no: this.selectedEncounterNo,\r\n      specific_disease_description: specificDescription,\r\n      disease_id: d.id,\r\n      phil_disease: {\r\n        mdisease_description: d.mdisease_description\r\n      },\r\n      modified_by: this.authentication.user.person.pid\r\n    };\r\n\r\n    console.log(newDisease);\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.addFamilyHistory(newDisease).subscribe((result: any) => {\r\n      console.log(result);\r\n\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          const fam = new FamilyMedicalHistory(newDisease);\r\n          fam.id = result.data.data.id;\r\n          this.familyMedicalHistories.push(fam);\r\n          this.familyIllnessDefaultIndex = 0;\r\n          this.familyHistoryModified.modified_by = result.data.modified_by;\r\n          this.familyHistoryModified.modified_dt = result.data.updated_at;\r\n          this.hasFamilyHistory = true;\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n\r\n  }\r\n\r\n  onRemoveFamilyDisease(disease: any) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.REMOVE_FAMILY_MEDICAL_HISTORY, {disease}, 'Family Medical History', 'Are you sure you want to delete this item?', 'Delete');\r\n  }\r\n\r\n  onRemoveFamilyDiseaseFunction(disease: any) {\r\n    console.log(disease);\r\n\r\n    this.loadingService.present();\r\n    // tslint:disable-next-line: max-line-length\r\n    this.pastMedicalHistoryApiService.deleteFamilyHistory(this.selectedEncounterNo, disease.id).subscribe((result: any) => {\r\n      console.log(result);\r\n\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          this.familyMedicalHistories.splice(this.familyMedicalHistories.indexOf(disease), 1);\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onIsSmokerChange(event: any) {\r\n\r\n  }\r\n\r\n  onIsDrinkerChange(event: any) {\r\n\r\n  }\r\n\r\n  onIsDrugUserChange(event: any) {\r\n\r\n  }\r\n\r\n  onYearsOfSmokingChange(event: any) {\r\n    this.stickPerYear = this.computeStringPerYear(this.yearsOfSmoking, this.stickPerDay);\r\n  }\r\n\r\n  onStickPerDay(event: any) {\r\n    this.stickPerYear = this.computeStringPerYear(this.yearsOfSmoking, this.stickPerDay);\r\n  }\r\n\r\n  computeStringPerYear(yearOfSmoking: number, stickPerDay: number): number {\r\n    return (stickPerDay / 20) * yearOfSmoking;\r\n  }\r\n\r\n  onUpdateSocialHistory(socialHistory: any) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.SAVE_SOCIAL_MEDICAL_HISTORY, {socialHistory}, 'Social Medical History', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onUpdateSocialHistoryFunction(socialHistory: any) {\r\n    console.log(socialHistory.value);\r\n    const s = socialHistory.value;\r\n\r\n    const updateSocialHistory = {\r\n      id: this.socialHistory.id,\r\n      encounter_no: this.selectedEncounterNo,\r\n      spin: this.selectedPatient.spin0.pid,\r\n      is_smoke: s.is_smoker,\r\n      years_smoking: 'Y' == s.is_smoker ? s.years_of_smoking : 0,\r\n      stick_per_day: 'Y' == s.is_smoker ? s.stick_per_day : 0,\r\n      stick_per_year:  'Y' == s.is_smoker ? s.stick_per_year : 0,\r\n      is_alcohol: s.is_drinker,\r\n      no_bottles: 'Y' == s.is_drinker ? s.no_of_bottles : 0 ,\r\n      is_drug:  s.is_drug_user ,\r\n      remarks: s.remarks,\r\n      modified_by: this.authentication.user.person.pid\r\n    };\r\n\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.saveSocialHistory(updateSocialHistory).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          this.socialHistoryModified.modified_by = result.data.modified_by;\r\n          this.socialHistoryModified.modified_dt = result.data.updated_at;\r\n          this.hasSocialHistory = false;\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onGynecologicalTabChange(event: any) {\r\n\r\n  }\r\n\r\n  onUpdateMenstrualHistory(menstrualHistory: any) {\r\n    this.openPromptModal(this.SAVE_MENSTRUAL_HISTORY, {menstrualHistory}, 'Menstrual History', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onUpdateMenstrualHistoryFunction(menstrualHistory: any) {\r\n    console.log(menstrualHistory.value);\r\n    const m = menstrualHistory.value;\r\n\r\n    const updateMenstrualHistory = {\r\n      id: m.id,\r\n      encounter_no: this.selectedEncounterNo,\r\n      spin: this.selectedPatient.spin0.pid,\r\n      is_applicable_menstrual: m.is_applicable_menstrual,\r\n      age_first_menstrual: m.age_first_menstrual,\r\n      last_period_menstrual: m.last_period_menstrual,\r\n      no_days_menstrual_period: m.no_days_menstrual_period,\r\n      interval_menstrual_period: m.interval_menstrual_period,\r\n      no_pads: m.no_pads,\r\n      age_sex_intercourse: m.age_sex_intercourse,\r\n      birth_control_used: m.birth_control_used ? m.birth_control_used : '',\r\n      is_menopause: m.is_menopause,\r\n      age_menopause: m.age_menopause ? m.age_menopause : 0,\r\n      remarks: m.remarks ? m.remarks : '',\r\n      modified_by: this.authentication.user.person.pid\r\n    };\r\n\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.saveMenstrualHistory(updateMenstrualHistory).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          this.menstrualHistoryModified.modified_by = result.data.modified_by;\r\n          this.menstrualHistoryModified.modified_dt = result.data.updated_at;\r\n          this.hasMenstrualHistory = true;\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onUpdatePregnantHistory(pregnantHistory: any) {\r\n    this.openPromptModal(this.SAVE_PREGNANCY_HISTORY, {pregnantHistory}, 'Pregnancy History', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onUpdatePregnantHistoryFunction(pregnantHistory: any) {\r\n    console.log(pregnantHistory.value);\r\n    const p = pregnantHistory.value;\r\n\r\n    const updatePregnantHistory = {\r\n      id: p.id,\r\n      encounter_no: this.selectedEncounterNo,\r\n      spin: this.selectedPatient.spin0.pid,\r\n      is_applicable_pregnant: p.is_applicable_pregnant,\r\n      date_gravidity: p.date_gravidity,\r\n      date_parity: p.date_parity,\r\n      type_delivery: p.type_delivery ? p.type_delivery : '',\r\n      no_full_term_preg: p.no_full_term_preg,\r\n      no_premature: p.no_premature,\r\n      no_abortion: p.no_abortion,\r\n      no_living_children: p.no_living_children,\r\n      induced_hyper: p.induced_hyper ? p.induced_hyper : '',\r\n      family_planning: p.family_planning ? p.family_planning : '',\r\n      remarks: p.remarks ? p.remarks : '',\r\n      modified_by: this.authentication.user.person.pid\r\n    };\r\n\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.savePregnantHistory(updatePregnantHistory).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          this.pregnantHistoryModified.modified_by = result.data.modified_by;\r\n          this.pregnantHistoryModified.modified_dt = result.data.updated_at;\r\n          this.hasPregnantHistory = true;\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  onUpdateImmunizationRecord(immunization: any) {\r\n    this.openPromptModal(this.SAVE_IMMUNIZATION_RECORD, {immunization}, 'Immunization Record', 'Would you like to save your changes?');\r\n  }\r\n\r\n  onUpdateImmunizationRecordFunction(immunization: any) {\r\n    console.log(immunization.value);\r\n\r\n    const i = immunization.value;\r\n    const childImmCode = this.childImmunizationCodes[i.child_immunization_index];\r\n    const adultImmCode = this.adultImmunizationCodes[i.adult_immunization_index];\r\n    const ederlyImmCode = this.elderlyImmunizationCodes[i.adult_immunization_index];\r\n\r\n    const updateImmunizationRecord = {\r\n      id: i.id,\r\n      encounter_no: this.selectedEncounterNo,\r\n      spin: this.selectedPatient.spin0.pid,\r\n      child_id: childImmCode.id,\r\n      young_id: adultImmCode.id,\r\n      preg_id: 0,\r\n      elder_id: ederlyImmCode.id,\r\n      other_code: i.other_immunization ? i.other_immunization : '',\r\n      remarks: i.remarks ? i.remarks : '',\r\n      modified_by: this.authentication.user.person.pid,\r\n    };\r\n    console.log('updateImmunizationRecord ', updateImmunizationRecord);\r\n    this.loadingService.present();\r\n    this.pastMedicalHistoryApiService.saveImmunizationRecord(updateImmunizationRecord).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 == result.code) {\r\n          this.immunizationRecordModified.modified_by = result.data.modified_by;\r\n          this.immunizationRecordModified.modified_dt = result.data.updated_at;\r\n          this.hasImmunization = true;\r\n          this.alertServ.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          console.log('failed');\r\n          this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  doRefreshPastMedicalHistory(event: any) {\r\n    if (this.selectedTab == 'present-illness') {\r\n      this.getPresentIllness().then(_ => {\r\n        this.isPresentIllnessLoaded = true;\r\n        event.target.complete();\r\n      }).catch(_ => {\r\n        event.target.complete();\r\n      });\r\n    } else if (this.selectedTab == 'past-history') {\r\n      this.getPastMedicalHistories().then(_ => {\r\n        this.isPastMedicalHistoryLoaded = true;\r\n        event.target.complete();\r\n      }).catch(err => {\r\n        event.target.complete();\r\n      });\r\n    } else if (this.selectedTab == 'surgical-history') {\r\n      this.getSurgicalHistories().then(_ => {\r\n        this.isSurgicalHistoryLoaded = true;\r\n        event.target.complete();\r\n      }).catch(err => {\r\n        event.target.complete();\r\n      });\r\n    } else if (this.selectedTab == 'family-history') {\r\n      this.getFamilyMedicalHistories().then(_ => {\r\n        this.isFamilyHistoryLoaded = true;\r\n        event.target.complete();\r\n      }).catch(_ => {\r\n        event.target.complete();\r\n      });\r\n    } else if (this.selectedTab == 'social-history') {\r\n      this.getSocialMedicalHistory().then(_ => {\r\n        this.isSocialHistoryLoaded = true;\r\n        event.target.complete();\r\n      }).catch(_ => {\r\n        event.target.complete();\r\n      });\r\n    } else if (this.selectedTab == 'gynecological-history') {\r\n      this.getGynecologicalHistory().then(_ => {\r\n        this.isGynecologicalHistoryLoaded = true;\r\n        event.target.complete();\r\n      }).catch(_ => {\r\n        event.target.complete();\r\n      });\r\n    }  else if (this.selectedTab == 'immunization-record') {\r\n      this.getImmunizationRecord().then(_ => {\r\n        this.isImmunizationRecordLoaded = true;\r\n        event.target.complete();\r\n      }).catch(err => {\r\n        event.target.complete();\r\n      });\r\n    }\r\n  }\r\n\r\n  async openPromptModal(option: number, data: any, title: string, message: string,  ok: string = 'Save', cancel: string = 'Cancel') {\r\n    this.alertServ.openPromptModal(title, message, ok,(result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (result.data && result.data.button == 'Save') {\r\n          if (option == this.SAVE_PRESENT_DISEASE) {\r\n            this.onSavePresentDiseaseFunction(data.presentDisease);\r\n          } else if (option == this.ADD_PAST_MEDICAL_HISTORY) {\r\n            this.onAddDiseaseFunction(data.disease);\r\n          } else if (option == this.ADD_SURGICAL_HISTORY) {\r\n            this.onAddSurgeryFunction(data.surgery);\r\n          } else if (option == this.ADD_FAMILY_MEDICAL_HISTORY) {\r\n            this.onAddFamilyDiseaseFunction(data.disease);\r\n          } else if (option == this.SAVE_SOCIAL_MEDICAL_HISTORY) {\r\n            this.onUpdateSocialHistoryFunction(data.socialHistory);\r\n          } else if (option == this.SAVE_MENSTRUAL_HISTORY) {\r\n            this.onUpdateMenstrualHistoryFunction(data.menstrualHistory);\r\n          } else if (option == this.SAVE_PREGNANCY_HISTORY) {\r\n            this.onUpdatePregnantHistoryFunction(data.pregnantHistory);\r\n          } else if (option == this.SAVE_IMMUNIZATION_RECORD) {\r\n            this.onUpdateImmunizationRecordFunction(data.immunization);\r\n          }\r\n        } else if (result.data && result.data.button == 'Delete') {\r\n          if (option == this.REMOVE_PAST_MEDICAL_HISTORY) {\r\n            this.onRemoveDiseaseFunction(data.disease);\r\n          } else if (option == this.REMOVE_SURGICAL_HISTORY) {\r\n            this.onRemoveSurgeryFunction(data.surgery);\r\n          } else if (option == this.REMOVE_FAMILY_MEDICAL_HISTORY) {\r\n            this.onRemoveFamilyDiseaseFunction(data.disease);\r\n          }\r\n        }\r\n      }\r\n    });\r\n  }\r\n\r\n\r\n  tapCoordinates(e) {\r\n    this.y = e.touches[0].clientY;\r\n    this.h = window.innerHeight;\r\n    this.offsetY = (this.h - this.y);\r\n    console.log('offset: ', this.offsetY);\r\n  }\r\n\r\n  keyboardShowHandler(e) {\r\n    const kH = e.keyboardHeight;\r\n    console.log('show: ', e.keyboardHeight);\r\n    const bodyMove = document.querySelector('ion-app') as HTMLElement, bodyMoveStyle = bodyMove.style;\r\n\r\n    // Adjust full keyboard height\r\n    bodyMoveStyle.bottom = kH + 'px';\r\n    /*\r\n    if (this.offsetY < kH + 40) {\r\n      bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';\r\n      // bodyMoveStyle.top = 'initial';\r\n    }\r\n    */\r\n    console.log(bodyMoveStyle.bottom);\r\n  }\r\n\r\n  keyboardHideHandler() {\r\n    console.log('gone');\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n\r\n  removeIonAppStyles() {\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n}\r\n","import { Injectable } from '@angular/core';\r\nimport { HttpClient, HttpHeaders, HttpParams, HttpEvent, HttpResponse} from '@angular/common/http';\r\nimport { Observable, throwError, from } from 'rxjs';\r\nimport { map, catchError, timeout, tap} from 'rxjs/operators';\r\nimport { Platform } from '@ionic/angular';\r\nimport { HTTP } from '@ionic-native/http/ngx';\r\nimport { PresentIllness } from '../models/present-illness';\r\nimport { PastMedicalHistory } from '../models/past-medical-history';\r\nimport { SurgicalHistory } from '../models/surgical-history';\r\nimport { FamilyMedicalHistory } from '../models/family-medical-history';\r\nimport { SocialMedicalHistory } from '../models/social-medical-history';\r\n\r\n@Injectable({\r\n  providedIn: 'root'\r\n})\r\nexport class PastMedicalHistoryApiService {\r\n  private url = 'http://localhost:3000';\r\n  private token: string;\r\n\r\n  private getPresentIllnessPath = '/api/doctor/patient/pmh/presentillness/data';\r\n  private getPastMedicalHistoryPath = '/api/doctor/patient/pmh/pastmedicalhistory/data';\r\n  private getSurgicalHistoryPath = '/api/doctor/patient/pmh/surgicalhistory/data';\r\n  private getFamilyMedicalHistoryPath = '/api/doctor/patient/pmh/familyhistory/data';\r\n  private getSocialMedicalHistoryPath = '/api/doctor/patient/pmh/socialhistory/data';\r\n  private getGynecologicalHistoryPath = '/api/doctor/patient/pmh/gynecologicalobstetrichistory/data';\r\n  private getImmunizationRecordPath = '/api/doctor/patient/pmh/immunizationrecord/data';\r\n\r\n  private savePresentIllnessPath = '/api/doctor/patient/pmh/presentillness/action';\r\n  private addPastMedicalHistoryPath = '/api/doctor/patient/pmh/pastmedicalhistory/action';\r\n  private addSurgicalHistoryPath = '/api/doctor/patient/pmh/surgicalhistory/action';\r\n  private addFamilyMedicalHistoryPath = '/api/doctor/patient/pmh/familyhistory/action';\r\n  private saveSocialHistoryPath = '/api/doctor/patient/pmh/socialhistory/action';\r\n  private saveMenstrualHistoryPath = '/api/doctor/patient/pmh/menstrualhistory/action';\r\n  private savePregnantHistoryPath = '/api/doctor/patient/pmh/pregnanthistory/action';\r\n  private saveImmunizationRecordPath = '/api/doctor/patient/pmh/immunizationrecord/action';\r\n\r\n  private deletePastMedicalHistoryPath = '/api/doctor/patient/pmh/pastmedicalhistory/delete';\r\n  private deleteSurgicalMedicalHistoryPath = '/api/doctor/patient/pmh/surgicalhistory/delete';\r\n  private deleteFamilyMedicalHistoryPath = '/api/doctor/patient/pmh/familyhistory/delete';\r\n\r\n  private getPastMedOptions = '/api/doctor/patient/pmh/presentillness/options';\r\n\r\n  constructor(private platform: Platform,\r\n              private httpClient: HttpClient,\r\n              private http: HTTP) { }\r\n\r\n  public setUrl(url: string) {\r\n    this.url = url;\r\n  }\r\n\r\n  public setToken(token: string) {\r\n    this.token = token;\r\n  }\r\n\r\n  public getDefaultOptions(): Observable<any>  {\r\n    if (this.platform.is('ios') || this.platform.is('android')) {\r\n      console.log('native');\r\n      const params = {};\r\n      const headers = {\r\n        Accept: 'application/json',\r\n        'Content-Type': 'application/json',\r\n        Authorization: 'Bearer ' + this.token\r\n      };\r\n\r\n      return from(this.http.get(this.url + this.getPastMedOptions, params, headers)).pipe(\r\n        timeout(30000),\r\n        map((response: any) => {\r\n          return JSON.parse(response.data);\r\n        }),\r\n        catchError((err: any, caught: Observable<any>) => {\r\n          if (typeof err.error === 'string') {\r\n            try {\r\n              err.error = JSON.parse(err.error);\r\n            } catch (e) {\r\n              // do nothing\r\n            }\r\n          }\r\n          console.log('Present Illness error:', err, caught);\r\n          return throwError(err);\r\n        })\r\n      );\r\n    } else {\r\n      console.log('browser');\r\n      const getParams = new HttpParams();\r\n      const httpOptions = {\r\n        params: getParams,\r\n        headers: new HttpHeaders({\r\n          'Content-Type':  'application/json',\r\n          Authorization: 'Bearer ' + this.token\r\n        })\r\n      };\r\n\r\n      return this.httpClient.get(this.url + this.getPastMedOptions, httpOptions).pipe(\r\n        timeout(30000),\r\n        map((results: any) => {\r\n                console.log(results);\r\n                return results;\r\n        }),\r\n        catchError((err: any, caught: Observable<any>) => {\r\n                console.log(caught);\r\n                console.error(err);\r\n                return throwError(err);\r\n        })\r\n      );\r\n    }\r\n  }\r\n\r\n  public getPresentIllness(encounter_no: string): Observable<PresentIllness>  {\r\n    if (this.platform.is('ios') || this.platform.is('android')) {\r\n      console.log('native');\r\n      const params = {\r\n        encounter_no\r\n      };\r\n      const headers = {\r\n        Accept: 'application/json',\r\n        'Content-Type': 'application/json',\r\n        Authorization: 'Bearer ' + this.token\r\n      };\r\n\r\n      return from(this.http.get(this.url + this.getPresentIllnessPath, params, headers)).pipe(\r\n        timeout(30000),\r\n        map((response: any) => {\r\n          const presentIllness = JSON.parse(response.data);\r\n          console.log('Present Illness:', presentIllness);\r\n          return presentIllness;\r\n        }),\r\n        catchError((err: any, caught: Observable<any>) => {\r\n          if (typeof err.error === 'string') {\r\n            try {\r\n              err.error = JSON.parse(err.error);\r\n            } catch (e) {\r\n              // do nothing\r\n            }\r\n          }\r\n          console.log('Presen