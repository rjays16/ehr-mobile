/***/ "./src/app/pages/drugs-and-medicines/drugs-and-medicines.page.ts":
/*!***********************************************************************!*\
  !*** ./src/app/pages/drugs-and-medicines/drugs-and-medicines.page.ts ***!
  \***********************************************************************/
/*! exports provided: DrugsAndMedicinesPage */
/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "DrugsAndMedicinesPage", function() { return DrugsAndMedicinesPage; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/fesm2015/core.js");
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/router */ "./node_modules/@angular/router/fesm2015/router.js");
/* harmony import */ var _services_drugs_and_medicines_api_service__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ./../../services/drugs-and-medicines-api.service */ "./src/app/services/drugs-and-medicines-api.service.ts");
/* harmony import */ var _services_drugs_and_medicines_detail_data_service__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ./../../services/drugs-and-medicines-detail-data.service */ "./src/app/services/drugs-and-medicines-detail-data.service.ts");
/* harmony import */ var _services_patient_service__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! ./../../services/patient.service */ "./src/app/services/patient.service.ts");
/* harmony import */ var _services_permission_validator_service__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ./../../services/permission-validator.service */ "./src/app/services/permission-validator.service.ts");
/* harmony import */ var _services_api_service__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! ./../../services/api.service */ "./src/app/services/api.service.ts");
/* harmony import */ var _services_login_service__WEBPACK_IMPORTED_MODULE_8__ = __webpack_require__(/*! ./../../services/login.service */ "./src/app/services/login.service.ts");
/* harmony import */ var _services_loading_service__WEBPACK_IMPORTED_MODULE_9__ = __webpack_require__(/*! ./../../services/loading.service */ "./src/app/services/loading.service.ts");
/* harmony import */ var _services_string_utils_service__WEBPACK_IMPORTED_MODULE_10__ = __webpack_require__(/*! ./../../services/string-utils.service */ "./src/app/services/string-utils.service.ts");
/* harmony import */ var _ionic_angular__WEBPACK_IMPORTED_MODULE_11__ = __webpack_require__(/*! @ionic/angular */ "./node_modules/@ionic/angular/dist/fesm5.js");
/* harmony import */ var src_app_app_services_alert_service_service__WEBPACK_IMPORTED_MODULE_12__ = __webpack_require__(/*! src/app/app/services/alert-service.service */ "./src/app/app/services/alert-service.service.ts");













let DrugsAndMedicinesPage = class DrugsAndMedicinesPage {
    constructor(router, drugsApi, drugsAndMedicinesDetailDataService, patientService, apiService, loginService, stringUtilsService, permissionValidatorService, loadingService, alertServ, menuController) {
        this.router = router;
        this.drugsApi = drugsApi;
        this.drugsAndMedicinesDetailDataService = drugsAndMedicinesDetailDataService;
        this.patientService = patientService;
        this.apiService = apiService;
        this.loginService = loginService;
        this.stringUtilsService = stringUtilsService;
        this.permissionValidatorService = permissionValidatorService;
        this.loadingService = loadingService;
        this.alertServ = alertServ;
        this.menuController = menuController;
        this.pMedicationSave = true;
        this.selectedEncounterNo = this.patientService.getSelectedEncounterNo();
        console.log(this.selectedEncounterNo);
        this.drugsApi.setUrl(this.apiService.getUrl());
        this.drugsApi.setToken(this.loginService.getAuthentication().token);
        this.meidcationConfigAndDefaults = this.loginService.getConfigAndDefaults().medication;
    }
    fetchDefaultOptions() {
        if (this.meidcationConfigAndDefaults['m-patient-medication']['default-options'].routes.length == 0) {
            this.drugsApi.getMedicationDefaultOptions().subscribe((response) => {
                this.loginService.getConfigAndDefaults().medication['m-patient-medication']['default-options'] = response;
                this.meidcationConfigAndDefaults = this.loginService.getConfigAndDefaults().medication;
            }, (err) => {
                console.log(err);
                this.loadingService.dismiss().then(_ => {
                    const message = this.stringUtilsService.parseErrorMessage(err);
                    this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
                });
            });
        }
    }
    initUserInterfaceByRole() {
        // tslint:disable-next-line: max-line-length
        this.pMedicationSave = this.permissionValidatorService.hasDrugsMedsEdit();
    }
    ngOnInit() {
        this.menuController.swipeGesture(true, 'mainMenu');
        this.menuController.swipeGesture(true, 'accountMenu');
        this.initUserInterfaceByRole();
        this.initDrugsAndMedicines();
        this.fetchDefaultOptions();
    }
    ionViewWillEnter() {
        console.log('ionViewWillEnter');
        const tempSelectedEncounterNo = this.patientService.getSelectedEncounterNo();
        if (this.selectedEncounterNo !== tempSelectedEncounterNo) {
            console.log('selected patient changed');
            this.selectedEncounterNo = tempSelectedEncounterNo;
            this.initUserInterfaceByRole();
            this.initDrugsAndMedicines();
        }
        const updatedDetails = this.drugsAndMedicinesDetailDataService.getDetails();
        if (updatedDetails) {
            const selectedItem = this.medications.find(element => {
                return element.refno == updatedDetails.refno && element.item_id == updatedDetails.item_id;
            });
            if (selectedItem) {
                selectedItem.route = updatedDetails.route;
                selectedItem.frequency = updatedDetails.frequency;
                selectedItem.dosage = updatedDetails.dosage;
            }
            this.drugsAndMedicinesDetailDataService.setDetails(null);
        }
    }
    initDrugsAndMedicines() {
        return new Promise((resolve, reject) => {
            this.loadingService.present();
            this.drugsApi.getDrugsAndMedicines(this.selectedEncounterNo).subscribe((response) => {
                this.medications = response;
                this.loadingService.dismiss().then(_ => {
                    resolve(response);
                });
            }, err => {
                console.log(err);
                this.loadingService.dismiss().then(_ => {
                    const message = this.stringUtilsService.parseErrorMessage(err);
                    this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
                    reject(err);
                });
            });
        });
    }
    viewDrugAndMedicine(index) {
        const data = this.medications[index];
        console.log(data);
        this.router.navigate(['/menu/drugs-and-medicines/drugs-and-medicines-view',
            {
                encounter_no: data.encounter_nr,
                refno: data.refno,
                item_id: data.item_id,
                drug_code: data.drug_code,
                generic_name: data.generic_name,
                item_name: data.item_name,
                route: data.route,
                frequency: data.frequency,
                dosage: data.dosage,
                source: data.meds
            }]);
    }
    doRefreshDrugsAndMedicines(event) {
        this.initDrugsAndMedicines().then(_ => {
            event.target.complete();
        }).catch(_ => {
            event.target.complete();
        });
    }
};
DrugsAndMedicinesPage.ctorParameters = () => [
    { type: _angular_router__WEBPACK_IMPORTED_MODULE_2__["Router"] },
    { type: _services_drugs_and_medicines_api_service__WEBPACK_IMPORTED_MODULE_3__["DrugsAndMedicinesApiService"] },
    { type: _services_drugs_and_medicines_detail_data_service__WEBPACK_IMPORTED_MODULE_4__["DrugsAndMedicinesDetailDataService"] },
    { type: _services_patient_service__WEBPACK_IMPORTED_MODULE_5__["PatientService"] },
    { type: _services_api_service__WEBPACK_IMPORTED_MODULE_7__["ApiService"] },
    { type: _services_login_service__WEBPACK_IMPORTED_MODULE_8__["LoginService"] },
    { type: _services_string_utils_service__WEBPACK_IMPORTED_MODULE_10__["StringUtilsService"] },
    { type: _services_permission_validator_service__WEBPACK_IMPORTED_MODULE_6__["PermissionValidatorService"] },
    { type: _services_loading_service__WEBPACK_IMPORTED_MODULE_9__["LoadingService"] },
    { type: src_app_app_services_alert_service_service__WEBPACK_IMPORTED_MODULE_12__["AlertServiceService"] },
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_11__["MenuController"] }
];
DrugsAndMedicinesPage = tslib__WEBPACK_IMPORTED_MODULE_0__["__decorate"]([
    Object(_angular_core__WEBPACK_IMPORTED_MODULE_1__["Component"])({
        selector: 'app-drugs-and-medicines',
        template: tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! raw-loader!./drugs-and-medicines.page.html */ "./node_modules/raw-loader/dist/cjs.js!./src/app/pages/drugs-and-medicines/drugs-and-medicines.page.html")).default,
        styles: [tslib__WEBPACK_IMPORTED_MODULE_0__["__importDefault"](__webpack_require__(/*! ./drugs-and-medicines.page.scss */ "./src/app/pages/drugs-and-medicines/drugs-and-medicines.page.scss")).default]
    }),
    tslib__WEBPACK_IMPORTED_MODULE_0__["__metadata"]("design:paramtypes", [_angular_router__WEBPACK_IMPORTED_MODULE_2__["Router"],
