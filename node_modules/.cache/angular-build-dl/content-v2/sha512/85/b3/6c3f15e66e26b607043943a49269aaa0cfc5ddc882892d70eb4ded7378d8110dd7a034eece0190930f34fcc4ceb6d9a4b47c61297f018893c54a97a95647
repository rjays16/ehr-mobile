this.symptoms);\r\n                console.log('this.selectedSigns: ', this.selectedSigns);\r\n                for (const selected of this.selectedSigns) {\r\n                  for (const symptom of this.symptoms) {\r\n                    console.log(symptom.id, ' === ', selected);\r\n                    // tslint:disable-next-line: max-line-length\r\n                    if ((typeof symptom.id === 'number' && symptom.id === selected) || (typeof symptom.id === 'string' && Number(symptom.id) === selected)) {\r\n                      symptom.checked = true;\r\n                      break;\r\n                    }\r\n\r\n                    if (symptom.checked) {\r\n                      this.psignsAllowEdit = false;\r\n                    }\r\n                  }\r\n                }\r\n              }\r\n            } else {\r\n              this.alertServ.openAlertModal('fail', 'Failed', 'Cannot parse signs and symptoms values!', 'Ok');\r\n            }\r\n            resolve(response);\r\n          } catch (err) {\r\n            reject(err);\r\n          }\r\n        });\r\n      }, err => {\r\n        console.log(err);\r\n        this.loadingService.dismiss().then(_ => {\r\n          reject(err);\r\n          const message = this.stringUtilsService.parseErrorMessage(err);\r\n          this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');\r\n        });\r\n      });\r\n    });\r\n  }\r\n\r\n  doRefreshSignsAndSymptoms(event: any) {\r\n    this.getSignsandSymptoms(this.selectedEncounterNo).then(response => {\r\n      event.target.complete();\r\n    }).catch(_ => {\r\n      event.target.complete();\r\n    });\r\n  }\r\n\r\n  painsOnFocus() {\r\n    setTimeout(() => {\r\n      document.documentElement.style.setProperty('--signs-and-symptoms-button-margin-bottom', '50vh');\r\n    }, 500);\r\n  }\r\n\r\n  painsOnBlur() {\r\n    this.validatePains();\r\n\r\n    document.documentElement.style.setProperty('--signs-and-symptoms-button-margin-bottom', '0px');\r\n  }\r\n\r\n  othersOnFocus() {\r\n    setTimeout(() => {\r\n      document.documentElement.style.setProperty('--signs-and-symptoms-button-margin-bottom', '50vh');\r\n    }, 500);\r\n  }\r\n\r\n  othersOnBlur() {\r\n    this.validateOthers();\r\n\r\n    document.documentElement.style.setProperty('--signs-and-symptoms-button-margin-bottom', '0px');\r\n  }\r\n\r\n  public searchPains = (keyword: string): Observable<any> => {\r\n    if (this.platform.is('ios') || this.platform.is('android')) {\r\n      console.log('native');\r\n      const params = {\r\n        q: keyword\r\n      };\r\n      const headers = {\r\n        Accept: 'application/json',\r\n        Authorization: 'Bearer ' + this.loginService.getAuthentication().token\r\n      };\r\n\r\n      return from(this.http.get(this.apiService.getUrl() + this.searchPainsPath, params, headers)).pipe(\r\n        timeout(30000),\r\n        map((response: any) => {\r\n          const painList = JSON.parse(response.data);\r\n          console.log('Pain List: ', painList);\r\n          return painList;\r\n        }),\r\n        catchError((err: any, caught: Observable<any>) => {\r\n          if (typeof err.error === 'string') {\r\n            try {\r\n              err.error = JSON.parse(err.error);\r\n            } catch (e) {\r\n              // do nothing\r\n            }\r\n          }\r\n          console.log('Pain List error: ', err, caught);\r\n          return throwError(err);\r\n        })\r\n      );\r\n\r\n    } else {\r\n      const httpOptions = {\r\n        params: new HttpParams()\r\n          .set('q', keyword),\r\n        headers: new HttpHeaders({\r\n          Accept: 'application/json',\r\n          Authorization: 'Bearer ' + this.loginService.getAuthentication().token\r\n        })\r\n      };\r\n      return this.httpClient.get(this.apiService.getUrl() + this.searchPainsPath , httpOptions).pipe(\r\n        map((response: any) => {\r\n          console.log('Pain List: ', response);\r\n          return respon