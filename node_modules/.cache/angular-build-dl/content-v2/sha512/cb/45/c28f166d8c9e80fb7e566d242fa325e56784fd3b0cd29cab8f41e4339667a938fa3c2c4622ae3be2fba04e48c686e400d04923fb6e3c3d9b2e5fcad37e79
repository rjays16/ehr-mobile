/***/ (function(module, __webpack_exports__, __webpack_require__) {

"use strict";
__webpack_require__.r(__webpack_exports__);
/* harmony export (binding) */ __webpack_require__.d(__webpack_exports__, "DrugsAndMedicinesViewPage", function() { return DrugsAndMedicinesViewPage; });
/* harmony import */ var tslib__WEBPACK_IMPORTED_MODULE_0__ = __webpack_require__(/*! tslib */ "./node_modules/tslib/tslib.es6.js");
/* harmony import */ var _angular_core__WEBPACK_IMPORTED_MODULE_1__ = __webpack_require__(/*! @angular/core */ "./node_modules/@angular/core/fesm2015/core.js");
/* harmony import */ var _angular_router__WEBPACK_IMPORTED_MODULE_2__ = __webpack_require__(/*! @angular/router */ "./node_modules/@angular/router/fesm2015/router.js");
/* harmony import */ var _services_permission_validator_service__WEBPACK_IMPORTED_MODULE_3__ = __webpack_require__(/*! ./../../services/permission-validator.service */ "./src/app/services/permission-validator.service.ts");
/* harmony import */ var _services_login_service__WEBPACK_IMPORTED_MODULE_4__ = __webpack_require__(/*! ./../../services/login.service */ "./src/app/services/login.service.ts");
/* harmony import */ var _ionic_angular__WEBPACK_IMPORTED_MODULE_5__ = __webpack_require__(/*! @ionic/angular */ "./node_modules/@ionic/angular/dist/fesm5.js");
/* harmony import */ var _services_drugs_and_medicines_api_service__WEBPACK_IMPORTED_MODULE_6__ = __webpack_require__(/*! ./../../services/drugs-and-medicines-api.service */ "./src/app/services/drugs-and-medicines-api.service.ts");
/* harmony import */ var _services_drugs_and_medicines_detail_data_service__WEBPACK_IMPORTED_MODULE_7__ = __webpack_require__(/*! ./../../services/drugs-and-medicines-detail-data.service */ "./src/app/services/drugs-and-medicines-detail-data.service.ts");
/* harmony import */ var _services_loading_service__WEBPACK_IMPORTED_MODULE_8__ = __webpack_require__(/*! ./../../services/loading.service */ "./src/app/services/loading.service.ts");
/* harmony import */ var _services_string_utils_service__WEBPACK_IMPORTED_MODULE_9__ = __webpack_require__(/*! ./../../services/string-utils.service */ "./src/app/services/string-utils.service.ts");
/* harmony import */ var src_app_app_services_alert_service_service__WEBPACK_IMPORTED_MODULE_10__ = __webpack_require__(/*! src/app/app/services/alert-service.service */ "./src/app/app/services/alert-service.service.ts");
/* harmony import */ var _angular_forms__WEBPACK_IMPORTED_MODULE_11__ = __webpack_require__(/*! @angular/forms */ "./node_modules/@angular/forms/fesm2015/forms.js");
/* harmony import */ var rxjs_operators__WEBPACK_IMPORTED_MODULE_12__ = __webpack_require__(/*! rxjs/operators */ "./node_modules/rxjs/_esm2015/operators/index.js");













let DrugsAndMedicinesViewPage = class DrugsAndMedicinesViewPage {
    constructor(permissionValidatorService, loginService, actRoute, alertServ, navCtrl, drugsApi, drugsAndMedicinesDetailDataService, loadingService, stringUtilsService, formBuilder) {
        this.permissionValidatorService = permissionValidatorService;
        this.loginService = loginService;
        this.actRoute = actRoute;
        this.alertServ = alertServ;
        this.navCtrl = navCtrl;
        this.drugsApi = drugsApi;
        this.drugsAndMedicinesDetailDataService = drugsAndMedicinesDetailDataService;
        this.loadingService = loadingService;
        this.stringUtilsService = stringUtilsService;
        this.formBuilder = formBuilder;
        this.encounter_no = '';
        this.ref_no = '';
        this.item_id = '';
        this.route = null;
        this.frequency = null;
        this.dosage = null;
        this.drug_code = '';
        this.generic_name = '';
        this.item_name = "";
        this.source = null;
        this.pMedicationSave = true;
        this.SAVE_DRUGS_AND_MEDICINE = 1;
        this.dosageMaxLength = 500;
        this.routeMaxLength = 500;
        this.frequencyMaxLength = 50;
        this.frequencyOptions = [];
        this.dosageOptions = [];
        this.dosageOptionsFinal = [];
        this.routeOptions = [];
        this.encounter_no = this.actRoute.snapshot.paramMap.get('encounter_no');
        this.ref_no = this.actRoute.snapshot.paramMap.get('refno');
        this.item_id = this.actRoute.snapshot.paramMap.get('item_id');
        this.meidcationConfigAndDefaults = this.loginService.getConfigAndDefaults().medication;
        this.drug_code = this.actRoute.snapshot.paramMap.get('drug_code');
        this.generic_name = this.actRoute.snapshot.paramMap.get('generic_name');
        this.item_name = this.actRoute.snapshot.paramMap.get('item_name');
        this.route = this.actRoute.snapshot.paramMap.get('route') == null ? '' : this.actRoute.snapshot.paramMap.get('route');
        this.frequency = this.actRoute.snapshot.paramMap.get('frequency') == null ? '' : this.actRoute.snapshot.paramMap.get('frequency');
        this.dosage = this.actRoute.snapshot.paramMap.get('dosage') == null ? '' : this.actRoute.snapshot.paramMap.get('dosage');
        this.source = this.actRoute.snapshot.paramMap.get('source');
        this.frequencyOptions = this.meidcationConfigAndDefaults['m-patient-medication']['default-options'].frequencies.map((item) => { return { id: item.frequency_code, text: item.frequency_disc }; });
        this.dosageOptions = this.meidcationConfigAndDefaults['m-patient-medication']['default-options'].dosages.map((item) => { return { id: item.strength_code, text: item.strength_disc }; });
        this.routeOptions = this.meidcationConfigAndDefaults['m-patient-medication']['default-options'].routes.map((item) => { return { id: item.route_code, text: item.route_disc }; });
    }
    onFrequencySelected(option) {
        console.log('option', option);
        this.form.get('frequency').setValue(option.text);
    }
    onRouteSelected(option) {
        console.log('option', option);
        this.form.get('route').setValue(option.text);
    }
    onDosageSelected(option) {
        console.log('option', option);
        this.form.get('dosage').setValue(option.text);
    }
    onDosageSearch() {
        console.log('this.dosageOptionsFinal', this.dosageOptionsFinal);
        new Promise((resolve, reject) => {
            this.dosageOptionsFinal = this.dosageOptions.filter((item) => {
                return item.text.toLocaleLowerCase().indexOf(this.form.get('dosage').value.toLocaleLowerCase().trim()) != -1 || this.form.get('dosage').value == '';
            });
        });
    }
    ionViewWillEnter() {
        console.log('ionViewWillEnter');
        window.addEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.addEventListener('native.keyboardhide', this.keyboardHideHandler);
    }
    ionViewWillLeave() {
        console.log('ionViewWillLeave');
        window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);
        this.removeIonAppStyles();
    }
    initUserInterfaceByRole() {
        // tslint:disable-next-line: max-line-length
        this.pMedicationSave = this.permissionValidatorService.hasDrugsMedsEdit();
    }
    ngOnInit() {
        this.form = this.formBuilder.group({
            route: [this.route, _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].compose([
                    _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].required,
                    _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].maxLength(this.routeMaxLength)
                ])],
            frequency: [this.frequency, _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].compose([
                    _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].required,
                    _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].maxLength(this.frequencyMaxLength)
                ])],
            dosage: [this.dosage, _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].compose([
                    _angular_forms__WEBPACK_IMPORTED_MODULE_11__["Validators"].maxLength(this.dosageMaxLength)
                ])],
        });
        this.initUserInterfaceByRole();
        this.form.get('dosage').valueChanges.pipe(Object(rxjs_operators__WEBPACK_IMPORTED_MODULE_12__["debounceTime"])(10)).subscribe((ok) => {
            this.onDosageSearch();
        });
    }
    saveMedicationData() {
        this.openPromptModal(this.SAVE_DRUGS_AND_MEDICINE, {}, 'Drugs and Medicine', 'Would you like to save your changes?');
    }
    saveMedicationDataFunction() {
        this.loadingService.present();
        const data = {
            refno: this.ref_no,
            item_id: this.item_id,
            frequency: this.form.get('frequency').value,
            route: this.form.get('route').value,
            dosage: this.form.get('dosage').value ? this.form.get('dosage').value : '',
            encounter_no: this.encounter_no,
            source: this.source
        };
        this.drugsApi.saveMedicationData(data).subscribe((response) => {
            console.log(response);
            this.loadingService.dismiss().then(_ => {
                if (response && response.code === 200 && response.saved) {
                    if (!response.data.data.item_id) {
                        response.data.data.item_id = response.data.data.item_code;
                    }
                    this.drugsAndMedicinesDetailDataService.setDetails(response.data.data);
                    this.alertServ.openAlertModal('success', 'Success', response.message);
                    this.backToDrugsAndMedicine();
                }
                else {
                    this.alertServ.openAlertModal('fail', 'Failed', response.message, 'Ok');
                }
            });
        }, err => {
            console.log(err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    backToDrugsAndMedicine() {
        this.navCtrl.pop();
    }
    openPromptModal(option, data, title, message, ok = 'Save', cancel = 'Cancel') {
        this.alertServ.openPromptModal(title, message, ok, (result) => {
            if (result !== null) {
                console.log('result.data: ', result.data);
                if (result.data && result.data.button === 'Save') {
                    if (option === this.SAVE_DRUGS_AND_MEDICINE) {
                        this.saveMedicationDataFunction();
                    }
                }
            }
        });
    }
    tapCoordinates(e) {
        this.y = e.touches[0].clientY;
        this.h = window.innerHeight;
        this.offsetY = (this.h - this.y);
        console.log('offset: ', this.offsetY);
    }
    keyboardShowHandler(e) {
        const kH = e.keyboardHeight;
        console.log('show: ', e.keyboardHeight);
        const bodyMove = document.querySelector('ion-app'), bodyMoveStyle = bodyMove.style;
        // Adjust full keyboard height
        bodyMoveStyle.bottom = kH + 'px';
        /*
        if (this.offsetY < kH + 40) {
          bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';
          // bodyMoveStyle.top = 'initial';
        }
        */
        console.log(bodyMoveStyle.bottom);
    }
    keyboardHideHandler() {
        console.log('gone');
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
    removeIonAppStyles() {
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
};
DrugsAndMedicinesViewPage.ctorParameters = () => [
    { type: _services_permission_validator_service__WEBPACK_IMPORTED_MODULE_3__["PermissionValidatorService"] },
    { type: _services_login_service__WEBPACK_IMPORTED_MODULE_4__["LoginService"] },
    { type: _angular_router__WEBPACK_IMPORTED_MODULE_2__["ActivatedRoute"] },
    { type: src_app_app_services_alert_service_service__WEBPACK_IMPORTED_MODULE_10__["AlertServiceService"] },
    { type: _ionic_angular__WEBPACK_IMPORTED_MODULE_5__["NavController"] },
    { type: _services_drugs_and_medicines_api_service__WEBPACK_IMPORTED_MODULE_6__["DrugsAndMedicinesApiService"] },
    { type: _services_drugs_and_medicines_detail_data_service__WEBPACK_IMPORTED_MODULE_7__["DrugsAndMedicinesDetailDataService"] },
    { type: _services_loading_service__WEBPACK_IMPORTED_MODULE_8__["LoadingService"] },
    { type: _services_string_utils_service__WEBPACK_IMPORTED_MODULE_9__["StringUtilsService"] },
    { type: _angular_forms__WEBPACK_IMPORTED_MODULE_11__["FormBuilder"] }
