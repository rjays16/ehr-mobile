is.min_date' ,this.min_date);\r\n\r\n    this.dateValueChange(this.max_date);\r\n  }\r\n\r\n  ionViewWillLeave() {\r\n    console.log('ionViewWillLeave');\r\n    window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n    this.removeIonAppStyles();\r\n  }\r\n\r\n  addAsCustomToggleChange(event: any) {\r\n    console.log(event);\r\n  }\r\n\r\n  toggleSigBuilder(value: boolean) {\r\n    this.sigBuilder = '';\r\n    this.sigBuilderEnabled = !value;\r\n    this.initSigBuilderUi(this.sigBuilderEnabled);\r\n  }\r\n\r\n  initSigBuilderUi(enable: boolean) {\r\n    if (enable) {\r\n      document.documentElement.style.setProperty('--sig-background-color', '#ccc');\r\n      document.documentElement.style.setProperty('--sig-button-color', '#fff');\r\n      document.documentElement.style.setProperty('--sig-button-background-color', '#f04141');\r\n    } else {\r\n      document.documentElement.style.setProperty('--sig-background-color', '#fff');\r\n      document.documentElement.style.setProperty('--sig-button-color', '#fff');\r\n      document.documentElement.style.setProperty('--sig-button-background-color', '#3880ff');\r\n    }\r\n\r\n    this.selectedMethod = '';\r\n    this.selectedDosage = '';\r\n    this.selectedPreparation = '';\r\n    this.selectedFrequency = '';\r\n    this.selectedRoute = '';\r\n    this.selectedTiming = '';\r\n    this.selectedAmount = '';\r\n    this.selectedInterval = '';\r\n  }\r\n\r\n  ngOnInit() {\r\n    this.initDateValues();\r\n    const action = this.activatedRoute.snapshot.paramMap.get('action');\r\n    if (action) {\r\n      if (action === 'edit') {\r\n        const prescriptionList = this.activatedRoute.snapshot.paramMap.get('prescriptionList');\r\n        const index = this.activatedRoute.snapshot.paramMap.get('index');\r\n        if (prescriptionList) {\r\n          this.prescriptionList = JSON.parse(prescriptionList)\r\n          .map(item=>{\r\n            item.order_time = item.order_time_ampm;\r\n            return item;\r\n          })\r\n          ;\r\n          console.log('this.prescriptionList: ', this.prescriptionList);\r\n\r\n          if (index && Number(index) >= 0 && Number(index) < this.prescriptionList.length) {\r\n            console.log('index: ',  Number(index));\r\n\r\n            this.initEditPrescriptionItem(Number(index));\r\n          }\r\n        }\r\n      } else if (action === 'view') {\r\n        // do nothing\r\n      }\r\n    }\r\n  }\r\n\r\n  backToPlanManagement() {\r\n    console.log('back');\r\n\r\n    this.navCtrl.pop();\r\n  }\r\n\r\n  currentDateToggleClick() {\r\n    this.dateChecked = !this.dateChecked;\r\n    if (this.dateChecked) {\r\n      this.dateTime = moment().format();\r\n      this.date = this.dateTime;\r\n      this.time = this.dateTime;\r\n    }\r\n    this.disableDate = this.dateChecked;\r\n    this.disableTime = this.dateChecked;\r\n  }\r\n\r\n  addPrescription(prescription: any) {\r\n    console.log('add prescription: ', prescription);\r\n    // tslint:disable-next-line: max-line-length\r\n    const value = JSON.parse(JSON.stringify(prescription.form.value));\r\n    console.log('prescriptionList: ', this.prescriptionList);\r\n\r\n    let sameItem = null;\r\n    if (value.is_custom === true) {\r\n      sameItem = this.prescriptionList.find(element => {\r\n        return element.custom_item === value.custom_item;\r\n      });\r\n    } else  if (value.is_custom === false) {\r\n      sameItem = this.prescriptionList.find(element => {\r\n        if(element.item === value.item)\r\n          return element.item.id === value.item.id;\r\n\r\n        return false;\r\n      });\r\n    }\r\n\r\n    console.log('sameItem: ', sameItem);\r\n    // if (sameItem) {\r\n    //   const sameSig = value.sig === sameItem.sig;\r\n    //   console.log('sameSig: ', sameSig);\r\n    //   if (sameSig) {\r\n    //     this.openAlertModal('fail', 'Prescription Order', 'Duplicate Item with same Sig! Please check your item.');\r\n    //     return;\r\n    //   }\r\n    // }\r\n\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.ADD_PRESCRIPTION, { prescription: prescription.form.value }, 'Prescription Order', 'Would you like to add this prescription?', 'Add');\r\n  }\r\n\r\n  addPrescriptionFunction(pres: any) {\r\n    const prescription = JSON.parse(JSON.stringify(pres));\r\n    console.error('const prescription', prescription);\r\n\r\n    let dateTimeObject: any;\r\n    if (this.dateChecked) {\r\n      prescription.order_check = true;\r\n      if (!this.dateTime) {\r\n        this.dateTime = moment().format();\r\n      }\r\n      dateTimeObject = new Date(this.dateTime);\r\n    } else {\r\n      // If dateChecked is false, combine date and time rather than using dateTime object\r\n      const dateObject = new Date(this.date);\r\n      const tempDate = moment(dateObject).format('YYYY-MM-DD');\r\n      console.log('dateObject: ', dateObject);\r\n      console.log('tempDate: ', tempDate);\r\n\r\n      let tempTime = moment(this.time).format('HH:mm:ss');\r\n      try {\r\n        const timestamp = Date.parse(tempTime);\r\n        if (isNaN(timestamp) === false) {\r\n          const d = new Date(timestamp);\r\n          tempTime = moment(d).format('HH:mm:ss');\r\n        }\r\n      } catch (err) {\r\n        // do nothing\r\n      }\r\n      console.log('tempTime: ', tempTime);\r\n\r\n      // combine date and time to form new dateTime object\r\n      const tempDateTime = tempDate + 'T' + tempTime+\"+08:00\";\r\n      dateTimeObject = new Date(tempDateTime);\r\n    }\r\n\r\n    console.log('dateTimeObject: ', dateTimeObject);\r\n    const prescriptionDate = moment(dateTimeObject).unix();\r\n    const prescriptionTime = moment(dateTimeObject).format('hh:mm A');\r\n\r\n    console.log('order_date formatted: ', moment.unix(prescriptionDate).format());\r\n    console.log('order_time formatted: ', prescriptionTime);\r\n    prescription.date = prescriptionDate;\r\n    prescription.time = prescriptionTime;\r\n\r\n    prescription.order_time = prescriptionTime;\r\n    prescription.order_date = prescriptionDate;\r\n    // Remove spaces in sig\r\n    console.error('prescription.sig',prescription.sig);\r\n    if(prescription.sig)\r\n      prescription.sig = prescription.sig.replace(/ +/g, ' ').trim();\r\n\r\n    if (prescription.is_custom === true) {\r\n      prescription.is_custom = 1;\r\n    } else  if (prescription.is_custom === false) {\r\n      prescription.is_custom = 0;\r\n    }\r\n    //currentEdittingIndex\r\n    // Delete keys if isEmpty\r\n    Object.keys(prescription).forEach(key => {\r\n      if (typeof prescription[key] === 'string' && prescription[key].length === 0 && key !== 'sig') {\r\n        delete prescription[key];\r\n      }\r\n      // if(key == this.currentEdittingIndex.toString())\r\n      //   delete prescription[key];\r\n    });\r\n\r\n    console.log(prescription);\r\n    this.prescriptionList.push(prescription);\r\n  }\r\n\r\n  saveEdittingPrescriptionItem(prescription: any) {\r\n    console.log('save prescription: ', prescription);\r\n    const value = JSON.parse(JSON.stringify(prescription.form.value));\r\n    console.log('prescriptionList: ', this.prescriptionList);\r\n\r\n    let sameItem = null;\r\n    if (value.is_custom === true) {\r\n      sameItem = this.prescriptionList.find(element => {\r\n        return element.custom_item === value.custom_item;\r\n      });\r\n    } else  if (value.is_custom === false) {\r\n      sameItem = this.prescriptionList.find(element => {\r\n        return element.item.id === value.item.id;\r\n      });\r\n    }\r\n\r\n    console.log('sameItem: ', sameItem);\r\n    // if (sameItem) {\r\n    //   const sameSig = value.sig === sameItem.sig;\r\n    //   console.log('sameSig: ', sameSig);\r\n    //   if (sameSig) {\r\n    //     this.openAlertModal('fail', 'Prescription Order', 'Duplicate Item with same Sig! Please check your item.');\r\n    //     return;\r\n    //   }\r\n    // }\r\n\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.EDIT_PRESCRIPTION, { prescription: prescription.form.value }, 'Prescription Order', 'Would you like to save your changes on this prescription?', 'Save');\r\n  }\r\n\r\n  saveEdittingPrescriptionItemFunction(pres: any) {\r\n    const prescription = JSON.parse(JSON.stringify(pres));\r\n    console.error('const prescription', prescription);\r\n\r\n    let dateTimeObject: any;\r\n    if (this.dateChecked) {\r\n      prescription.order_check = true;\r\n      if (!this.dateTime) {\r\n        this.dateTime = moment().format();\r\n      }\r\n      dateTimeObject = new Date(this.dateTime);\r\n    } else {\r\n      // If dateChecked is false, combine date and time rather than using dateTime object\r\n      const dateObject = new Date(this.date);\r\n      const tempDate = moment(dateObject).format('YYYY-MM-DD');\r\n      console.log('dateObject: ', dateObject);\r\n      console.log('tempDate: ', tempDate);\r\n\r\n      let tempTime = moment(this.time).format('HH:mm:ss');;\r\n      try {\r\n        const timestamp = Date.parse(tempTime);\r\n        if (isNaN(timestamp) === false) {\r\n          const d = new Date(timestamp);\r\n          tempTime = moment(d).format('HH:mm:ss');\r\n        }\r\n      } catch (err) {\r\n        // do nothing\r\n      }\r\n      console.log('tempTime: ', tempTime);\r\n\r\n      // combine date and time to form new dateTime object\r\n      const tempDateTime = tempDate + 'T' + tempTime+\"+08:00\";\r\n      dateTimeObject = new Date(tempDateTime);\r\n    }\r\n\r\n    console.log('dateTimeObject: ', dateTimeObject);\r\n    const prescriptionDate = moment(dateTimeObject).unix();\r\n    const prescriptionTime = moment(dateTimeObject).format('hh:mm A');\r\n\r\n    console.log('order_date formatted: ', moment.unix(prescriptionDate).format());\r\n    console.log('order_time formatted: ', prescriptionTime);\r\n    prescription.date = prescriptionDate;\r\n    prescription.date_obj = dateTimeObject;\r\n    prescription.time = prescriptionTime;\r\n\r\n    prescription.order_time = prescriptionTime;\r\n    prescription.order_date = prescriptionDate;\r\n    // Remove spaces in sig\r\n    if(prescription.sig)\r\n      prescription.sig = prescription.sig.replace(/ +/g, ' ').trim();\r\n\r\n    if (prescription.is_custom === true) {\r\n      prescription.is_custom = 1;\r\n    } else if (prescription.is_custom === false) {\r\n      prescription.is_custom = 0;\r\n    }\r\n\r\n    // Delete keys if isEmpty\r\n    Object.keys(prescription).forEach(key => {\r\n      if (typeof prescription[key] === 'string' && prescription[key].length === 0 && key !== 'sig') {\r\n        delete prescription[key];\r\n      }\r\n    });\r\n\r\n    console.log(prescription);\r\n    this.prescriptionList[this.currentEdittingIndex] = prescription;\r\n\r\n    this.deinitEditPrescriptionItem();\r\n  }\r\n\r\n  cancelEdittingPrescriptionItem(prescription: any) {\r\n    console.log('cancelEdittingPrescriptionItem');\r\n\r\n    this.deinitEditPrescriptionItem();\r\n  }\r\n\r\n  deinitEditPrescriptionItem() {\r\n    this.editting = false;\r\n    this.currentEdittingIndex = -1;\r\n    this.currentEdittingPrescription = null;\r\n\r\n    this.dateToggle = false;\r\n    this.dateChecked = false;\r\n\r\n    this.disableDate = false;\r\n    this.disableTime = false;\r\n\r\n    this.date = null;\r\n    this.time = null;\r\n\r\n    this.isCustomItemToggle = false;\r\n    this.customItem = '';\r\n    this.selectedItem = '';\r\n\r\n    this.selectedDosage = '';\r\n    this.selectedAmount = '';\r\n    this.selectedInterval = '';\r\n    this.selectedFrequency = '';\r\n    this.selectedPreparation = '';\r\n    this.selectedMethod = '';\r\n    this.selectedRoute = '';\r\n    this.selectedTiming = '';\r\n\r\n    this.orderQuantity = 0;\r\n    this.orderRemarks = '';\r\n\r\n    this.sigBuilder = '';\r\n\r\n    this.toggleSigBuilder(true);\r\n  }\r\n\r\n  editPrescriptionItem(index: number) {\r\n    this.initEditPrescriptionItem(index);\r\n  }\r\n\r\n  convertTo24Hour(time:any = \"\") {\r\n    time = time.toLowerCase();\r\n\r\n    if(!(time.indexOf('am') != -1 || time.indexOf('pm') != -1)){\r\n      if(time.length == 8)\r\n        time = moment().format('YYYY-MM-DD')+\"T\"+time+\"+08:00\";\r\n      if(time.length == 5)\r\n        time = moment().format('YYYY-MM-DD')+\"T\"+time+\":00+08:00\";\r\n    }\r\n\r\n    var hours = parseInt(time.substr(0, 2));\r\n    if(time.indexOf('am') != -1) {\r\n        time = time.replace('12', '00');\r\n    }\r\n    if(time.indexOf('pm')  != -1 && hours != 12) {\r\n        time = time.replace(time.substr(0, 2), (hours + 12));\r\n    }\r\n\r\n    var finalTime = time.replace(/(am|pm)/, '').trim()+':00';\r\n\r\n    if(finalTime.length == 8)\r\n      finalTime = moment().format('YYYY-MM-DD')+\"T\"+finalTime+\"+08:00\";\r\n\r\n    return finalTime;\r\n  }\r\n\r\n  initEditPrescriptionItem(index: number) {\r\n    this.editting = true;\r\n    this.currentEdittingIndex = index;\r\n    this.currentEdittingPrescription = this.prescriptionList[index];\r\n\r\n    console.log('this.currentEdittingPrescription: ',  this.currentEdittingPrescription);\r\n    if (this.currentEdittingPrescription.dosage || this.currentEdittingPrescription.duration_amount ||\r\n      this.currentEdittingPrescription.duration_interval || this.currentEdittingPrescription.frequency ||\r\n      this.currentEdittingPrescription.timing || this.currentEdittingPrescription.method ||\r\n      this.currentEdittingPrescription.preparation || this.currentEdittingPrescription.route ) {\r\n      this.toggleSigBuilder(false);\r\n    }\r\n\r\n    this.dateToggle = this.currentEdittingPrescription.order_check;\r\n    this.dateChecked = this.currentEdittingPrescription.order_check;\r\n\r\n    this.disableDate = this.dateChecked;\r\n    this.disableTime = this.dateChecked;\r\n\r\n    this.date = moment.unix(this.currentEdittingPrescription.order_date).format();\r\n    console.error('this.currentEdittingPrescription.order_time', this.currentEdittingPrescription.order_time);\r\n    this.time = this.convertTo24Hour(this.currentEdittingPrescription.order_time);\r\n    console.error('this.time', this.time);\r\n\r\n    this.isCustomItemToggle = !(this.currentEdittingPrescription.is_custom == 0 || this.currentEdittingPrescription.is_custom == false) ;\r\n    this.customItem = this.currentEdittingPrescription.custom_item;\r\n    this.selectedItem = this.currentEdittingPrescription.item;\r\n\r\n    this.selectedDosage = this.currentEdittingPrescription.dosage;\r\n    this.selectedAmount = this.currentEdittingPrescription.duration_amount;\r\n    this.selectedInterval = this.currentEdittingPrescription.duration_interval;\r\n    this.selectedFrequency = this.currentEdittingPrescription.frequency;\r\n    this.selectedPreparation = this.currentEdittingPrescription.preparation;\r\n    this.selectedMethod = this.currentEdittingPrescription.method;\r\n    this.selectedRoute = this.currentEdittingPrescription.route;\r\n    this.selectedTiming = this.currentEdittingPrescription.timing;\r\n\r\n    this.orderQuantity = this.currentEdittingPrescription.quantity;\r\n    this.orderRemarks = this.currentEdittingPrescription.remarks;\r\n\r\n    this.sigBuilder = this.currentEdittingPrescription.sig;\r\n  }\r\n\r\n  deletePrescriptionItem(index: number) {\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.DELETE_PRESCRIPTION, { index }, 'Prescription Order', 'Would you like to delete this prescription?', 'Delete');\r\n  }\r\n\r\n  deletePrescriptionItemFunction(index: number) {\r\n    this.prescriptionList.splice(index, 1);\r\n  }\r\n\r\n  savePrescriptions(prescriptionList: any) {\r\n    console.log('this.prescriptionList: ', this.prescriptionList);\r\n    const prescriptions = [];\r\n    // Recreate items to be sent\r\n    this.prescriptionList.forEach(element => {\r\n      const prescription = JSON.parse(JSON.stringify(element));\r\n      console.error(\"prescription\",prescription);\r\n      console.error(\"currentEdittingPrescription\",this.currentEdittingPrescription);\r\n      // Delete unnecessary keys\r\n      if (prescription.is_custom === 0) {\r\n        delete prescription.is_custom;\r\n        if(prescription.item)\r\n          prescription.item = prescription.item.id;\r\n        else\r\n          prescription.item = prescription.item;\r\n      }\r\n\r\n      if (prescription.order_check === true || prescription.order_check === false) {\r\n        prescription.check = prescription.order_check;\r\n      }\r\n      if (!prescription.date) {\r\n        prescription.date = prescription.order_date;\r\n      }\r\n      if (!prescription.time && this.currentEdittingPrescription) {\r\n        prescription.time = this.currentEdittingPrescription.order_time;\r\n      }\r\n\r\n      if(!prescription.time)\r\n        prescription.time = prescription.order_time;\r\n      \r\n      console.error(\"this.convertTo24Hour(prescription.time)\",this.convertTo24Hour(prescription.time));\r\n      prescription.time = moment(this.convertTo24Hour(prescription.time)).format('HH:mm');\r\n      \r\n      \r\n\r\n      delete prescription.order_check;\r\n      delete prescription.order_date;\r\n      delete prescription.order_time;\r\n\r\n      if (prescription.order_check) {\r\n        delete prescription.order_check;\r\n      }\r\n\r\n      // Delete keys if isEmpty or null\r\n      Object.keys(prescription).forEach(key => {\r\n        if (typeof prescription[key] === 'string' && prescription[key].length === 0) {\r\n          delete prescription[key];\r\n        } else if (prescription[key] === null) {\r\n          delete prescription[key];\r\n        }\r\n      });\r\n\r\n      delete prescription.cash;\r\n      delete prescription.id;\r\n      delete prescription.is_ehr;\r\n      delete prescription.kardexGroup;\r\n      delete prescription.kardexGroupRef;\r\n      delete prescription.order_date_new;\r\n      delete prescription.order_time_ampm;\r\n      delete prescription.refno;\r\n      delete prescription.stat;\r\n      delete prescription.status;\r\n\r\n      prescriptions.push(prescription); \r\n    });\r\n\r\n    console.log('prescriptions: ', prescriptions);\r\n\r\n    // tslint:disable-next-line: max-line-length\r\n    this.openPromptModal(this.SAVE_PRESCRIPTION, { prescriptions }, 'Prescription Order', 'Would you like to save following prescriptions?', 'Save');\r\n  }\r\n\r\n  savePrescriptionsFunction(prescriptions: any) {\r\n    console.log('prescriptions: ', prescriptions);\r\n    this.loadingService.present();\r\n    this.planManagementApiService.savePrescription(this.selectedEncounterNo, prescriptions).subscribe((response) => {\r\n      console.log(response);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (response.code && response.code === 200) {\r\n          const batch = response.data.batch;\r\n          this.planManagementDataService.setUnfinalizedBatch(batch);\r\n          console.log('batch: ', batch);\r\n          this.openAlertModal('success', 'Success', response.message);\r\n        } else {\r\n          this.openAlertModal('fail', 'Failed', response.message, 'Ok');\r\n        }\r\n      });\r\n    }, err => {\r\n      console.log(err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n\r\n  }\r\n\r\n  searchMedicine(event: {component: IonicSelectableComponent, text: string }) {\r\n    if (event.text.length > 3) {\r\n      clearTimeout(this.timeout);\r\n      this.timeout = setTimeout(() => {\r\n        console.log('Executing search ...');\r\n        event.component.startSearch();\r\n        this.planManagementApiService.searchMedicine(event.text).subscribe((response) => {\r\n          const list = [];\r\n          if (response) {\r\n            Object.keys(response).forEach(key => {\r\n              list.push(response[key]);\r\n            });\r\n          }\r\n          this.medicinelist = list;\r\n          console.log('medicineList: ', this.medicinelist);\r\n          event.component.endSearch();\r\n        }, err => {\r\n          console.log(err);\r\n          event.component.endSearch();\r\n        });\r\n      }, 2000);\r\n    }\r\n  }\r\n\r\n  onMethodChange(event: any) {\r\n    console.log('onMethodChange: ', event);\r\n    const key = event.detail.value;\r\n    const method = this.methodValues.find(item => {\r\n      return key === item.key;\r\n    });\r\n    if (method) {\r\n      this.selectedMethodDisplay = method.value;\r\n    } else {\r\n      this.selectedMethodDisplay = '';\r\n    }\r\n  }\r\n\r\n  onPreparationChange(event: any) {\r\n    console.log('onPreparationChange: ', event);\r\n    const key = event.detail.value;\r\n    const method = this.preparationValues.find(item => {\r\n      return key === item.key;\r\n    });\r\n    if (method) {\r\n      this.selectedPreparationDisplay = method.value;\r\n    } else {\r\n      this.selectedPreparationDisplay = '';\r\n    }\r\n  }\r\n\r\n  onFrequencyChange(event: any) {\r\n    console.log('onFrequencyChange: ', event);\r\n    const key = event.detail.value;\r\n    const method = this.frequencyValues.find(item => {\r\n      return key === item.key;\r\n    });\r\n    if (method) {\r\n      this.selectedFrequencyDisplay = method.value;\r\n    } else {\r\n      this.selectedFrequencyDisplay = '';\r\n    }\r\n  }\r\n\r\n  onRouteChange(event: any) {\r\n    console.log('onRouteChange: ', event);\r\n    const key = event.detail.value;\r\n    const method = this.routeValues.find(item => {\r\n      return key === item.key;\r\n    });\r\n    if (method) {\r\n      this.selectedRouteDisplay = method.value;\r\n    } else {\r\n      this.selectedRouteDisplay = '';\r\n    }\r\n  }\r\n\r\n  onTimingChange(event: any) {\r\n    console.log('onTimingChange: ', event);\r\n    const key = event.detail.value;\r\n    const method = this.timingValues.find(item => {\r\n      return key === item.key;\r\n    });\r\n    if (method) {\r\n      this.selectedTimingDisplay = method.value;\r\n    } else {\r\n      this.selectedTimingDisplay = '';\r\n    }\r\n  }\r\n\r\n  onDurationAmountChange(event: any) {\r\n    console.log('onDurationAmountChange: ', event);\r\n    const key = event.detail.value;\r\n    const method = this.durAmountValues.find(item => {\r\n      return key === item.key;\r\n    });\r\n    if (method) {\r\n      this.selectedAmountDisplay = method.value;\r\n    } else {\r\n      this.selectedAmountDisplay = '';\r\n    }\r\n  }\r\n\r\n  onDurationIntervalChange(event: any) {\r\n    console.log('onDurationIntervalChange: ', event);\r\n    const key = event.detail.value;\r\n    const method = this.durIntervalValues.find(item => {\r\n      return key === item.key;\r\n    });\r\n    if (method) {\r\n      this.selectedIntervalDisplay = method.value;\r\n    } else {\r\n      this.selectedIntervalDisplay = '';\r\n    }\r\n  }\r\n\r\n  openPromptModal(option: number, data: any, title: string, message: string,  ok: string = 'Add', cancel: string = 'No') {\r\n    this.alertServ.openPromptModal(title, message, ok, (result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (result.data && result.data.button === 'Add') {\r\n          if (option === this.ADD_PRESCRIPTION) {\r\n            this.addPrescriptionFunction(data.prescription);\r\n          }\r\n        } else if (result.data && result.data.button === 'Save') {\r\n          if (option === this.SAVE_PRESCRIPTION) {\r\n            this.savePrescriptionsFunction(data.prescriptions);\r\n          } else if (option === this.EDIT_PRESCRIPTION) {\r\n            this.saveEdittingPrescriptionItemFunction(data.prescription);\r\n          }\r\n        } else if (result.data && result.data.button === 'Delete') {\r\n          if (option === this.DELETE_PRESCRIPTION) {\r\n            this.deletePrescriptionItemFunction(data.index);\r\n          }\r\n        }\r\n      }\r\n    });\r\n  }\r\n\r\n  openAlertModal(alert: string, title: string, message: string, ok: string = 'Done') {\r\n    this.alertServ.openAlertModal(alert, title, message, ok, (result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (alert === 'success' && result.data.button === 'Done') {\r\n          this.backToPlanManagement();\r\n        }\r\n      }\r\n    });\r\n  }\r\n\r\n  tapCoordinates(e) {\r\n    this.y = e.touches[0].clientY;\r\n    this.h = window.innerHeight;\r\n    this.offsetY = (this.h - this.y);\r\n    console.log('offset: ', this.offsetY);\r\n  }\r\n\r\n  keyboardShowHandler(e) {\r\n    const kH = e.keyboardHeight;\r\n    console.log('show: ', e.keyboardHeight);\r\n    const bodyMove = document.querySelector('ion-app') as HTMLElement, bodyMoveStyle = bodyMove.style;\r\n\r\n    // Adjust full keyboard height\r\n    bodyMoveStyle.bottom = kH + 'px';\r\n    /*\r\n    if (this.offsetY < kH + 40) {\r\n      bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';\r\n      // bodyMoveStyle.top = 'initial';\r\n    }\r\n    */\r\n    console.log(bodyMoveStyle.bottom);\r\n  }\r\n\r\n  keyboardHideHandler() {\r\n    console.log('gone');\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n\r\n  removeIonAppStyles() {\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n}\r\n"],"sourceRoot":"webpack:///","file":"prescription-orders-prescription-orders-module-es5.js"}