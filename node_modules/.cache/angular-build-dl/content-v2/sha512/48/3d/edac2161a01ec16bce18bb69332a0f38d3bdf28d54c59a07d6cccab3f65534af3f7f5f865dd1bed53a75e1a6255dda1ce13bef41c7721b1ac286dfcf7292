ic_name = '';\r\n  item_name = \"\";\r\n  source = null;\r\n  medication: any;\r\n  pMedicationSave = true;\r\n  meidcationConfigAndDefaults: any;\r\n  private SAVE_DRUGS_AND_MEDICINE = 1;\r\n\r\n  dosageMaxLength = 500;\r\n  routeMaxLength = 500;\r\n  frequencyMaxLength = 50;\r\n\r\n  form : FormGroup;\r\n  // keyboard params\r\n  y: any;\r\n  h: any;\r\n  offsetY: any;\r\n\r\n  frequencyOptions = [];\r\n  dosageOptions = [];\r\n  dosageOptionsFinal = [];\r\n  routeOptions = [];\r\n\r\n  constructor(\r\n    private permissionValidatorService: PermissionValidatorService,\r\n    public loginService: LoginService,\r\n    private actRoute: ActivatedRoute,\r\n    private alertServ: AlertServiceService,\r\n    private navCtrl: NavController,\r\n    private drugsApi: DrugsAndMedicinesApiService,\r\n    private drugsAndMedicinesDetailDataService: DrugsAndMedicinesDetailDataService,\r\n    private loadingService: LoadingService,\r\n    private stringUtilsService: StringUtilsService,\r\n    private formBuilder: FormBuilder\r\n  ) {\r\n    this.encounter_no = this.actRoute.snapshot.paramMap.get('encounter_no');\r\n    this.ref_no = this.actRoute.snapshot.paramMap.get('refno');\r\n    this.item_id = this.actRoute.snapshot.paramMap.get('item_id');\r\n    this.meidcationConfigAndDefaults = this.loginService.getConfigAndDefaults().medication;\r\n\r\n    this.drug_code = this.actRoute.snapshot.paramMap.get('drug_code');\r\n    this.generic_name = this.actRoute.snapshot.paramMap.get('generic_name');\r\n    this.item_name = this.actRoute.snapshot.paramMap.get('item_name');\r\n    this.route = this.actRoute.snapshot.paramMap.get('route') == null ? '':this.actRoute.snapshot.paramMap.get('route');\r\n    this.frequency = this.actRoute.snapshot.paramMap.get('frequency') == null ? '':this.actRoute.snapshot.paramMap.get('frequency');\r\n    this.dosage = this.actRoute.snapshot.paramMap.get('dosage') == null? '': this.actRoute.snapshot.paramMap.get('dosage');\r\n    this.source = this.actRoute.snapshot.paramMap.get('source');\r\n\r\n    this.frequencyOptions = this.meidcationConfigAndDefaults['m-patient-medication']['default-options'].frequencies.map((item)=>{return {id:item.frequency_code, text: item.frequency_disc}});\r\n    this.dosageOptions = this.meidcationConfigAndDefaults['m-patient-medication']['default-options'].dosages.map((item)=>{return {id:item.strength_code, text: item.strength_disc}});\r\n    this.routeOptions = this.meidcationConfigAndDefaults['m-patient-medication']['default-options'].routes.map((item)=>{return {id:item.route_code, text: item.route_disc}});\r\n  }\r\n\r\n\r\n  onFrequencySelected(option){\r\n    console.log('option',option);\r\n    this.form.get('frequency').setValue(option.text);\r\n  }\r\n\r\n  onRouteSelected(option){\r\n    console.log('option',option);\r\n    this.form.get('route').setValue(option.text);\r\n  }\r\n\r\n  onDosageSelected(option){\r\n    console.log('option',option);\r\n    this.form.get('dosage').setValue(option.text);\r\n  }\r\n\r\n\r\n  onDosageSearch(){\r\n    console.log('this.dosageOptionsFinal', this.dosageOptionsFinal);\r\n    new Promise((resolve, reject) =>{\r\n      this.dosageOptionsFinal = this.dosageOptions.filter((item)=>{\r\n        return item.text.toLocaleLowerCase().indexOf(this.form.get('dosage').value.toLocaleLowerCase().trim()) != -1 || this.form.get('dosage').value == '';\r\n      });\r\n    });\r\n  } \r\n\r\n  ionViewWillEnter() {\r\n    console.log('ionViewWillEnter');\r\n    window.addEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.addEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n  }\r\n\r\n  ionViewWillLeave() {\r\n    console.log('ionViewWillLeave');\r\n    window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n    this.removeIonAppStyles();\r\n  }\r\n\r\n  initUserInterfaceByRole() {\r\n    // tslint:disable-n