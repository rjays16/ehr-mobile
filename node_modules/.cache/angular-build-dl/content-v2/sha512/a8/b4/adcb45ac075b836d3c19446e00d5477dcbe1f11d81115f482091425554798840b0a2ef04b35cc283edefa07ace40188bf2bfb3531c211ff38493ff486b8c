        const date = new Date();
        date.setFullYear(new Date().getFullYear() + 1);
        this.rep_start_max_time = this.rep_end_max_time = this.rep_start_max_date = this.rep_end_max_date = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(date.getFullYear() + moment__WEBPACK_IMPORTED_MODULE_10__().format('-MM-DD') + 'T00:00:00+08:00')).format();
        this.printDateRules();
        this.validateRepStart_vs_endDate(this.start_date);
        this.validateRepEnd_vs_startDate(this.end_date);
    }
    printDateRules() {
        console.error('Date Rules: ', {
            start_date: this.start_date,
            start_time: this.start_time,
            end_date: this.end_date,
            end_time: this.end_time,
            rep_start_max_date: this.rep_start_max_date,
            rep_start_min_date: this.rep_start_min_date,
            rep_start_min_time: this.rep_start_min_time,
            rep_start_max_time: this.rep_start_max_time,
            rep_end_min_date: this.rep_end_min_date,
            rep_end_min_time: this.rep_end_min_time,
            rep_end_max_time: this.rep_end_max_time,
            rep_end_max_date: this.rep_end_max_date,
        });
    }
    hasMissingDateTimeInputs() {
        return !(this.start_date) || !(this.start_time) || !(this.end_date) || !(this.end_time);
    }
    validateRepStart_vs_endDate(repstartDateAndTime = "") {
        this.invalidDateTimeRange = this.invalidDateTimeLessThenAdmissionDate = false;
        console.error('validateRepStart_vs_endDate() triggerd');
        this.startDateAndTime = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(repstartDateAndTime));
        console.error('startDateAndTime', this.startDateAndTime.format());
        const resetTime = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(moment__WEBPACK_IMPORTED_MODULE_10__().format('YYYY-MM-DD') + 'T00:00:00+08:00'));
        this.endDateAndTime = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.end_date)).format('YYYY-MM-DD') + "T" + moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.end_time)).format('HH:mm') + ":00++08:00"));
        console.error('endDateAndTime', this.endDateAndTime.format());
        if (this.startDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD')) {
            console.error("startDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD')", this.startDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD'));
            this.rep_start_min_time = this.admission_date.format();
        }
        else {
            console.error("else");
            this.rep_start_min_time = resetTime.format();
        }
    }
    validateRepEnd_vs_startDate(repEndDateAndTime = "") {
        this.invalidDateTimeRange = this.invalidDateTimeLessThenAdmissionDate = false;
        console.error('validateRepEnd_vs_startDate() triggerd');
        this.endDateAndTime = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(repEndDateAndTime));
        console.error('endDateAndTime', this.endDateAndTime.format());
        this.startDateAndTime = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.start_date)).format('YYYY-MM-DD') + "T" + moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.start_time)).format('HH:mm') + ":00+08:00"));
        console.error('startDateAndTime', this.startDateAndTime.format());
        const resetTime = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(moment__WEBPACK_IMPORTED_MODULE_10__().format('YYYY-MM-DD') + 'T00:00:00+08:00'));
        if (this.endDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD')) {
            console.error("endDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD')", this.endDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD'));
            this.rep_end_min_time = this.admission_date.format();
        }
        else {
            console.error("else");
            this.rep_end_min_time = resetTime.format();
        }
    }
    datePickerChanged() {
        console.error("datePickerChanged() triggerd");
        if (this.hasMissingDateTimeInputs()) {
            this.printDateRules();
            return false;
        }
        console.error(' endDateAndTime.unix() < startDateAndTime.unix() ', this.endDateAndTime.unix(), this.startDateAndTime.unix(), this.endDateAndTime.unix() < this.startDateAndTime.unix());
        if (this.endDateAndTime.unix() < this.startDateAndTime.unix()) {
            this.invalidDateTimeRange = true;
        }
        else if (this.endDateAndTime.unix() < this.admission_date.unix() || this.startDateAndTime.unix() < this.admission_date.unix()) {
            this.invalidDateTimeLessThenAdmissionDate = true;
        }
        this.printDateRules();
    }
    repStartDateUpdated(event) {
        console.error('repStartDateUpdated() triggerd');
        const startDate = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(event)).format('YYYY-MM-DD') + "T" + moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.start_time)).format('HH:mm') + ":00+08:00";
        console.error('startDate', startDate);
        this.validateRepStart_vs_endDate(startDate);
    }
    repStartTimeUpdated(event) {
        console.error('repStartTimeUpdated() triggerd');
        const startDate = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.start_date)).format('YYYY-MM-DD') + "T" + moment__WEBPACK_IMPORTED_MODULE_10__(new Date(event)).format('HH:mm') + ":00+08:00";
        console.error('startDate', startDate);
        this.validateRepStart_vs_endDate(startDate);
    }
    repEndDateUpdated(event) {
        console.error('repEndDateUpdated() triggerd');
        const endDate = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(event)).format('YYYY-MM-DD') + "T" + moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.end_time)).format('HH:mm') + ":00+08:00";
        console.error('endDate', endDate);
        this.validateRepEnd_vs_startDate(endDate);
    }
    repEndTimeUpdated(event) {
        console.error('repEndTimeUpdated() triggerd');
        const endDate = moment__WEBPACK_IMPORTED_MODULE_10__(new Date(this.end_date)).format('YYYY-MM-DD') + "T" + moment__WEBPACK_IMPORTED_MODULE_10__(new Date(event)).format('HH:mm') + ":00+08:00";
        console.error('endDate', endDate);
        this.validateRepEnd_vs_startDate(endDate);
    }
    ionViewWillLeave() {
        console.log('ionViewWillLeave');
        window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);
        this.removeIonAppStyles();
    }
    ngOnInit() {
        this.initDateValues();
        const action = this.route.snapshot.paramMap.get('action');
        if (action) {
            if (action === 'edit') {
                const order = this.route.snapshot.paramMap.get('order');
                if (order) {
                    const selectedDoctorOrder = JSON.parse(order);
                    console.log(selectedDoctorOrder);
                    if (selectedDoctorOrder.action) {
                        this.remarks = selectedDoctorOrder.action;
                    }
                    else if (selectedDoctorOrder.remarks) {
                        this.remarks = selectedDoctorOrder.remarks;
                    }
                    const order_checked = selectedDoctorOrder.order_checked;
                    if (typeof order_checked === 'boolean') {
                        this.dateToggle = order_checked;
                    }
                    else if (typeof order_checked === 'number') {
                        this.dateToggle = order_checked === 1;
                    }
                    else if (typeof order_checked === 'string') {
                        this.dateToggle = order_checked === '1';
                    }
                    else {
                        this.dateToggle = false;
                    }
                    this.id = selectedDoctorOrder.id;
                    this.dateChecked = this.dateToggle;
                    this.disableDate = this.dateToggle;
                    this.disableTime = this.dateToggle;
                    this.date = moment__WEBPACK_IMPORTED_MODULE_10__["unix"](selectedDoctorOrder.order_date).format();
                    this.time = moment__WEBPACK_IMPORTED_MODULE_10__().format('YYYY-MM-DD') + " " + selectedDoctorOrder.order_time;
                }
            }
            else if (action === 'view') {
                this.disableElement();
            }
        }
    }
    disableElement() {
        this.disableToggleDate = true;
        this.disableSaveButton = true;
        this.disableDate = true;
        this.disableTime = true;
    }
    backToPlanManagement() {
        console.log('back');
        this.navCtrl.pop();
    }
    currentDateToggleClick() {
        this.dateChecked = !this.dateChecked;
        if (this.dateChecked) {
            this.dateTime = moment__WEBPACK_IMPORTED_MODULE_10__().format();
            this.date = this.dateTime;
            this.time = this.dateTime;
        }
        this.disableDate = this.dateChecked;
        this.disableTime = this.dateChecked;
    }
    saveRepetitiveOrder(event) {
        console.log(event);
        this.openPromptModal(this.SAVE_DOCTOR_ORDER, {}, 'Repetitive Order', 'Would you like to save this order?');
    }
    saveDoctorOrderFunction() {
        const start_order_date = moment__WEBPACK_IMPORTED_MODULE_10__(this.start_date).unix();
        const start_order_time = moment__WEBPACK_IMPORTED_MODULE_10__(this.start_time).format('HH:mm');
        const end_order_date = moment__WEBPACK_IMPORTED_MODULE_10__(this.end_date).unix();
        const end_order_time = moment__WEBPACK_IMPORTED_MODULE_10__(this.end_time).format('HH:mm');
        const courseward = {
            // id: "7a89e6ab-151e-11ea-9e25-005056af800c",
            procedure_session: this.procedure.id,
            procedure_remarks_session: this.remarks,
            requestDateSessionEnd_session: end_order_date,
            requestDateSessionStart_session: start_order_date,
            requestTimeSessionEnd_session: end_order_time,
            requestTimeSessionStart_session: start_order_time,
        };
        this.loadingService.present();
        this.planManagementApiService.saveRepetitive(this.selectedEncounterNo, courseward).subscribe((result) => {
            console.log(result);
            this.loadingService.dismiss().then(_ => {
                if (result && 200 === result.code) {
                    const batch = result.data.batch;
                    this.planManagementDataService.setUnfinalizedBatch(batch);
                    console.log('batch: ', batch);
                    this.openAlertModal('success', 'Success', result.message);
                }
                else {
                    this.openAlertModal('fail', 'Failed', result.message, 'Ok');
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    openPromptModal(option, data, title, message, ok = 'Save', cancel = 'Cancel') {
        this.alertServ.openPromptModal(title, message, ok, (result) => {
            if (result !== null) {
                console.log('result.data: ', result.data);
                if (result.data && result.data.button === 'Save') {
                    if (option === this.SAVE_DOCTOR_ORDER) {
                        this.saveDoctorOrderFunction();
                    }
                }
            }
        });
    }
    openAlertModal(alert, title, message, ok = 'Done') {
        this.alertServ.openAlertModal(alert, title, message, ok, (result) => {
            if (result !== null) {
                console.log('result.data: ', result.data);
                if (alert === 'success' && result.data.button === 'Done') {
                    this.backToPlanManagement();
                }
            }
        });
    }
    tapCoordinates(e) {
        this.y = e.touches[0].clientY;
        this.h = window.innerHeight;
        this.offsetY = (this.h - this.y);
        console.log('offset: ', this.offsetY);
    }
    keyboardShowHandler(e) {
        const kH = e.keyboardHeight;
        console.log('show: ', e.keyboardHeight);
        const bodyMove = document.querySelector('ion-app'), bodyMoveStyle = bodyMove.style;
        // Adjust full keyboard height
        bodyMoveStyle.bottom = kH + 'px';
        /*
        if (this.offsetY < kH + 40) {
          bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';
          // bodyMoveStyle.top = 'initial';
        }
        */
        console.log(bodyMoveStyle.bottom);
    }
    keyboardHideHandler() {
        console.log('gone');
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
    removeIonAppStyles() {
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
