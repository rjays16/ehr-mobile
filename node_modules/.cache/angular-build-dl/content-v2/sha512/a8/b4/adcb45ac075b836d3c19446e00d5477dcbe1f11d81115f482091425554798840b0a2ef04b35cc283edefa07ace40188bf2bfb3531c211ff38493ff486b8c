        this.rep_start_min_time = null;
        this.rep_start_max_time = null;
        this.rep_end_min_date = null;
        this.rep_end_min_time = null;
        this.rep_end_max_time = null;
        this.rep_end_max_date = null;
        this.startDateAndTime = null;
        this.endDateAndTime = null;
