        this.invalidDateTimeLessThenAdmissionDate = false;
        this.admission_date = null;
        this.configAndDefaults = this.loginService.getConfigAndDefaults().repetitive;
        this.procedures = this.configAndDefaults['m-patient-repetitive']['default-options']['repetitive-procedure'].map((item) => {
            return {
                id: item.id,
                procedure: ("(" + item.rvs_code + ") " + item.procedure)
            };
        });
        console.error('this.procedures', this.procedures);
        this.authentication = this.loginService.getAuthentication();
        this.planManagementApiService.setUrl(this.apiService.getUrl());
        this.planManagementApiService.setToken(this.authentication.token);
        this.selectedEncounterNo = this.patientService.getSelectedEncounterNo();
    }
    ionViewWillEnter() {
        console.log('ionViewWillEnter');
        window.addEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.addEventListener('native.keyboardhide', this.keyboardHideHandler);
        this.initDateValues();
    }
    onPredureChange(event) {
        console.log('onPredureChange:', event);
    }
    initDateValues() {
        this.admission_date = moment__WEBPACK_IMPORTED_MODULE_10__["unix"](this.patientService.getSelectedPatientBriefInformation().encounter_date);
        // this.start_date = this.end_date = this.start_time = this.end_time = moment().format();
