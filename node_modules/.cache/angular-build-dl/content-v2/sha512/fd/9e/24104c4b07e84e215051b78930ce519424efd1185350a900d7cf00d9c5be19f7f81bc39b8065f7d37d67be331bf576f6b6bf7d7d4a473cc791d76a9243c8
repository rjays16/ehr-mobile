:00+08:00\"));\r\n    console.error('startDateAndTime', this.startDateAndTime.format());\r\n\r\n    const resetTime = moment(new Date(moment().format('YYYY-MM-DD')+'T00:00:00+08:00'));\r\n\r\n\r\n    if(this.endDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD')){\r\n      console.error(\"endDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD')\", this.endDateAndTime.format('YYYY-MM-DD') == this.admission_date.format('YYYY-MM-DD'));\r\n      this.rep_end_min_time = this.admission_date.format();\r\n    }\r\n    else{\r\n      console.error(\"else\");\r\n      this.rep_end_min_time = resetTime.format();\r\n    }\r\n\r\n    \r\n  }\r\n\r\n  datePickerChanged(){\r\n    console.error(\"datePickerChanged() triggerd\");\r\n    if(this.hasMissingDateTimeInputs()){\r\n      this.printDateRules();\r\n      return false;\r\n    }\r\n\r\n    console.error(' endDateAndTime.unix() < startDateAndTime.unix() ',  this.endDateAndTime.unix() , this.startDateAndTime.unix(), this.endDateAndTime.unix() < this.startDateAndTime.unix() );\r\n    if( this.endDateAndTime.unix() < this.startDateAndTime.unix() ){\r\n      this.invalidDateTimeRange = true;\r\n    }\r\n    else if( this.endDateAndTime.unix() < this.admission_date.unix() ||  this.startDateAndTime.unix() < this.admission_date.unix() ){\r\n      this.invalidDateTimeLessThenAdmissionDate = true;\r\n    }\r\n\r\n    this.printDateRules();\r\n  }\r\n\r\n  repStartDateUpdated(event:any){\r\n    console.error('repStartDateUpdated() triggerd');\r\n    const startDate = moment(new Date(event)).format('YYYY-MM-DD')+\"T\"+moment(new Date(this.start_time)).format('HH:mm')+\":00+08:00\";\r\n    console.error('startDate', startDate);\r\n\r\n    this.validateRepStart_vs_endDate(startDate);\r\n  }\r\n\r\n  repStartTimeUpdated(event:any){\r\n    console.error('repStartTimeUpdated() triggerd');\r\n    const startDate = moment(new Date(this.start_date)).format('YYYY-MM-DD')+\"T\"+moment(new Date(event)).format('HH:mm')+\":00+08:00\";\r\n    console.error('startDate', startDate);\r\n\r\n    this.validateRepStart_vs_endDate(startDate);\r\n  }\r\n\r\n\r\n  repEndDateUpdated(event:any){\r\n    console.error('repEndDateUpdated() triggerd');\r\n    const endDate = moment(new Date(event)).format('YYYY-MM-DD')+\"T\"+moment(new Date(this.end_time)).format('HH:mm')+\":00+08:00\";\r\n    console.error('endDate', endDate);\r\n\r\n    this.validateRepEnd_vs_startDate(endDate);\r\n  }\r\n\r\n  repEndTimeUpdated(event:any){\r\n    console.error('repEndTimeUpdated() triggerd');\r\n    const endDate = moment(new Date(this.end_date)).format('YYYY-MM-DD')+\"T\"+moment(new Date(event)).format('HH:mm')+\":00+08:00\";\r\n    console.error('endDate', endDate);\r\n\r\n    this.validateRepEnd_vs_startDate(endDate);\r\n  }\r\n\r\n\r\n\r\n  ionViewWillLeave() {\r\n    console.log('ionViewWillLeave');\r\n    window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);\r\n    window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);\r\n    this.removeIonAppStyles();\r\n  }\r\n\r\n  ngOnInit() {\r\n    this.initDateValues();\r\n    const action = this.route.snapshot.paramMap.get('action');\r\n    if (action) {\r\n      if (action === 'edit') {\r\n        const order = this.route.snapshot.paramMap.get('order');\r\n        if (order) {\r\n          const selectedDoctorOrder: Courseward = JSON.parse(order);\r\n          console.log(selectedDoctorOrder);\r\n          if (selectedDoctorOrder.action) {\r\n            this.remarks = selectedDoctorOrder.action;\r\n          } else if (selectedDoctorOrder.remarks) {\r\n            this.remarks = selectedDoctorOrder.remarks;\r\n          }\r\n          const order_checked = selectedDoctorOrder.order_checked;\r\n          if (typeof order_checked === 'boolean') {\r\n            this.dateToggle = order_checked;\r\n          } else if (typeof order_checked === 'number') {\r\n            this.dateToggle = order_checked === 1;\r\n          } else if (typeof order_checked === 'string') {\r\n            this.dateToggle = order_checked === '1';\r\n          } else {\r\n            this.dateToggle = false;\r\n          }\r\n          this.id = selectedDoctorOrder.id;\r\n          this.dateChecked = this.dateToggle;\r\n          this.disableDate = this.dateToggle;\r\n          this.disableTime = this.dateToggle;\r\n          this.date = moment.unix(selectedDoctorOrder.order_date).format();\r\n          this.time = moment().format('YYYY-MM-DD')+\" \"+selectedDoctorOrder.order_time;\r\n        }\r\n      } else if (action === 'view') {\r\n        this.disableElement();\r\n      }\r\n    }\r\n  }\r\n\r\n  disableElement() {\r\n    this.disableToggleDate = true;\r\n    this.disableSaveButton = true;\r\n    this.disableDate = true;\r\n    this.disableTime = true;\r\n  }\r\n\r\n  backToPlanManagement() {\r\n    console.log('back');\r\n    this.navCtrl.pop();\r\n  }\r\n\r\n  currentDateToggleClick() {\r\n    this.dateChecked = !this.dateChecked;\r\n    if (this.dateChecked) {\r\n      this.dateTime = moment().format();\r\n      this.date = this.dateTime;\r\n      this.time = this.dateTime;\r\n    }\r\n    this.disableDate = this.dateChecked;\r\n    this.disableTime = this.dateChecked;\r\n  }\r\n\r\n  saveRepetitiveOrder(event: any) {\r\n    console.log(event);\r\n\r\n    this.openPromptModal(this.SAVE_DOCTOR_ORDER, {}, 'Repetitive Order', 'Would you like to save this order?');\r\n  }\r\n\r\n  saveDoctorOrderFunction() {\r\n    const start_order_date = moment(this.start_date).unix();\r\n    const start_order_time = moment(this.start_time).format('HH:mm');\r\n\r\n    const end_order_date = moment(this.end_date).unix();\r\n    const end_order_time = moment(this.end_time).format('HH:mm');\r\n\r\n\r\n\r\n    const courseward = {\r\n      // id: \"7a89e6ab-151e-11ea-9e25-005056af800c\",\r\n      procedure_session: this.procedure.id,\r\n\r\n      procedure_remarks_session: this.remarks,\r\n\r\n      requestDateSessionEnd_session: end_order_date,\r\n\r\n      requestDateSessionStart_session: start_order_date,\r\n\r\n      requestTimeSessionEnd_session: end_order_time,\r\n\r\n      requestTimeSessionStart_session: start_order_time,\r\n    };\r\n\r\n\r\n    this.loadingService.present();\r\n    this.planManagementApiService.saveRepetitive(this.selectedEncounterNo,  courseward).subscribe((result: any) => {\r\n      console.log(result);\r\n      this.loadingService.dismiss().then(_ => {\r\n        if (result && 200 === result.code) {\r\n          const batch = result.data.batch;\r\n          this.planManagementDataService.setUnfinalizedBatch(batch);\r\n          console.log('batch: ', batch);\r\n          this.openAlertModal('success', 'Success', result.message);\r\n        } else {\r\n          this.openAlertModal('fail', 'Failed', result.message, 'Ok');\r\n        }\r\n      });\r\n    }, (err) => {\r\n      console.log('err ', err);\r\n      this.loadingService.dismiss().then(_ => {\r\n        const message = this.stringUtilsService.parseErrorMessage(err);\r\n        this.openAlertModal('fail', 'Failed', message, 'Ok');\r\n      });\r\n    });\r\n  }\r\n\r\n  openPromptModal(option: number, data: any, title: string, message: string,  ok: string = 'Save', cancel: string = 'Cancel') {\r\n    this.alertServ.openPromptModal(title, message, ok, (result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (result.data && result.data.button === 'Save') {\r\n          if (option === this.SAVE_DOCTOR_ORDER) {\r\n            this.saveDoctorOrderFunction();\r\n          }\r\n        }\r\n      }\r\n    });\r\n  }\r\n\r\n  openAlertModal(alert: string, title: string, message: string, ok: string = 'Done') {\r\n    this.alertServ.openAlertModal(alert, title, message, ok, (result) => {\r\n      if (result !== null) {\r\n        console.log('result.data: ', result.data);\r\n        if (alert === 'success' && result.data.button === 'Done') {\r\n          this.backToPlanManagement();\r\n        }\r\n      }\r\n    });\r\n  }\r\n\r\n  tapCoordinates(e) {\r\n    this.y = e.touches[0].clientY;\r\n    this.h = window.innerHeight;\r\n    this.offsetY = (this.h - this.y);\r\n    console.log('offset: ', this.offsetY);\r\n  }\r\n\r\n  keyboardShowHandler(e) {\r\n    const kH = e.keyboardHeight;\r\n    console.log('show: ', e.keyboardHeight);\r\n    const bodyMove = document.querySelector('ion-app') as HTMLElement, bodyMoveStyle = bodyMove.style;\r\n\r\n    // Adjust full keyboard height\r\n    bodyMoveStyle.bottom = kH + 'px';\r\n    /*\r\n    if (this.offsetY < kH + 40) {\r\n      bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';\r\n      // bodyMoveStyle.top = 'initial';\r\n    }\r\n    */\r\n    console.log(bodyMoveStyle.bottom);\r\n  }\r\n\r\n  keyboardHideHandler() {\r\n    console.log('gone');\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n\r\n  removeIonAppStyles() {\r\n    const removeStyles = document.querySelector('ion-app') as HTMLElement;\r\n    removeStyles.removeAttribute('style');\r\n  }\r\n}\r\n"],"sourceRoot":"webpack:///","file":"repetitive-order-repetitive-order-module-es5.js"}