    ngOnInit() {
        this.initUserInterfaceByRole();
        this.menHistform = this.formBuilder.group(this.getMenhisFormRules());
        this.pregHistform = this.formBuilder.group(this.getPreghisFormRules());
        this.socialHistform = this.formBuilder.group(this.getSocialhisFormRules());
        this.updateMenstrualHistoryRules();
        console.error('on ngOnInit');
        this.menuController.swipeGesture(true, 'mainMenu');
        this.menuController.swipeGesture(false, 'accountMenu');
        this.onUpdateTab();
    }
    ionViewWillEnter() {
        console.log('ionViewWillEnter');
        const tempSelectedEncounterNo = this.patientService.getSelectedEncounterNo();
        this.selectedPatient = this.patientService.getSelectedPatientBriefInformation();
        if (this.selectedEncounterNo !== tempSelectedEncounterNo) {
            console.log('selected patient changed');
            this.selectedEncounterNo = tempSelectedEncounterNo;
            // init load flag
            this.isPresentIllnessLoaded = false;
            this.isPastMedicalHistoryLoaded = false;
            this.isSurgicalHistoryLoaded = false;
            this.isFamilyHistoryLoaded = false;
            this.isSocialHistoryLoaded = false;
            this.isGynecologicalHistoryLoaded = false;
            this.isImmunizationRecordLoaded = false;
            this.initUserInterfaceByRole();
            // if tab doest not change after initUserInterfaceByRoleAndSelectedPatient()
            if (this.currentTab == this.selectedTab) {
                this.onUpdateTab();
            }
        }
        window.addEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.addEventListener('native.keyboardhide', this.keyboardHideHandler);
    }
    ionViewWillLeave() {
        console.log('ionViewWillLeave');
        window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);
        this.removeIonAppStyles();
        this.currentTab = this.selectedTab;
    }
    focusSegment(segmentId) {
        document.getElementById(segmentId).scrollIntoView({
            behavior: 'smooth',
            block: 'start',
            inline: 'start'
        });
    }
    onSegmentChange(event) {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            console.log(event);
            console.log(this.selectedTab);
            const segment = this.tabs.indexOf(this.selectedTab);
            this.focusSegment(event.srcElement.children[segment].id);
            yield this.slider.slideTo(segment);
        });
    }
    onSlideChange(event) {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            console.log(event);
            console.log(this.selectedTab);
            yield this.slider.getActiveIndex().then(index => {
                this.selectedTab = this.tabs[index];
                this.onUpdateTab();
            });
        });
    }
    onUpdateTab() {
        if (this.selectedTab == 'present-illness') {
            this.pageTitle = 'History of Present Illness';
            this.menuController.swipeGesture(true, 'mainMenu');
            this.menuController.swipeGesture(false, 'accountMenu');
            if (!this.isPresentIllnessLoaded) {
                this.getPresentIllness().then(_ => {
                    this.isPresentIllnessLoaded = true;
                }).catch(_ => {
                    // do nothing
                });
            }
        }
        else if (this.selectedTab == 'past-history') {
            this.pageTitle = 'Past Medical History';
            this.menuController.swipeGesture(false, 'mainMenu');
            this.menuController.swipeGesture(false, 'accountMenu');
            if (!this.isPastMedicalHistoryLoaded) {
                this.getPastMedicalHistories().then(_ => {
                    this.isPastMedicalHistoryLoaded = true;
                }).catch(_ => {
                    // do nothing
                });
            }
        }
        else if (this.selectedTab == 'surgical-history') {
            this.pageTitle = 'Surgical History';
            this.menuController.swipeGesture(false, 'mainMenu');
            this.menuController.swipeGesture(false, 'accountMenu');
            if (!this.isSurgicalHistoryLoaded) {
                this.getSurgicalHistories().then(_ => {
                    this.isSurgicalHistoryLoaded = true;
                }).catch(_ => {
                    // do nothing
                });
            }
        }
        else if (this.selectedTab == 'family-history') {
            this.pageTitle = 'Family History';
            this.menuController.swipeGesture(false, 'mainMenu');
            this.menuController.swipeGesture(false, 'accountMenu');
            if (!this.isFamilyHistoryLoaded) {
                this.getFamilyMedicalHistories().then(_ => {
                    this.isFamilyHistoryLoaded = true;
                }).catch(_ => {
                    // do nothing
                });
            }
        }
        else if (this.selectedTab == 'social-history') {
            this.pageTitle = 'Social History';
            this.menuController.swipeGesture(false, 'mainMenu');
            this.menuController.swipeGesture(false, 'accountMenu');
            if (!this.isSocialHistoryLoaded) {
                this.getSocialMedicalHistory().then(_ => {
                    this.isSocialHistoryLoaded = true;
                }).catch(_ => {
                    // do nothing
                });
            }
        }
        else if (this.selectedTab == 'gynecological-history') {
            this.pageTitle = 'Gynecological/ Obstetric History';
            this.menuController.swipeGesture(false, 'mainMenu');
            this.menuController.swipeGesture(false, 'accountMenu');
            if (!this.isGynecologicalHistoryLoaded) {
                this.getGynecologicalHistory().then(_ => {
                    this.isGynecologicalHistoryLoaded = true;
                }).catch(_ => {
                    // do nothing
                });
            }
        }
        else if (this.selectedTab == 'immunization-record') {
            this.pageTitle = 'Immunization Record';
            this.menuController.swipeGesture(false, 'mainMenu');
            this.menuController.swipeGesture(true, 'accountMenu');
            if (!this.isImmunizationRecordLoaded) {
                this.getImmunizationRecord().then(_ => {
                    this.isImmunizationRecordLoaded = true;
                }).catch(_ => {
                    // do nothing
                });
            }
        }
    }
    onSavePresentDisease(presentDisease) {
        this.openPromptModal(this.SAVE_PRESENT_DISEASE, { presentDisease }, 'Present Illness', 'Would you like to save your changes?');
    }
    onSavePresentDiseaseFunction(presentDisease) {
        console.log(presentDisease.value);
        const data = {
            id: this.selectedEncounterNo,
            encounter_no: this.selectedEncounterNo,
            history: presentDisease.value.name,
            spin: this.selectedPatient.spin0.pid,
            modified_by: this.authentication.user.person.pid
        };
        console.log(data);
        this.loadingService.present();
        this.pastMedicalHistoryApiService.savePresentIllness(data).subscribe((result) => {
            console.log(result);
            this.loadingService.dismiss().then(_ => {
                if (result && 200 == result.code) {
                    this.alertServ.openAlertModal('success', 'Success', result.message);
                    this.presentIllnessModified.modified_by = result.data.modified_by;
                    this.presentIllnessModified.modified_dt = result.data.updated_at;
                    this.haspresentIllness = true;
                }
                else {
                    this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    onDiseaseChange(event) {
        console.log(event);
    }
    onAddDisease(disease) {
        this.openPromptModal(this.ADD_PAST_MEDICAL_HISTORY, { disease }, 'Past Medical History', 'Would you like to save your changes?');
    }
    onAddDiseaseFunction(disease) {
        console.log(disease);
        const specificDescription = this.pastMed_DeseacDescription;
        const i = this.pastIllnessDefaultIndex;
        const d = this.pastIllnesses[i];
        const newDisease = {
            encounter_no: this.selectedEncounterNo,
            specific_disease_description: specificDescription,
            disease_id: d.id,
            phil_disease: {
                mdisease_description: d.mdisease_description
            },
            modified_by: this.authentication.user.person.pid
        };
        this.loadingService.present();
        this.pastMedicalHistoryApiService.addPastMedicalHistory(newDisease).subscribe((result) => {
            console.log(result);
            this.loadingService.dismiss().then(_ => {
                if (result && 200 == result.code) {
                    const med = new _models_past_medical_history__WEBPACK_IMPORTED_MODULE_10__["PastMedicalHistory"](newDisease);
                    med.id = result.data.data.id;
                    this.medicalHistories.push(med);
                    this.pastIllnessDefaultIndex = 0;
                    this.pastMedicalHistoryModified.modified_by = result.data.modified_by;
                    this.pastMedicalHistoryModified.modified_dt = result.data.updated_at;
                    this.hasPastMedicalHistory = true;
                    this.alertServ.openAlertModal('success', 'Success', result.message);
                }
                else {
                    console.log('failed');
                    this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    onRemoveDisease(disease) {
        // tslint:disable-next-line: max-line-length
        this.openPromptModal(this.REMOVE_PAST_MEDICAL_HISTORY, { disease }, 'Past Medical History', 'Are you sure you want to delete this item?', 'Delete');
    }
    onRemoveDiseaseFunction(disease) {
        console.log(disease);
        this.loadingService.present();
        // tslint:disable-next-line: max-line-length
        this.pastMedicalHistoryApiService.deletePastMedicalHistory(this.selectedEncounterNo, disease.id).subscribe((result) => {
            console.log(result);
            this.loadingService.dismiss().then(_ => {
                if (result && 200 == result.code) {
                    console.log('onRemoveDisease success');
                    this.medicalHistories.splice(this.medicalHistories.indexOf(disease), 1);
                    this.alertServ.openAlertModal('success', 'Success', result.message);
                }
                else {
                    console.log('onRemoveDisease failed');
                    this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    onAddSurgery(surgery) {
        this.openPromptModal(this.ADD_SURGICAL_HISTORY, { surgery }, 'Surgical History', 'Would you like to save your changes?');
    }
    onAddSurgeryFunction(surgery) {
        console.log(surgery.value);
        const dateObject = new Date(surgery.value.date);
        const date_of_operation = moment__WEBPACK_IMPORTED_MODULE_15__(dateObject).format('YYYY-MM-DD');
        console.log('dateObject: ', date_of_operation);
        const newSurgery = {
            encounter_no: this.selectedEncounterNo,
            description: surgery.value.description,
            date_of_operation,
            remarks: surgery.value.remarks,
            modified_by: this.authentication.user.person.pid
        };
        this.loadingService.present();
        this.pastMedicalHistoryApiService.addSurgicalHistory(newSurgery).subscribe((result) => {
            console.log(result);
            this.loadingService.dismiss().then(_ => {
                if (result && 200 == result.code) {
                    console.log(newSurgery);
                    const surg = new _models_surgical_history__WEBPACK_IMPORTED_MODULE_11__["SurgicalHistory"](newSurgery);
                    surg.id = result.data.data.id;
                    this.surgicalHistories.push(surg);
                    surgery.reset();
                    this.surgicalHistoryModified.modified_by = result.data.modified_by;
                    this.surgicalHistoryModified.modified_dt = result.data.updated_at;
                    this.hasSurgicalHistory = false;
                    this.alertServ.openAlertModal('success', 'Success', result.message);
                }
                else {
                    console.log('failed');
                    this.alertServ.openAlertModal('fail', 'Failed', result.message, 'Ok');
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.alertServ.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
