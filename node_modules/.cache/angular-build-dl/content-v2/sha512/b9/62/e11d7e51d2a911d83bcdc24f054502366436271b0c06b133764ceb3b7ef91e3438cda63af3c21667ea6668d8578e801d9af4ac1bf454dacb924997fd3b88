../../services/api.service';\r\nimport { PdfViewerService } from './../../services/pdf-viewer.service';\r\nimport { LoginService } from './../../services/login.service';\r\nimport { StringUtilsService } from './../../services/string-utils.service';\r\nimport { LaboratoryResults } from './../../models/laboratory-results';\r\nimport { RadiologyResults } from './../../models/radiology-results';\r\nimport { InAppBrowser, InAppBrowserOptions } from '@ionic-native/in-app-browser/ngx';\r\nimport { ModalController, Platform, MenuController } from '@ionic/angular';\r\nimport { PromptModalPage } from './../../pages/prompt-modal/prompt-modal.page';\r\nimport { AlertServiceService } from 'src/app/app/services/alert-service.service';\r\n\r\n@Component({\r\n  selector: 'app-view-result',\r\n  templateUrl: './view-result.page.html',\r\n  styleUrls: ['./view-result.page.scss'],\r\n})\r\nexport class ViewResultPage implements OnInit {\r\n  selectedTab = 'radiology';\r\n  laboratoryResults: LaboratoryResults[];\r\n  radiologyResults: RadiologyResults[];\r\n  selectedEncounterNo: string;\r\n\r\n  options: InAppBrowserOptions = {\r\n    location : 'yes', // Or 'no'\r\n    hidden : 'no',  // Or  'yes'\r\n    clearcache : 'yes',\r\n    clearsessioncache : 'yes',\r\n    zoom : 'yes', // Android only ,shows browser zoom controls\r\n    hardwareback : 'yes',\r\n    mediaPlaybackRequiresUserAction : 'no',\r\n    shouldPauseOnSuspend : 'no', // Android only\r\n    closebuttoncaption : 'Close', // iOS only\r\n    disallowoverscroll : 'no', // iOS only\r\n    toolbar : 'yes', // iOS only\r\n    enableViewportScale : 'no', // iOS only\r\n    allowInlineMediaPlayback : 'no', // iOS only\r\n    presentationstyle : 'pagesheet', // iOS only\r\n    fullscreen : 'yes', // Windows only\r\n  };\r\n\r\n  constructor(private resultsApiService: ResultsApiService,\r\n              private patientService: PatientService,\r\n              private apiService: ApiService,\r\n              public loginService: L