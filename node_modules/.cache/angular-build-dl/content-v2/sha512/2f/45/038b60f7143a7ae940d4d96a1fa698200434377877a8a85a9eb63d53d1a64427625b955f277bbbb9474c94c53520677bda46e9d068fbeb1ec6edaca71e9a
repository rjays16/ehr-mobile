let DoctorOrderPage = class DoctorOrderPage {
    constructor(navCtrl, alertServ, loadingService, apiService, loginService, patientService, stringUtilsService, planManagementApiService, planManagementDataService, route) {
        this.navCtrl = navCtrl;
        this.alertServ = alertServ;
        this.loadingService = loadingService;
        this.apiService = apiService;
        this.loginService = loginService;
        this.patientService = patientService;
        this.stringUtilsService = stringUtilsService;
        this.planManagementApiService = planManagementApiService;
        this.planManagementDataService = planManagementDataService;
        this.route = route;
        this.id = '';
        this.dateChecked = false;
        this.dateToggle = false;
        this.disableToggleDate = false;
        this.disableSaveButton = false;
        this.disableDate = false;
        this.disableTime = false;
        this.SAVE_DOCTOR_ORDER = 1;
        this.authentication = this.loginService.getAuthentication();
        this.planManagementApiService.setUrl(this.apiService.getUrl());
        this.planManagementApiService.setToken(this.authentication.token);
        this.selectedEncounterNo = this.patientService.getSelectedEncounterNo();
    }
    ionViewWillEnter() {
        console.log('ionViewWillEnter');
        window.addEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.addEventListener('native.keyboardhide', this.keyboardHideHandler);
    }
    ionViewWillLeave() {
        console.log('ionViewWillLeave');
        window.removeEventListener('native.keyboardshow', this.keyboardShowHandler);
        window.removeEventListener('native.keyboardhide', this.keyboardHideHandler);
        this.removeIonAppStyles();
    }
    ngOnInit() {
        const action = this.route.snapshot.paramMap.get('action');
        if (action) {
            if (action === 'edit') {
                const order = this.route.snapshot.paramMap.get('order');
                if (order) {
                    const selectedDoctorOrder = JSON.parse(order);
                    console.log(selectedDoctorOrder);
                    if (selectedDoctorOrder.action) {
                        this.remarks = selectedDoctorOrder.action;
                    }
                    else if (selectedDoctorOrder.remarks) {
                        this.remarks = selectedDoctorOrder.remarks;
                    }
                    const order_checked = selectedDoctorOrder.order_checked;
                    if (typeof order_checked === 'boolean') {
                        this.dateToggle = order_checked;
                    }
                    else if (typeof order_checked === 'number') {
                        this.dateToggle = order_checked === 1;
                    }
                    else if (typeof order_checked === 'string') {
                        this.dateToggle = order_checked === '1';
                    }
                    else {
                        this.dateToggle = false;
                    }
                    this.id = selectedDoctorOrder.id;
                    this.dateChecked = this.dateToggle;
                    this.disableDate = this.dateToggle;
                    this.disableTime = this.dateToggle;
                    this.date = moment__WEBPACK_IMPORTED_MODULE_10__["unix"](selectedDoctorOrder.order_date).format();
                    this.time = selectedDoctorOrder.order_time;
                }
            }
            else if (action === 'view') {
                this.disableElement();
            }
        }
    }
    disableElement() {
        this.disableToggleDate = true;
        this.disableSaveButton = true;
        this.disableDate = true;
        this.disableTime = true;
    }
    backToPlanManagement() {
        console.log('back');
        this.navCtrl.pop();
    }
    currentDateToggleClick() {
        this.dateChecked = !this.dateChecked;
        if (this.dateChecked) {
            this.dateTime = moment__WEBPACK_IMPORTED_MODULE_10__().format();
            this.date = this.dateTime;
            this.time = this.dateTime;
        }
        this.disableDate = this.dateChecked;
        this.disableTime = this.dateChecked;
    }
    saveDoctorOrder(event) {
        console.log(event);
        this.openPromptModal(this.SAVE_DOCTOR_ORDER, {}, 'Doctor Order/ Action', 'Would you like to save your changes?');
    }
    saveDoctorOrderFunction() {
        let dateTimeObject;
        if (this.dateChecked) {
            if (!this.dateTime) {
                this.dateTime = moment__WEBPACK_IMPORTED_MODULE_10__().format();
            }
            dateTimeObject = new Date(this.dateTime);
        }
        else {
            // If dateChecked is false, combine date and time rather than using dateTime object
            const dateObject = new Date(this.date);
            const tempDate = moment__WEBPACK_IMPORTED_MODULE_10__(dateObject).format('YYYY-MM-DD');
            console.log('dateObject: ', dateObject);
            console.log('tempDate: ', tempDate);
            let tempTime = this.time;
            try {
                const timestamp = Date.parse(tempTime);
                if (isNaN(timestamp) === false) {
                    const d = new Date(timestamp);
                    tempTime = moment__WEBPACK_IMPORTED_MODULE_10__(d).format('HH:mm');
                }
            }
            catch (err) {
                // do nothing
            }
            console.log('tempTime: ', tempTime);
            // combine date and time to form new dateTime object
            const tempDateTime = tempDate + ' ' + tempTime;
            dateTimeObject = new Date(tempDateTime);
        }
        console.log('dateTimeObject: ', dateTimeObject);
        const order_date = moment__WEBPACK_IMPORTED_MODULE_10__(dateTimeObject).unix();
        const order_time = moment__WEBPACK_IMPORTED_MODULE_10__(dateTimeObject).format('HH:mm');
        console.log('order_date formatted: ', moment__WEBPACK_IMPORTED_MODULE_10__["unix"](order_date).format());
        console.log('order_time formatted: ', order_time);
        const courseward = {
            encounter_no: this.selectedEncounterNo,
            id: this.id,
            order: this.remarks,
            order_date,
            order_time,
            order_check: this.dateChecked ? 1 : 0,
        };
        this.loadingService.present();
        this.planManagementApiService.saveCourseward(courseward).subscribe((result) => {
            console.log(result);
            this.loadingService.dismiss().then(_ => {
                if (result && 200 === result.code) {
                    const batch = result.data.batch;
                    this.planManagementDataService.setUnfinalizedBatch(batch);
                    console.log('batch: ', batch);
                    this.openAlertModal('success', 'Success', result.message);
                }
                else {
                    this.openAlertModal('fail', 'Failed', result.message, 'Ok');
                }
            });
        }, (err) => {
            console.log('err ', err);
            this.loadingService.dismiss().then(_ => {
                const message = this.stringUtilsService.parseErrorMessage(err);
                this.openAlertModal('fail', 'Failed', message, 'Ok');
            });
        });
    }
    openPromptModal(option, data, title, message, ok = 'Save', cancel = 'Cancel') {
        this.alertServ.openPromptModal(title, message, ok, (result) => {
            if (result !== null) {
                console.log('result.data: ', result.data);
                if (result.data && result.data.button === 'Save') {
                    if (option === this.SAVE_DOCTOR_ORDER) {
                        this.saveDoctorOrderFunction();
                    }
                }
            }
        });
    }
    openAlertModal(alert, title, message, ok = 'Done') {
        return tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"](this, void 0, void 0, function* () {
            this.alertServ.openAlertModal(alert, title, message, ok, (result) => {
                if (result !== null) {
                    console.log('result.data: ', result.data);
                    if (alert === 'success' && result.data.button === 'Done') {
                        this.backToPlanManagement();
                    }
                }
            });
        });
    }
    tapCoordinates(e) {
        this.y = e.touches[0].clientY;
        this.h = window.innerHeight;
        this.offsetY = (this.h - this.y);
        console.log('offset: ', this.offsetY);
    }
    keyboardShowHandler(e) {
        const kH = e.keyboardHeight;
        console.log('show: ', e.keyboardHeight);
        const bodyMove = document.querySelector('ion-app'), bodyMoveStyle = bodyMove.style;
        // Adjust full keyboard height
        bodyMoveStyle.bottom = kH + 'px';
        /*
        if (this.offsetY < kH + 40) {
          bodyMoveStyle.bottom = (kH - this.offsetY + 40) + 'px';
          // bodyMoveStyle.top = 'initial';
        }
        */
        console.log(bodyMoveStyle.bottom);
    }
    keyboardHideHandler() {
        console.log('gone');
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
    removeIonAppStyles() {
        const removeStyles = document.querySelector('ion-app');
        removeStyles.removeAttribute('style');
    }
};
